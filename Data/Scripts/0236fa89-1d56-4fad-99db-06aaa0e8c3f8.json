                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fe350a70-e168-471b-9bd4-1cdc196992cc",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I'm sensing magic from <i>inside</i> the skeleton. That's creepy.",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "1db4bcdd-dc32-4855-8e56-49ec7fd8fc19",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "As far as I can tell, the skeleton was tied to the anchor after the guy died. Why bother? It doesn't look all that secure.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "20e4d3e8-38ce-4ead-acce-bcf9ff73057a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I mean, if it wants to jump up and come after me, a few pieces of rotten cloth aren't going to stop it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "00c9b62a-baa7-45e7-a101-53473ab7df29",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Why do I think of these things? I'm scaring myself.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
