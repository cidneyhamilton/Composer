                        {
                            "type": "options.text",
                            "id": "d3d0ffe9-bc10-4cde-8e17-c15cf2a79fd5",
                            "text": "Look at the Skeleton",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "f9400d00-f4ea-4b16-92d7-2cbded380d8a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "This gruesome skeleton has been bound to an anchor. He must have been here a long time... there's no flesh left on the bones.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "b08b2fa1-eb4f-4226-8171-8413a426b31c",
                            "text": "Sense Magic on the Skeleton",
