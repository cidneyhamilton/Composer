                        {
                            "type": "options.text",
                            "id": "0a4b67c8-ff4f-4e90-922b-75f20d0ccbfe",
                            "text": "Ask About Gregor",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "d6725117-cd7a-44ea-9009-94a335431e5e",
                                    "scope": "scene",
                                    "scopeId": "af912e02-46a1-4d88-a547-59f3d2093c7b",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "GreetedGregorToday",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "93e10f53-a640-4d68-b339-a7dd843b9c53",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "How are things with you today?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "7ddf3ee4-9c6a-4db0-a3aa-ea571520a79d",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "d36c1a16-74e1-4b29-9561-b061f6786797",
                                            "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Gregor is grumpy. Sooo many Warrior Drats running around this school.",
                                            "emotion": "2",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "57900643-e6d8-431a-b3e9-d4cc8df092d9",
                                            "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "It is not a good day when Gregor fears for his life from Warrior Drat bites!",
                                            "emotion": "2",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "76ee2046-167e-494a-aef2-3a9be09cfe98",
                                            "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Sad, sad, sad. Gregor hates Warrior Drats.",
                                            "emotion": "2",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "884a609f-5b1e-43e4-a28b-08515fe0119a",
                                            "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Nasty, bad Warrior Drats. Not a good day.",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "a5723fca-a6a4-430e-b7e0-bad3d2cdb80e",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                }
                            ],
                            "expression": {
                                "type": "expressions.and",
                                "left": {
                                    "type": "expressions.inTags",
