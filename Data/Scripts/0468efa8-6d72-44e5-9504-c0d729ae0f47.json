{
    "type": "script",
    "id": "0468efa8-6d72-44e5-9504-c0d729ae0f47",
    "name": "Interact with Sconce - all",
    "sceneId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
    "propId": "55ccc68d-258f-4d5d-8690-5f34d2eed538",
    "actorId": null,
    "trigger": {
        "type": "triggers.interact",
        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "b89c3d5a-adf6-4fd2-b8ba-ad5655c7ec83",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "scene",
                                "scopeId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                "scopeFilterId": null,
                                "tags": "GerhardRevealed",
                                "has": false
                            },
                            "nodes": [
                                {
                                    "type": "nodes.showMenu",
                                    "id": "58622f88-4c8c-4789-9206-43cfb8951436",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "059af6e8-618b-489c-86ec-5aad679a7d00",
                                            "text": "Look at the Sconce",
                                            "nodes": [
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "scene",
                                                                "scopeId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                                                "scopeFilterId": null,
                                                                "tags": "GerhardPresent",
                                                                "has": true
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "8fa35e88-1280-4769-90fc-9922c7ecb27c",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "text": "That's a lovely sconce you have there. Very...",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "ac23d4d9-8f07-4e37-8670-69e0264e2e9e",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "text": "Uh...",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "41d07807-b82c-464c-8674-ea2e9bf5e70e",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "text": "... lovely.",
                                                                    "emotion": "2",
                                                                    "emotion2": "3",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "0d7890af-5877-474a-a102-74918e986c53",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Is it my imagination, or has your presence in my office actually lowered the quality of discourse here?",
                                                                    "emotion": "3",
                                                                    "emotion2": "2",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "1946803d-f3cc-4bd5-b098-1e88af34139c",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Yes, now that I think on it, I'm quite sure. I <i>was</i> having more interesting conversations when I was alone. ",
                                                                    "emotion": "3",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "acaf4385-176d-486d-b3c2-a8e06c050e32",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Trust the Master to have the finest sconces in the whole joint.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "5ad00438-f8e9-4a06-a4cf-46f973ccfa40",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "And by \"joint,\" I mean this corner of the castle, where that side meets that other side.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": 0,
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "4b9bde76-a545-4e99-8af5-005b6bf4d45d",
                                            "text": "Move the Sconce",
                                            "nodes": [
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "scene",
                                                                "scopeId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                                                "scopeFilterId": null,
                                                                "tags": "GerhardPresent",
                                                                "has": true
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "af85eb55-c22b-4dcd-b9e6-2f201f1d4c58",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Searching for hidden levers or hinges or whatnot, are you? Very industrious.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "c1235638-1efd-4317-9941-d9250d6be724",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Had you demonstrated such assiduousness in your previous dealings, you would not be here among the reprobates.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "90484bfe-ed55-47e2-a85a-6fb559381ec9",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "text": "Reprobates? I'm not sure what those are, but I know when I've been insulted.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "8f9ef1a7-0bfe-4fd5-a7d6-f17aa4f581f9",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Then my day is complete. Feel free to show yourself out.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "9e9adf2e-aa6a-42ad-ab7f-1bafd32b49ea",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "If there's any kind of mechanism or secret to this sconce, it's beyond me. It seems devilishly normal.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": 0,
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "5d533aee-2662-4deb-aedb-bb6782e4d0ce",
                                            "text": "Steal the Sconce",
                                            "nodes": [
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "scene",
                                                                "scopeId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                                                "scopeFilterId": null,
                                                                "tags": "GerhardPresent",
                                                                "has": true
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "5ad31e4d-feb7-44b8-a597-0eac67fbe6cc",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "That blatantly covetous look has reminded me, Herr O'Conner. Of what metal do you prefer your manacles when they take you away for... ridiculously overrating yourself? ",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.showMenu",
                                                                    "id": "10d043df-5614-4611-902c-dd8fd6e5d96b",
                                                                    "options": [
                                                                        {
                                                                            "type": "options.text",
                                                                            "id": "def94a1b-35c1-4f1a-942f-c37778e03913",
                                                                            "text": "Answer Innocently",
                                                                            "nodes": [
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "7782743c-4e97-44b7-8ee1-25876cd59b08",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "text": "True... true... I did look at the sconce with desire in my heart.",
                                                                                    "emotion": "4",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "3131d532-3283-4654-913d-66ad91bb8b07",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "text": "Desire to help the children. Desire to ease the hunger. A sconce such as that one would bring a fine price, and I could afford to buy so many more apples for the children. The children, sir. Take pity on them.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "8157796a-1072-4c4f-ad89-cbe8f9bfb55b",
                                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "text": "The children. Ah, that one really takes me back. A bit old-fashioned, isn't it, Herr O'Conner? Nicely executed, though. It might actually have worked on me, if only I'd been an <b><i>utter dimwit</i></b>.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                }
                                                                            ],
                                                                            "expression": null,
                                                                            "ignoreChildAvailability": true,
                                                                            "minutes": 0,
                                                                            "ExitMenu": false
                                                                        },
                                                                        {
                                                                            "type": "options.text",
                                                                            "id": "79695d4a-46d9-4a1d-9485-7adc9427ca2e",
                                                                            "text": "Answer Charmingly",
                                                                            "nodes": [
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "495bf71f-7006-4198-b3f4-4e045f067822",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "text": "I will be the first to admit to coveting, Master von Urwald. I covet the <i>skill</i> to create such an impressive... and <i>stylish</i>... sconce. That's... that's what I <i>really</i> admire.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "450bcab2-36e4-403e-8ae3-934e5633fb54",
                                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "text": "Oh, then this is indeed fortuitous, Herr O'Conner! I hear that prisons these days offer very satisfactory training in practical metalwork! Shall I recommend you to the authorities?",
                                                                                    "emotion": "1",
                                                                                    "emotion2": "4",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "872cc71e-c202-458a-bfa5-b8e1ae4eb231",
                                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "text": "I thought not.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                }
                                                                            ],
                                                                            "expression": null,
                                                                            "ignoreChildAvailability": true,
                                                                            "minutes": 0,
                                                                            "ExitMenu": false
                                                                        },
                                                                        {
                                                                            "type": "options.text",
                                                                            "id": "2a1ec576-64f4-44fb-b2ef-82cae118054d",
                                                                            "text": "Answer Honestly",
                                                                            "nodes": [
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "b28ca58b-10ab-4ff2-ad66-5331737e6a3d",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "text": "Well, yes, I was clearly wondering if it was something I could even steal if I wanted to... and I hadn't even decided whether or not I wanted to. ",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "03ef9185-26cb-4709-a957-80f9ab93baeb",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "text": "But, Master... doesn't everyone always think of doing larcenous, or dishonest, or hurtful things from time to time? I consider stealing things constantly just for the fun of figuring out how I'd get away with it. Most of the time, when I think these things, I don't actually go through with them.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "2deba57a-4922-43f2-bf6d-ef05d5a461bc",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "text": "You'll notice that about me.",
                                                                                    "emotion": "1",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "c6931353-cbc5-4072-a293-a859edc35856",
                                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "text": "You know, Herr O'Conner, your face has a tendency to betray you. I thought we'd discussed Rule 6 at sufficient length, but apparently not.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                }
                                                                            ],
                                                                            "expression": null,
                                                                            "ignoreChildAvailability": true,
                                                                            "minutes": 0,
                                                                            "ExitMenu": false
                                                                        }
                                                                    ],
                                                                    "AutoAddDone": false,
                                                                    "Unique": true
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "f174f86e-9a45-4762-8e41-60e733c830c9",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "It's attached to the wall. I can't even <i>lift</i> it, let alone <b>lift</b> it.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": 0,
                                            "ExitMenu": false
                                        }
                                    ],
                                    "AutoAddDone": true,
                                    "Unique": false
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}