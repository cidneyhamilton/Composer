        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "24d3d8e0-311b-4d2e-a50b-0a354bba27da",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "0ddc0e17-1982-403a-8d64-78069e32d5d1",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "7c3505e8-856a-4c45-8f4f-154a169d67d7",
                            "text": "Look at the Cabinet",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "e0267f51-e788-46ec-ba0a-7ff8b3b075ab",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "She's going to need a bigger cabinet.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "4a242808-9299-44eb-b1cb-d9e012229f4f",
                            "text": "Look Behind the Cabinet",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.actorPresent",
                                                "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5b6a1040-aa6a-49a3-9cc2-5a8fc970c5bf",
                                                    "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Well, now, where do you think <i>you're</i> going, Sweet Cheeks?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "36cdbd1a-4e7a-46d8-a177-ab81f892a420",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "text": "I was... uh... just checking on that cabinet back there, It looked like it was about to topple over.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7360c8ec-b301-4d72-bf43-4eeda25905eb",
                                                    "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Oh! Well, you see this desk? You see this little part of it that lifts up and down so a person can get in or out?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cfc8173e-7cfb-4415-b46c-fe6632244cbb",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "text": "Yes!",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ab443f3f-0bd4-42c4-a085-87b5264d2f2e",
                                                    "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Okay! Now, you just stay on THAT side of this desk, <b><i>always</i></b>, unless you work for the school or unless I invite you. You understand that, Beefcake? Are we clear on that?",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0172de23-93e3-4cf9-b017-59f612025fc2",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "text": "Yes.",
                                                    "emotion": "2",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b2ff1ee1-9cf5-458a-8b9f-b97fa321a623",
                                                    "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "There, now! You see how nice?",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d812acad-bf4b-4b29-9d92-d2139b5ffd53",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "text": "Yes.",
                                                    "emotion": "2",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "427d8406-ece5-4825-a92c-f76d8a93fbc6",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I bet there isn't anything interesting behind those shelves. You never want to pick a heavily-traveled, central location for your hidden doors and safes and whatnot. The odds that they'll be discovered go way up.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cc03a1fe-19ff-4b92-be58-0ff9f2169f70",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Which is just an evasive way of saying that I don't want to get caught behind that desk. You never know when some little weasel-faced heap of parrot droppings will show up and make trouble for you.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "7f70f11c-52d2-4e5d-a4d4-5f0a83c6e684",
                            "text": "Search the Cabinet",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.actorPresent",
                                                "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c3e78a35-c318-480b-8ac8-43e571c222f1",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Even if Sophia wasn't right here, I can't imagine what I'd find that wouldn't bore me to death. Visitor sign-in books? Supply requisition forms? I've read more interesting stuff in a <i>library,</i> for cryin' out loud.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "90b70fbe-208b-4aac-bafd-babc5c55fe0b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can't imagine what I'd find that wouldn't bore me to death. Visitor sign-in books? Supply requisition forms? I'll skip it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
}