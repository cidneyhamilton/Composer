                        {
                            "type": "options.text",
                            "id": "8c1fb247-284d-479f-9a4b-f1418aa95505",
                            "text": "Look at the Skeleton",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
