                            "text": "Talk to the Skeletons",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "Exhausted",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.nodeCycle",
                                                    "id": "2b35a30e-a9f8-4ad2-8057-8deec92335dc",
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.branch",
                                                            "sections": [
                                                                {
                                                                    "type": "nodes.branchSection",
                                                                    "expression": null,
                                                                    "nodes": [
                                                                        {
                                                                            "type": "nodes.speak",
                                                                            "id": "a0c9db51-ce64-445c-b66b-136fd4ba77b2",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "You're just going to stand there and not grab me or split me in two, right?",
                                                                            "emotion": "0",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
                                                                        },
                                                                        {
                                                                            "type": "nodes.speak",
                                                                            "id": "7687d9d0-1287-4841-9228-722b9555b5fb",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "I'll take your silence to mean \"Yes.\"",
                                                                            "emotion": "1",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
                                                                        }
                                                                    ]
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "4c4c034d-7f6a-4b1c-93ec-a757c2358e45",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "I'll bet you're not as sternum as you look.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "8d6810f9-3c15-4ac8-8a45-d17bc6fe1844",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "If you guys just want to stand around and watch, that's spine with me.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "30730a71-38a2-474e-af59-3fdefc5331d8",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Did I see you guys doing a song and dance about which bones are connected to which other bones? That was a great act!",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "089d7fdf-7742-4d94-a8ee-8db5406f7c32",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "How nice that you still have most of your own teeth.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.branch",
                                                            "sections": [
                                                                {
                                                                    "type": "nodes.branchSection",
                                                                    "expression": null,
                                                                    "nodes": [
                                                                        {
                                                                            "type": "nodes.speak",
                                                                            "id": "2d3a7832-d0fa-4e04-a4d0-4db398045f1a",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "text": "I don't seem to be able to have a decent conversation with these skeletons.",
                                                                            "emotion": "0",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
                                                                        },
                                                                        {
                                                                            "type": "nodes.changeTags",
                                                                            "id": "d3faee97-069d-4821-b65b-2eba0cf83a4d",
                                                                            "scope": "target",
                                                                            "scopeId": null,
                                                                            "scopeFilterId": null,
                                                                            "tagsToAdd": "Exhausted",
                                                                            "tagsToRemove": null
                                                                        }
                                                                    ]
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0767d94f-bea5-4a23-a842-ec593c9b247d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I'm not going to waste any more time talking to these guys. They're giving me the cold clavicle.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
