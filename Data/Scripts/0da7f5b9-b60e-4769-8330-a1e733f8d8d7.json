    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "0be13927-2c27-4a79-8d3d-0d7e1cdda657",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "b6ed0695-0f87-428f-8de8-f93b31e1bd73",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "4afbf601-247f-4b4c-8862-ae50993fef16",
                            "text": "Look at the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.showCloseUp",
                                                    "imageName": "OldDarkOne_framed.jpg"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0c2044e0-aec8-496b-b9bc-8d52a94e3dcc",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Yikes. Why would anyone want a painting like this? Why would anyone even paint such a thing? It's not like something like this could be real.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "76be6a9f-8e1d-4d65-a57c-d7c162c8fccd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Could it?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.hideCloseUp",
                                                    "waitForSeconds": 1
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8bfd25fa-d451-405c-93e0-e3637b5bdcc5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I'm not sure why it feels so cold standing near this painting. I need more light before I can tell anything about it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "2ad9db5d-83ff-4d6c-bf66-82e400dc9591",
                            "text": "Look Behind the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.invokeScript",
                                                    "id": "d08eb54b-acda-40a1-93e7-8d246fe1b034",
                                                    "scriptId": "b034c7b9-29f5-46ab-b66a-3a39f340b33b",
                                                    "entryPointId": "0be13927-2c27-4a79-8d3d-0d7e1cdda657",
                                                    "currentScope": "Scenes",
                                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                                    "currentActorId": null,
                                                    "currentStoryEventId": null,
                                                    "currentPropId": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9c0cb12b-d785-4b7d-966a-2ee2c1230343",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "There's nothing behind it. Well, except a lot of spiders I just disturbed. And they're <i>cranky</i>. Maybe I interrupted their nap. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "dac6e3d8-d2d5-4a93-b1bf-f550f8da4254",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Even if there was something in back of this painting, I wouldn't be able to see it in this light.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "9a950ab4-7f2f-4f88-9431-9c06f56907ba",
                            "text": "Take the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "dadd86f7-312f-484a-85ae-cc3852f673ab",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I don't know. It's so hideous, I don't even really want to touch it. Could it be... cursed or something? ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f9842169-a889-4811-8427-6093cf625a74",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I really don't have a good feeling about this. I'm leaving it alone.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ef0c754f-4b07-45a8-9da6-dd3e76668883",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I wouldn't try to manage it without better visibility.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "7c3dac0f-5d30-4234-b1c6-76c6e61f6271",
                            "text": "Sense Magic on the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "e4dcb871-fc52-48dd-b34f-5e185a25e54e",
                                    "scriptId": "533e1ce1-13d3-439f-8544-dbdf808d30c4",
                                    "entryPointId": "0be13927-2c27-4a79-8d3d-0d7e1cdda657",
                                    "currentScope": "Scenes",
