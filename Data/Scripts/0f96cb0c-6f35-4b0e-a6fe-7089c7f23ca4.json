    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "0016349f-1e87-429a-8063-61b0aae8e790",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.and",
                                "left": {
                                    "type": "expressions.actorPresent",
                                    "actorId": "fdf2a22c-39ae-4ce9-8f9d-13ba189897a3",
                                    "hasNot": false
                                },
                                "right": {
                                    "type": "expressions.inTags",
                                    "scope": "actor",
                                    "scopeId": "fdf2a22c-39ae-4ce9-8f9d-13ba189897a3",
                                    "scopeFilterId": null,
                                    "tags": "dead",
                                    "has": false
                                }
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "0e21d5fb-048c-4104-a9e6-460f1fd91e4a",
                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Ya best be talking ta the Cap'n, he don't likes people talking behind his back. He figures we be plotting mutiny, ya see.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.isEquipped",
                                "propId": "56ac5b3c-8a3c-4d32-974f-dc1fdb1e031a",
                                "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4"
                            },
                            "nodes": [
                                {
                                    "type": "nodes.invokeCommand",
                                    "id": "771e3bf0-d2f7-4c5d-a5ba-c9d5464dccd1",
                                    "entityType": "actor",
                                    "entityId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                    "command": "salute",
                                    "resultVariableScope": "script"
                                },
                                {
                                    "type": "nodes.onceOnly",
                                    "id": "7caffe28-c028-46ca-b3d2-09bc23ca1112",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "b8ba02c3-fb3b-45b7-9a4f-903cd3d78ea5",
                                            "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                            "actorId2": null,
                                            "text": "Ahoy, Cap'n, ya be looking bright-eyed and bushy-tailed today.",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "a1551fef-b327-4500-99b3-47971fde3dbc",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "b129b408-a4ce-488c-9411-e69af6d9854d",
                                            "text": "Ask About the Captain",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f07ace3f-48a4-4422-acd7-9b86e3e0bf03",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "So who's the captain now?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "84e06290-06dd-4a7e-b4cf-205837d34428",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Why, you be cap'n, Cap'n.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "64cba6b4-c167-4f50-ac2c-ff0feaac6bfa",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "What just happened to the old captain?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1854b1c9-06f1-4549-bd2d-da200d821cb4",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Can't rightly say, sir, but I suspects that he be dancing with the devil's daughter right now. ",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "33e77db9-7495-4050-8ba6-6f8b028130a4",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "So, does this mean I get to give the orders around here?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7004469d-55c0-434a-a29c-02405539f8bd",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Aye, sir. Orders be what a captain gives.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": "5",
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "882d9c00-00fd-4092-850f-c06de16a12d3",
                                            "text": "Ask About the Zombies",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "170e7e42-c560-4623-a8c8-146acd4618c3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "So how did you people all become Zombies? It's not like I want to join the same Zombie club as you guys did.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c6041bc6-1ddf-4e44-a73e-949df835685f",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "The Zombie curse might be from the time we crossed the Voodoo Queen. Or when we gutstuppered the witchdoctor. Maybe it was ramsacking that temple to Kallee? Or the cursed gold we took from the ghostly galleon? Can't rightly say, Cap'n.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": "4",
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "1fec02db-055f-4ccc-8ef1-e931f3a3ee94",
                                            "text": "Ask About the Treasure",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a3676601-70a1-42c3-91da-67100d997a6c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "So I hear that there's a big treasure hidden somewhere in the sea caves...",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1c626b0b-a761-4d97-90ea-550172912018",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "The treasure don't be hidden, Cap'n. We just made sure only Cap'n Bronson and his crew could get the treasure. But then we all up and gots ourselves killed, so treasure be a bit o' a moot point right now.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "94a464aa-21fd-4c9b-b8bb-a8e6eeb80499",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "So will you help me get the treasure?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e51566c1-4915-4489-9bff-152effb64ee8",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Aye, sir.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "faebf13f-cf11-44f0-9dbc-4b23745b6b54",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "So where is it?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1df13640-a0d0-42f4-add1-46dbda795116",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Where is what, sir?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "21665c0d-f99e-4cbb-a450-8c0e9da815f0",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "The treasure... where is the treasure?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f5dd0066-ff4d-4377-9869-e60a053637c9",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Can't say as I know, sir. Things be a bit foggy now. Guess it comes of being a zombie and all.",
                                                    "emotion": "0",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": "5",
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "993d60c2-36e8-4735-9fd1-2eb51fedcb8c",
                                            "text": "Ask About Orders",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "525e3f34-c2f5-4b60-8e12-a0d369a62338",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "So if I'm the Captain, I can order the Zombies around?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c7621983-0d58-44d3-ac35-bca41b853036",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Aye, Cap'n, but they don't be listening to you. They only be listening to me. I be relaying yer orders for ye.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ec4f470f-56ac-40cb-ac1a-ab3d2b89c03c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "Why can't I give them orders?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "232be989-62f5-4ec9-b085-aab98093d8a5",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Zombies don't be hearing you, Cap'n, No ears. They all fell off long ago.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4324ef06-ab3c-4e91-9c30-47b9e27725e9",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "text": "Wait... you're a Zombie. How come you hear me then?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "72870589-38e0-4a15-a2c9-dfcb91664b88",
                                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I don't be just any Zombie. I be yer First Mate, sir.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": 0,
                                            "ExitMenu": false
                                        }
                                    ],
                                    "AutoAddDone": true,
                                    "Unique": false
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": null,
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "532d5fe8-50da-473d-b7fd-3d9601d40b14",
                                    "actorId": "31e7b33f-83fe-48aa-bbfe-23730a880c40",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I don't be talking to strangers.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "182be8a4-606c-41a5-97ed-857ea9522c2d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "But... but... ",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}