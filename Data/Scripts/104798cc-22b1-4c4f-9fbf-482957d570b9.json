                                {
                                    "type": "nodes.speak",
                                    "id": "e5bab49f-776e-4317-b7c3-be8ef873cb93",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I suppose I could take this apart and see what makes it so special...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
