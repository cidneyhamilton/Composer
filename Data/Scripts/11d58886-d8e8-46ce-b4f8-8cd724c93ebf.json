                                                                "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                "text": "Thanks. Why don't you cover my fingers in Gooey while you're at it?",
                                                                "emotion": "4",
                                                                "emotion2": "2",
                                                                "device": "0",
                                                                "device2": "0"
