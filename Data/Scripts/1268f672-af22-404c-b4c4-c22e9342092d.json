    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "0eb4f3ad-2596-4f42-9d2f-50322978e2a2",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.speak",
                    "id": "6f94cdfe-c2fb-4c18-ba29-2aab461652de",
                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "Last Poobah match of the year.",
                    "emotion": "1",
                    "emotion2": "1",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                "scopeFilterId": null,
                                "tags": "EsmeDatesAeolus",
                                "has": true
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "00d95690-42bd-4820-a218-47239ca87edb",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Maybe this time, I'll make enough money to take Esme out on a fancy date.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                "scopeFilterId": null,
                                "tags": "AeolusHappy",
                                "has": true
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b7b2fffe-7b11-4b79-aaf1-63c16b4f23cf",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "If I win big tonight, I'm treating you to a night on the town of Caligari. I know a really good tavern that has the best music in the city.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "nodes.showMenu",
                    "id": "69c97275-3c33-40f4-8cda-de1738cc9f7e",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "c00f4d77-3055-4423-96fa-8133b79a1722",
                            "text": "Ask About Aeolus",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b6f64029-9f1c-4698-af17-0589f1474618",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "So what are you planning to do when school is over for the semester?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "scopeFilterId": null,
                                                "tags": "EsmeDatesAeolus",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b1b847fc-6c3e-429c-98e5-7ba84fc72773",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Since Esme has always wanted a home of her own, I'm going to invite Esme to come home with me to visit my family. I hope my family likes her as much as I do.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                "scopeFilterId": null,
                                                "tags": "AeolusHappy",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "00ed726f-076c-4089-8e2e-f61e4c816cf2",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I thought that you and I could discuss this subject when we are alone together.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "724d9bcd-a926-406b-ba01-16b6648f750e",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I thought that I would hang around Caligari and busk at the taverns. I want to see if I can make a living from being a bard.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e88014d0-b8e7-4ab4-b185-bbae9d161e09",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "What are your plans for vacation?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e8edb422-fe05-461d-af48-37f5ba4a59b5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "I have a lot of options for my future. I haven't come to a real decision yet.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "fb91d3c4-3f5e-459a-9af1-069cfe9882a4",
                            "text": "Talk About Poobah",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "f30ecc0d-9d8f-4879-b8b5-507be60942d4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "What makes you think you can beat me at this game?",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "bd0c0420-3a59-432c-a567-a138fb0e575f",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Hey, anyone can get lucky sometime. But win, lose, or draw, I'll have fun as long as Sosi gets taken for every Lyra he has.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "56885445-1243-4caa-a3b5-f7622b58c1ea",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "I agree, I'd love to see Sosi go broke, but what do you have against Sosi?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "scopeFilterId": null,
                                                "tags": "EsmeDatesAeolus",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c4003a22-e7d3-49b2-b4fc-db0377ac3a05",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Sosi insulted Esme. He deserves to be taken down for his arrogance.",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                "scopeFilterId": null,
                                                "tags": "AeolusHappy",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8518ae1b-3e66-45d9-9fa4-ca09a6b37130",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I trust your judgment on Sosi. Besides, Sosi is always so smug, pompous, and toxic when he plays and it's a pleasure to knock that smirk off his face.",
                                                    "emotion": "3",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c43437ef-ec52-42b8-b76d-a2a41c308e94",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Sosi is always so smug, pompous, and toxic when he plays so it's a pleasure to knock that smirk off his face.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "b65dce8a-1d67-4b54-8b03-ee36ad33320b",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "748ed65a-1d72-416d-9a7c-4681209ecb0b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "Good luck at Poobah!",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "ba34f1e8-19bd-42e9-b84e-e044c20c14d4",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'm going to need it, thanks.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
