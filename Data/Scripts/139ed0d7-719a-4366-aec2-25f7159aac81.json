                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "This is a book is titled \"Crescendo's and Don'ts: C'ing Sharp without B'ing Flat\". I'm guessing you had to be there.",
                                                                            "emotion": "0",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
                                                                        },
                                                                        {
                                                                            "type": "nodes.playSoundEffect",
                                                                            "soundEffectName": "book_close.wav",
                                                                            "wait": false
                                                                        }
                                                                    ]
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branch",
                                                            "sections": [
                                                                {
                                                                    "type": "nodes.branchSection",
                                                                    "expression": null,
                                                                    "nodes": [
                                                                        {
