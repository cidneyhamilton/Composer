                                                                            "id": "ea708378-4754-4685-81f5-192211f47810",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "\"Barding and You: How to Properly Dress a Warhorse\".",
                                                                            "emotion": "0",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
