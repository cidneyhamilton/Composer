    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "dd67a586-f5ce-4c54-aab9-0fd8a39b7369",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "78bba68a-c10c-4908-b560-d76b6f4bef0c",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "c1a6e1d0-decc-46c9-9357-02bb198828f8",
                            "text": "Look at the Bulletin Board",
                            "nodes": [
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "6f306f4f-3774-4671-b0d7-7a5b098e1c65",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "5304e25c-75da-44f1-a6a2-d2169e2199f5",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Any student reaching 100 demerits will be expelled immediately.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "9899786f-e380-4ea4-b6c1-776402b34552",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Any student who argues with a faculty or staff member will receive 10 demerits.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "5b14b4ff-e54b-4fd9-900a-0432a504e299",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Students found outside their room after 10 pm will receive 10 demerits.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "4dd6fb5b-90b0-4934-8619-0847597e907e",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Wading in the fountain is 10 demerits.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "ee507b9a-93f6-4fda-97e6-374144ff36b8",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Cheating on a test will result in immediate expulsion.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "befbe898-ae45-4756-a3fa-2f30ee592b8b",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Theft of school property will result in immediate expulsion.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "7237f8b4-7b2d-455f-acff-06030eb7602a",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Students who arrive late to class will receive 10 demerits.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "6ea0feb7-a8df-49bf-b48a-4c8991134e7d",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Why are the demerits always in multiples of 10? Does anyone ever receive, say, 7 demerits for this, or 2 demerits for that?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "b88fd504-8c82-4199-8fc5-3d6fba804551",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "This list of demerits goes on and on! Terk is such a jerk. We should all sit around in a circle and talk about what a jerk he is.",
                                                            "emotion": "3",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "b44bb304-27cd-4f43-90e9-2515a9f8c3e9",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "Hey, we could call it a circle Terk!",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "9b17bb56-e673-411a-b32f-c6cb3ed950e7",
                            "text": "Look Behind the Bulletin Board",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "bfc830f6-fea2-44c2-ab57-5563a4c15fac",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I better not. There's one entry on the list that says, \"Looking behind this bulletin board will result in immediate expulsion.\" Cripes!",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
