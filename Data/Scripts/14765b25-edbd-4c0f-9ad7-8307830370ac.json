                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "1c424a13-8bcc-42e2-a615-99577c6148c9",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "fca85dd0-c73b-40cd-93e2-a5f100919f1a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "We'll talk later.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b9a36d43-8016-479e-b2ac-4af9041fcee1",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Good luck!",
                                    "emotion": "1",
                                    "emotion2": "1",
