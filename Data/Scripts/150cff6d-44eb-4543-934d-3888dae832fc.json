                        {
                            "type": "nodes.speak",
                            "id": "da740e90-7e52-4682-99b1-ac664cd5d44a",
                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                            "actorId2": null,
                            "text": "No - not a ghost clown!",
