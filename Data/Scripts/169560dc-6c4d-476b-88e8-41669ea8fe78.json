                        },
                        {
                            "type": "options.text",
                            "id": "7d58dd9e-cd69-4049-bbe5-c4e6e4813519",
                            "text": "Chat with the Statue",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "846ef6a4-68ab-4183-ba51-715a31c476c7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I wouldn't know what to say to such a wise fellow! I'm all tongue-tied!",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b9a5b254-81e1-4611-9c50-e3cc361b7a0a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "But just being in his presence makes me giddy as a schoolgirl!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
