                    "options": [
                        {
                            "type": "options.text",
                            "id": "a3299d01-9cb4-40d2-b6fb-594ff072adbb",
                            "text": "Look at the Orb",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "prop",
                                                "scopeId": "1e9eedea-d6ef-474c-b3b5-8634f81dda62",
                                                "scopeFilterId": "5aa634b1-8fc5-4b1c-a556-7719d1e665a3",
                                                "tags": "LookedAt",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f8e957de-ff42-4362-854b-02692305f72c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "What in...",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6d6866b7-30bc-4658-984d-b48d12c9ac84",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "What's it for? Where did it come from? How can something so bright not be... even a little bit warm?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e9e8431d-85d9-4285-ba5e-a9d878f55b1f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I have no idea what it is, but... I think I want one!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "30b7f44c-f50b-4d77-b0d4-3ef9f35e251d",
                                                    "scope": "prop",
                                                    "scopeId": "1e9eedea-d6ef-474c-b3b5-8634f81dda62",
                                                    "scopeFilterId": "5aa634b1-8fc5-4b1c-a556-7719d1e665a3",
                                                    "tagsToAdd": "LookedAt",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a4108590-0159-44c6-a27f-2fb3a7a6898e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I ordinarily try not to let flies in my mouth, but... I've just never seen anything like it!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "042d12cc-d9e0-42ce-89a7-d9a59918d819",
                            "text": "Steal the Orb",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "prop",
                                                "scopeId": "1e9eedea-d6ef-474c-b3b5-8634f81dda62",
                                                "scopeFilterId": "5aa634b1-8fc5-4b1c-a556-7719d1e665a3",
                                                "tags": "AlreadyTried",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "10b4da2f-7812-49f6-b3f2-b22cfaf324c0",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It... it would never fit through the door!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "33cb9d3d-00e3-480b-b35d-d0964f16280b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Wait, what am I thinking? There's no <i>way</i> to steal that! <i>Nobody</i> could! It's ridiculous to even consider it!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "47f09aaf-b470-4129-82c5-10dde3cce3fd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Now let me think!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3c253103-4f59-4e06-bdb6-86db070a3f98",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Nah.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "a9097a77-a187-4c8c-8b6d-123ab2d249aa",
                                                    "scope": "prop",
                                                    "scopeId": "1e9eedea-d6ef-474c-b3b5-8634f81dda62",
                                                    "scopeFilterId": "5aa634b1-8fc5-4b1c-a556-7719d1e665a3",
                                                    "tagsToAdd": "AlreadyTried",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4010db42-8e9b-4092-ae3e-91e5061c235b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I would love to! It would be the greatest accomplishment of my life! Of <i>any</i> thief's life!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8d45a717-b3a8-4bbf-88a3-cd0daaa49ba1",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I just...",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "70b0834d-fb17-41d8-94a5-d689c5235c78",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "... haven't worked out the logistics quite yet!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
