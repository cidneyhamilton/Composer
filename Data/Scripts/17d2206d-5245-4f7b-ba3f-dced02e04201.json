                            "text": "Examine Stationery",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a5988d6f-d8d7-4a20-ba3d-5e43ae62a2c5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Hmmm... I can see the impression of the pen marks from the last thing the Master wrote...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
