    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "36a1d201-860b-44e9-8097-f63954fb5c4b",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "3e01d769-affe-432c-a97b-b8925c0314b1",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "2e23886f-7370-4bd6-be16-046d329913a4",
                            "text": "Look at the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "9e97b617-f5b2-4ed8-8603-253c05d64641",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "There's a slot in the center of the counter top.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "5961ec04-afc9-4b07-8869-4e237d339fe8",
                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "That's where ya puts your money when ya pays me. So, ya just here to sightsee? Or shall we get down to business?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "c60ad274-a0d7-4fc9-82a0-4c15992d46c1",
                            "text": "Climb over the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "ef87fda9-9481-4cd8-84b1-b169735c8c9b",
                                    "nodes": [
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "89cb5c2f-8ddc-4303-a210-71c613f376cd",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Don't even think about it!",
                                                            "emotion": "3",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "320fa67f-3dc9-4d59-8405-fb704b0535e7",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "text": "Sorry. Just curious.",
                                                            "emotion": "2",
                                                            "emotion2": "3",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "4d84badc-cfb3-423e-81d6-b2bac4b6a55c",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Where do ya think yer going?",
                                                            "emotion": "3",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "a4a814d8-fdce-43dd-b2d7-13adee82a08c",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "text": "I didn't know we couldn't go back there.",
                                                            "emotion": "2",
                                                            "emotion2": "3",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "d76c58ec-e118-43ab-ba8f-41503c068dd4",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Ya didn't? What do ya think the spiky fence is for?",
                                                            "emotion": "3",
                                                            "emotion2": "2",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "f4bf483f-16ea-43ed-89c3-c7fe799b9ac4",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "text": "Sorry.",
                                                            "emotion": "2",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "5656d130-7814-4099-b7d0-6cef66e3d444",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "What's all this, then?",
                                                            "emotion": "3",
                                                            "emotion2": "4",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "efda5c38-00d4-4dba-a27e-6597559c151e",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "text": "You mean I can't climb over the thing?",
                                                            "emotion": "4",
                                                            "emotion2": "3",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "e7c54a11-bb0c-4ab3-b5b6-7fc779181cfb",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Of course ya can't! This is my area, back here!",
                                                            "emotion": "3",
                                                            "emotion2": "4",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "a330c186-8def-4458-bb72-86fc614a7fda",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "I hate stupid customers.",
                                                            "emotion": "3",
                                                            "emotion2": "2",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "7b6dc5ae-5afd-4337-a5d1-fdb278dd2b09",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Hey!",
                                                            "emotion": "3",
                                                            "emotion2": "4",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "17ab268c-97eb-4c88-a7cb-8cdedbed1c60",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "What in blazes are ya doin'?",
                                                            "emotion": "3",
                                                            "emotion2": "4",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "9bb9449a-203e-46d7-9400-14faeca60a96",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "You just stay on yer side of the counter, and I'll stay on mine! Got it?",
                                                            "emotion": "3",
                                                            "emotion2": "4",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "eb409519-1c06-4dd7-be29-48f58326296d",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "text": "I guess we'll see.",
                                                            "emotion": "1",
                                                            "emotion2": "3",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "a7a5bcf7-95c7-4c80-89b5-1617c44ecce3",
                                                            "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "I don't want to have to tell ya again, but I will if I need ta!",
                                                            "emotion": "3",
                                                            "emotion2": "1",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
