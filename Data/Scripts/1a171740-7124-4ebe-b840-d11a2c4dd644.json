    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "fdd4f0ac-9403-4637-83cb-9c5597083c18",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "2c2eaa75-fb4f-458d-a98a-57bd1570e7e7",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "6d9de902-a96b-4676-99ba-4e3460c4e641",
                            "text": "Talk About Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "bf80db55-dd04-42eb-8b8e-39d954d40976",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I take it that you don't like exploring the Sea Caves?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8994b7ed-0598-42cf-bc55-972f07b105a8",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I am not fond of sea or caves. Sea vater burns my nose so I can't smell anything around it. And nothing good lives in caves.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "062642c8-6745-49d3-81f1-09c60bb18db2",
                            "text": "Talk About the Sea Caves",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7393c8a0-43b4-4aeb-bf53-14c1c88b04dc",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Have you been down in the Sea Caves?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fb03bd67-1c56-48ea-9f47-2cf7fb2f0c95",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": null,
                                    "text": "Yes, Katie and I explored some of caves over the break. They are not good places to be. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "db56a155-53bb-49df-9e76-beebcc1b2cfe",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "If you go there, be careful. Many monsters lurk there.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d07a2a68-ca86-46cf-a792-f8ddf4865fd5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Would you be sad if the monsters got me?",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "cc9f3dff-81f0-446a-af1d-9e55b50d3449",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a6c1b221-87c9-4d02-a5eb-66f09f2a8b22",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "No... I vould laugh because you vere varned and didn't listen to me!",
                                    "emotion": "1",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "921deaf0-235b-4c72-a0e3-359a03ff75c4",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "And maybe a little bit sad. So don't get eaten by monsters, okay?",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "6aa5a0ce-a900-4a31-8d5f-e04962fa6892",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I'll do my best not to get eaten, just because you ask so sweetly.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "7",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "a0be0703-b707-4741-9400-0cb55a792e11",
                            "text": "Talk About the Giant Proach",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "1395e587-de3e-4a70-ade1-bcbe58e4ff37",
                                    "scope": "actor",
                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "EsmeProachTalk",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "46626df4-9c3d-45be-bd7a-845e812f8f9b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I'd like to talk to you about what we did with the Giant Proach...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "34e05e39-f7ff-44a8-a8bc-0285b0cc0edd",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "No, not here. Ve should go to courtyard to talk... ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeScene",
                                    "sceneId": "cf824b0f-5af6-4a19-bac8-552a2a1bbebf",
                                    "spawnId": "a570406d-caf5-4e75-9778-1efea7ac71c5",
                                    "fadeTime": 2
                                }
                            ],
                            "expression": {
                                "type": "expressions.and",
                                "left": {
                                    "type": "expressions.inTags",
                                    "scope": "actor",
                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "scopeFilterId": null,
                                    "tags": "EsmeProach",
                                    "has": true
                                },
                                "right": {
                                    "type": "expressions.and",
                                    "left": {
                                        "type": "expressions.inTags",
                                        "scope": "actor",
                                        "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                        "scopeFilterId": null,
                                        "tags": "EsmeProachTalk",
                                        "has": false
                                    },
                                    "right": {
                                        "type": "expressions.inTags",
                                        "scope": "actor",
                                        "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                        "scopeFilterId": null,
                                        "tags": "HadProachCourtyard",
                                        "has": false
                                    }
                                }
                            },
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "2179d47e-92a5-4737-8560-393898280072",
                            "text": "Flirt With Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "aeafae3e-57ca-46f8-967c-4bee0159c7fb",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Wouldn't it be romantic to sneak back here after curfew and cuddle by the fire, just the two of us?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b5599f88-e524-4bd7-86e2-c30201f12ab3",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Depends upon vhat you mean by <i>cuddling</i>.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "238327f7-9333-4e64-9d95-e42caa258fe4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I just meant sitting together on the couch with my arm around you while we watch the fire and enjoy the warmth... and company...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "39a490cf-9968-4735-b7a0-b491d0ab3bca",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You do not make good husband, Shawn. You have no home. You have no money. ",
                                    "emotion": "0",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.reputationComparison",
                                                "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "operator": "gte",
                                                "value": "20"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "2eb2789d-dee0-4a0e-abbe-46918eb4efc6",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "amount": "1",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "74eaa81d-c4f9-494e-b921-8857635179a2",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "But you are a good friend, Shawn. Maybe you and I vill be more some day.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a5e79d1d-9c3d-4943-9781-1e01a669d69a",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Cuddling is something you do vith lovers, Shawn. Ve are not lovers.",
                                                    "emotion": "0",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "6",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "71d6f28f-837b-4366-98ee-3dce60814b9c",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "6046f41d-4c6f-49e6-af02-b1a107be421f",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Sweet dreams, Esme.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3e77b2f8-4abb-4021-a7ea-f894d0cfdb13",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Good night to you, Shawn.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
