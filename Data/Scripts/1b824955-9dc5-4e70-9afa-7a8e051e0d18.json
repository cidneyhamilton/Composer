                        },
                        {
                            "type": "options.text",
                            "id": "95811ea3-3c0a-4c91-8f2a-765cb8966f65",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "3be44cb2-64a3-48c3-81e4-4ed1f179fc87",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "See you around, Thomas.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d92b81b6-3037-4b91-a4a1-07c3fd0fe01b",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'll see you.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
