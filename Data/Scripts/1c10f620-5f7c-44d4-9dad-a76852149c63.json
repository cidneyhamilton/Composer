                        },
                        {
                            "type": "options.text",
                            "id": "127efc4e-07c2-40ad-828f-98486fbe433e",
                            "text": "Reach Into the Grate",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7268be70-d392-483e-88a7-7c40c6d205e3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Um...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "75eac911-ee58-4915-a197-4ee82d9e0ef0",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I don't think my arm would fit through. The muscles are so big, you see. It's not anything I can help.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
