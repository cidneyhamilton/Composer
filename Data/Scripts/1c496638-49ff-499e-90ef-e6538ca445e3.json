                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "aca17647-cb1b-4cc0-9f88-ea3e88fda349",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The locket isn't locked, so I don't need to be picky.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
