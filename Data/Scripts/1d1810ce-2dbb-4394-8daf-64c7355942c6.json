                                {
                                    "type": "nodes.comment",
                                    "message": "",
                                    "commentor": "Drat Queen is Defeated by Shawn"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "3ec7237c-65ea-4a23-9e21-697f17a2dbe8",
                                                    "scopeFilterId": null,
                                                    "tags": "DratQueenDead",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "scene",
                                                    "scopeId": "a8efa640-212a-4108-8251-612933f66abc",
                                                    "scopeFilterId": null,
                                                    "tags": "hadDratCongrats",
                                                    "has": false
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "27948157-9d24-41c0-a713-0f4163305b42",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "So you were the one who defeated the Queen Drat, Shawn? Esme and I found the body late last night.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "85c56a67-b463-44d6-97b9-7ba95a333d9a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Yes, she put up a fight, but I put her down. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fb32d83c-fb1b-47a9-8684-9284794b4194",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": null,
                                                    "text": "Good! She vas vicked and evil. She deserved to die.",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "scene",
                                                                "scopeId": "9897ecb3-4200-4884-867a-2840471747b8",
                                                                "scopeFilterId": null,
                                                                "tags": "AeolusGone",
                                                                "has": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "48b6d619-e6b3-4335-bada-02943e7956ac",
                                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "text": "Why do you say that? I mean, aside from the invasion of the Warrior Drats and the attack on all the students at the school, maybe she was just trying to protect her home.",
                                                                    "emotion": "4",
                                                                    "emotion2": "4",
                                                                    "device": "0",
                                                                    "device2": "0",
                                                                    "notes": ""
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "9d9e3509-ab40-4404-919e-fbbb4b1d548c",
                                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "actorId2": null,
                                                                    "text": "Anyone who starts a var is evil. She sent her Drats to the castle to be killed.",
                                                                    "emotion": "3",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0",
                                                                    "notes": ""
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "da15d01a-a01e-4bb1-994f-a9370edc1ab7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "In the end, she was just another dirty Drat.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "453a4a87-b56b-4330-8378-01467a772fc0",
                                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Good going, Shawn. I guess you do have a bit of hero in you.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "scene",
                                                "scopeId": "a8efa640-212a-4108-8251-612933f66abc",
                                                "scopeFilterId": null,
                                                "tags": "hadDratCongrats",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "06b8e550-355d-4a2d-a9d7-53430dc3c6b1",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": null,
                                                    "text": "Esme and I found what we thought was the Drat Queen's lair, but all the Drats were gone.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fd7d508d-eb83-4e47-8827-3c28cbe30850",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The Queen Drat? I forced her to leave the castle for good.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f9974b10-6289-42ec-b0f2-b9dad03301d4",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "How did you make the Queen Drat leave?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0764687e-fe92-49cd-a35f-3df1146e985a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Let's just say I made her an offer she couldn't refuse.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d95d0925-03c4-4e6c-9835-b70933d2e203",
                                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Why didn't you say you stopped the invasion earlier?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "53a41476-5adc-4e7a-8584-b38b3add47e6",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "First I wanted to make sure that the Drat Queen stopped the invasion. If she didn't, then well, it would take more than words to stop her.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2efc9e7d-416b-4e4c-8301-df4c3cb7dc3d",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Very good, Shawn. You are hero of all school!",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.invokeScript",
                                                    "id": "45e35cbd-df4d-44c7-8144-cb74a61df45f",
                                                    "scriptId": "1d1810ce-2dbb-4394-8daf-64c7355942c6",
                                                    "entryPointId": "c1b62487-9205-4dde-9d73-b464fe7fbd43",
                                                    "currentScope": "Current",
                                                    "currentSceneId": null,
                                                    "currentActorId": null,
                                                    "currentStoryEventId": null,
                                                    "currentPropId": null
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": null,
                            "nodes": [
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "c485bbbd-b334-41c4-9f19-94b52f87fc45",
                                    "scriptId": "1d1810ce-2dbb-4394-8daf-64c7355942c6",
                                    "entryPointId": "c1b62487-9205-4dde-9d73-b464fe7fbd43",
                                    "currentScope": "Current",
                                    "currentSceneId": null,
                                    "currentActorId": null,
