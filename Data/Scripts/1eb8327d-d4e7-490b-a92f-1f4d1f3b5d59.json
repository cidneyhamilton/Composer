{
    "type": "script",
    "id": "1eb8327d-d4e7-490b-a92f-1f4d1f3b5d59",
    "name": "Interact with Music Box",
    "sceneId": "060e6095-bf24-4ad0-95ab-fd69ad9df67e",
    "propId": "19938816-da82-430d-b8e8-9349b8827514",
    "actorId": null,
    "trigger": {
        "type": "triggers.interact",
        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "ce84afe8-0a3e-4c8c-98b0-1ffbb63b3687",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "ecbce667-ba0a-4865-8d48-399b4f906a41",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "2323f148-743b-4a0c-ae77-e37b4d0a1fba",
                            "text": "Look at the Music Box",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "Looked",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7c0de9d1-732b-4c50-9d05-19f328b62244",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "From here, that looks like a music box. A pretty fancy-shmancy one, with the ballerina on top and everything.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8588cfc2-6279-4748-8289-acc88b7d37a7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It seems a little... <i>delicate</i> for a place like this.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0d133d8b-47a3-4413-8bbf-88e7e8f8c565",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Hey, I likes pretty things just as much as the next disembodied spirit.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "a19c3f93-03dc-46a6-a5d3-b00ff718bb98",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "Looked",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "924d12af-5f41-4705-8d43-b1908bcc8e02",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It's a sweet little music box. I mean, I presume it's sweet. Unless it plays Wagner.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "07aaf9d8-59d9-4a87-a3d7-48ee6b5a9ffd",
                            "text": "Buy the Music Box",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "Bought",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6cb25ec9-c71b-4463-9d80-f736d61d959d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "Is the music box for sale? It might make a good gift.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cbc9b2c7-03f3-46e4-815b-5618b9f7cd2e",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You couldn't afford it. It's magic, y'see.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c64fda8b-3e78-4345-a96f-d0f7e6c13da3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "Magic?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "57634982-81ef-489b-bc48-cdf76495cf43",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You open it up, it plays music.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a5c3ffd3-1800-463f-a9d6-d58db852b9c8",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "I think it's <i>supposed</i> to do that. ",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "79f1cd34-da01-400c-bfa5-eeb2bbd5ae6f",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "What? ",
                                                    "emotion": "4",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2d26dece-23b1-4ee6-b400-82f0d2213bfd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "Yeah. There's this little cylinder inside that turns around if you've wound up the spring, and it plays a little melody. ",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1cbb07df-9b7c-4463-9ff5-af01cc8429d2",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Well, I guess that explains it.",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d12a34fe-a344-41cd-a554-e8296907d9d7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "So will you sell it to me?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e2c0de46-4a65-4178-8f3c-b5527288805d",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "... you still couldn't afford it. Just because <i>you</i> know it ain't magic don't mean I couldn't sell it as magic ta someone who ain't a know-it-all like <i>you!</i>",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "ac63e7d8-ce72-463b-a0e1-648c51b6f968",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "Bought",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "BoughtTwice",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2a520bda-f8ca-46b6-be95-73c4efcff0cc",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "You can't sell that music box as magic. If they break it, or open it up the right way, they'll find the works inside. They'll find out they've been cheated.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "36610bb7-0d4a-40bf-8f2c-5be589bbb6bd",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "So what're they gonna do? Kill me? ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "65dc42be-108d-4290-97df-0614966f5c5f",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Been there, done that!",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "cd04958c-379c-4169-be2d-842faef2df3d",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "BoughtTwice",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9fa46ef9-8c5d-4f61-afd6-877036b78d2d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "He's pricing it out of my range. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
