                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "1",
                                                            "ExitMenu": true
                                                        },
                                                        {
                                                            "type": "options.text",
                                                            "id": "bdeecdac-a868-43e4-8a3e-2a12c230ac3e",
                                                            "text": "Put Off the Job",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "c55a456b-96c7-4d96-9bbc-dba3dc3d33eb",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                                                    "text": "Maybe some other time.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "7eef9820-dd6e-46d2-92ff-2bddb0f2b032",
                                                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
