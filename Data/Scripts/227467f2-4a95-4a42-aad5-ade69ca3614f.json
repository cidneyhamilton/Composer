            "nodes": [
                {
                    "type": "nodes.changeTags",
                    "id": "fe1efb9a-ab05-4664-ba01-4f735a1d4745",
                    "scope": "actor",
                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
