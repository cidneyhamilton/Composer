                                        },
                                        {
                                            "type": "options.text",
                                            "id": "cc5c62e7-292b-4ae9-8403-3663c294b816",
                                            "text": "Support Katie",
                                            "nodes": [
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
