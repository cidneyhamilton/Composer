                                        },
                                        {
                                            "type": "options.text",
                                            "id": "7743c6bd-1b21-4f7f-828c-fb9abbc6d70c",
                                            "text": "Say Nothing",
                                            "nodes": [
                                                {
                                                    "type": "nodes.invokeScript",
                                                    "id": "4b7495be-baf8-48de-9366-a9764610bf70",
                                                    "scriptId": "d427356c-b661-420d-a0e5-809908e768bc",
                                                    "entryPointId": "",
                                                    "currentScope": "Scenes",
                                                    "currentSceneId": "ea6c0ba0-0d23-4c0d-80ed-b8611588a665",
                                                    "currentActorId": null,
                                                    "currentStoryEventId": null,
                                                    "currentPropId": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "eb0a9bb9-a00e-470e-b9e6-09b6b550f410",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Caesari Sosi - you think you so great? You lie, you cheat, you sneer at everyone else. Someday, someone going to rip your throat out like you deserve. No one cry at your funeral. Good riddance.",
                                                    "emotion": "3",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": 0,
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                },
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "01c27dfd-be75-43af-80fb-02d07cd78a21",
                                    "scriptId": "b7c6c182-81f2-4413-afa0-a82e7df107d7",
                                    "entryPointId": "",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "ea6c0ba0-0d23-4c0d-80ed-b8611588a665",
                                    "currentActorId": null,
