                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "a08551c5-0451-4b6e-8f90-682f42671069",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                                    "text": "Sosi, you are a few cards short of a full deck. Katie's the Queen of the Pirates and you're just a joker.",
                                                                    "emotion": "1",
                                                                    "emotion2": "3",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
