                                        },
                                        {
                                            "type": "options.text",
                                            "id": "8a532921-067a-4331-8ff3-0f953de7ad38",
                                            "text": "Defend Katie",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1419c3ba-57b0-4a63-b63f-4c2a16d66bc3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "text": "When it comes to risk taking, Katie's shown more moxie and cleverness than any of us here. It's clear to me that Katie's future will be smooth sailing.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
