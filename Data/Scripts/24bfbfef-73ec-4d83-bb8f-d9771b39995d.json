                                    "id": "17ad1740-b106-4950-9c4f-0588ab849f54",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "There's the overwhelming odor of Drat coming from behind the door.",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
