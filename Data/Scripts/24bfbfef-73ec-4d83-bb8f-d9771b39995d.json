                        {
                            "type": "options.text",
                            "id": "b720ed21-54bf-44d7-b43e-ddba5cfe62a5",
                            "text": "Open the Door",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "65eaa2d1-b76d-4a8b-be7c-2962e25fa44d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "This door won't budge. All kinds of junk are blocking it. It would takes days to move all that junk away. That's not something I want to do.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
