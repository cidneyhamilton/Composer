                                    "type": "nodes.speak",
                                    "id": "eb965625-a8d1-4109-a102-58c7e6e60382",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Why would something this ugly and lying in a pile of junk look brand new?",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
