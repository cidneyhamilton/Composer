                                    "id": "08efef29-bc94-4816-be5e-8c35ffae541b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The sound is muffled, but I think I can hear things with sharp claws moving around in the next room.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
