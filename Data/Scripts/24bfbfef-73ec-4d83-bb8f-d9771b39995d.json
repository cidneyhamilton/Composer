                                                    "entryPointId": "f4989865-6108-4751-9ef7-32ca01e6399e",
                                                    "currentScope": "Scenes",
                                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                                    "currentActorId": null,
                                                    "currentStoryEventId": null,
                                                    "currentPropId": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5deb09e3-edfb-4f7d-8d00-528d4ce6bda3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The tacky-looking pair of supenders are magical... I sense that they are defensive in nature... and they protect against panda attacks!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "7ea72f87-5027-4a91-9365-5cb6a167814c",
