    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "76d6b3e7-c593-4bf6-9f6f-653633e2dc91",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "a2e15554-524d-4310-99b3-1fce79ee92da",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "b41d00b4-1f1d-4385-85a4-b005fa06b536",
                            "text": "What's New With You?",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "scopeFilterId": null,
                                                "tags": "EsmeDatesAeolus",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "595f67e1-d116-4ab2-a095-26d0cef40ed1",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "So are you still dating Esme in addition to all your studying?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "190ae21b-bbee-4f4e-acd2-63a570396631",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I tried to tell her how important this final will be to my future. I hope she understands why I didn't see her today. If I fail this final, I may never see her again!",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                "scopeFilterId": null,
                                                "tags": "AeolusHappy",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "153e5a5f-9e95-4227-bfe4-0a819ec8ab7b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "So what's up besides the finals? Can you take a break and let me distract you from your work?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4d945d3b-c9cd-455d-be4b-0fabac239235",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": null,
                                                    "text": "Let me get done with about ten more pages of essay and then maybe I can take a few moments for you.",
                                                    "emotion": "2",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f8a0e447-cdc5-4efa-8600-e44429c59a66",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "By that time, I'll be in bed.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3a1c36ea-0248-48ac-ba17-313197d492d3",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Works for me.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7982dc25-ca09-47fa-b6d2-ee77c4225186",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "So do you have anything in your life other than schoolwork right now?",
                                                    "emotion": "0",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2a4ab9dc-ecd8-49ce-8b41-040c3da9512a",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "<shrug> It keeps me from being too lonely.",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e1e165c0-8db7-4e4e-b7b3-73101cf65e3c",
                            "text": "Ask About Finals",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "26f37b97-11bb-4394-af15-6e3b8e344ae7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "So how are your Finals coming along?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "199f086e-236d-4437-a228-8e3b9f55de9f",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": null,
                                    "text": "I finished my song about the Bard School...",
                                    "emotion": "0",
                                    "device": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "5d89b87b-7999-4fa6-8ca9-1bde83a9e836",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": null,
                                    "text": "Bards go to class\nEvery day\nStudy our lessons\nConstantly play.",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3274d092-d7da-43b8-83d2-b1f50e88863b",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": null,
                                    "text": "And we work hard,\npracticing long\nDoing our best to express \nOur need to impress the Maestro in Song!\n",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4413ecbc-21e5-4120-9d78-543dd807c86f",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": null,
                                    "text": "And the show must go on and on and on and on!",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "9a19c35e-efdb-44af-b83e-c2757fe7842a",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": null,
                                    "text": "We are the heroes of U.\nWe'll keep on singing\nTo get through.",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "ab80dcb0-c95b-4cf9-ac28-d81034c27ac6",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": null,
                                    "text": "We are the Bard School\nYou know that we rule.\nShowtime for jongleurs\n'Cause we are the Heroes of the U\n",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3b3c09d3-dbb9-4438-b34a-71cca9758577",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "What do you think? Will it get a passing grade?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "cc11af2e-3b16-42b8-8768-1356e9b1c026",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "c140e7ba-ff97-4bd5-9931-bbf7782a097e",
                                            "text": "Answer Politely",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "99f9ea53-e67b-4844-aa58-5b2e704d3c4e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "It should impress your Maestro that you actually managed to use the word 'jongleurs' in a song.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "c5ae60ca-595c-4b47-abb1-46970678e264",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "90",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "7a16c9be-2922-46cf-bc13-6771b00ab42c",
                                                                "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            },
                                                            {
                                                                "type": "nodes.speak",
                                                                "id": "d9d40edd-e7da-4a84-98c2-ce8c330c5fd5",
                                                                "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                                "actorId2": null,
                                                                "text": "<grin>",
                                                                "emotion": "1",
                                                                "device": "0"
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.speak",
                                                                "id": "7de4a74b-238c-4cc5-9049-08e28530f3b8",
                                                                "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                                "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                "text": "If that's the best thing Maestro Lorenzo finds in the song, I'll be lucky if I pass.",
                                                                "emotion": "2",
                                                                "emotion2": "0",
                                                                "device": "0",
                                                                "device2": "0"
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "faf9dc47-b307-4a17-863a-482c63a19424",
                                            "text": "Answer Cleverly",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "66644a11-8a83-434d-86a8-5b62cf9e53ea",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "To answer your question, this song is the best one to get a great grade for this serenade.\n",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "b2ae37e8-37d1-427b-9226-0430c02a820a",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "90",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "4d942114-ae3b-4d9c-90c2-2f08c56e56b0",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "amount": "1",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0b970a88-48dd-468c-a4c0-38f916c23548",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": null,
                                                    "text": "<grin>",
                                                    "emotion": "1",
                                                    "device": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "f8cda513-595c-4677-8d1a-33531cbacd5a",
                                            "text": "Answer Snarkily",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "852b83f9-9d15-499e-9344-be713908264d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "That's your premise - 'Bards rule'? Does that mean 'Warriors drool'? Is it written for pre-schoolers?",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "a6c2cbbc-9df3-4e33-a4e9-3ed882882699",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "90",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "0415cbf6-9410-4600-ae66-3e88790c864a",
                                                                "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b718014e-4d00-4116-a76a-13433db6534b",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": null,
                                                    "text": "<sigh>",
                                                    "emotion": "2",
                                                    "device": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "10",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e88bc8a5-7bb0-4ae1-8841-168471b46c6b",
                            "text": "Flirt With Aeolus",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "0a460d3f-25a3-4d9e-9db1-44ca2bc5bb01",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "I've heard that exercise is a good way to relieve stress. We could do some pushups together.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "00ca7cb9-207e-4545-becc-e5e5819d9aac",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'll settle for a hug to shrug off the stress of my test. If I don't get this through, it'll be toodleloo to you and Bard School.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                "scopeFilterId": null,
                                "tags": "AeolusHappy",
                                "has": true
                            },
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "110bdda6-c994-4433-a418-94b10253e2f2",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "aeab6e69-949b-4f92-95a7-c44ad4aec900",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "Try to get to bed tonight or you'll fall asleep during your test.",
                                    "emotion": "0",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7c059880-5222-4192-81ea-e7cd1d35a7aa",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Sleep is for the weak. I have to pass or this class is my last. I can rest after the test.",
                                    "emotion": "2",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
