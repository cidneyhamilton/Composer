                            "text": "Say Something Snarky",
                            "nodes": [
                                {
                                    "id": "1dd4968a-e3ad-49bf-aad4-a8c734a1a95d",
                                    "type": "nodes.skillBranch",
                                    "skill": "Moxie",
                                    "target": "98",
