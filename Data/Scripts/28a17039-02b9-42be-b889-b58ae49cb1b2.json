                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                    "actorId2": null,
                    "text": "The rough seas be behind us and thar be smooth sailing ahead, Maties!",
                    "emotion": "1",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0",
                    "notes": ""
