                                {
                                    "type": "nodes.speak",
                                    "id": "ea5436cf-8d00-4271-84a5-ac7f4a25fd08",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "And here's a toast to Sosi - who tried to make our lives miserable. For all his attempts at making us look bad, making us feel bad, trying to use us, and trying to get us kicked out of school, he failed.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0",
                                    "notes": ""
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "af3525a4-986e-4b37-848b-d527018a7871",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "May we always raise a toast to our enemies - and have the satisfaction of laughing at their failures.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0",
                                    "notes": ""
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": true,
                            "notes": ""
                        }
