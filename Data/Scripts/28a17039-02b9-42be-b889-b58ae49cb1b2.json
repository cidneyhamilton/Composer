                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                    "actorId2": null,
                    "text": "To the best friends von can ever have!",
                    "emotion": "1",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0",
                    "notes": ""
                },
