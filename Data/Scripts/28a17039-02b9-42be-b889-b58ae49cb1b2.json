                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "Conner - stop wasting time. Follow me.",
                    "emotion": "3",
                    "emotion2": "3",
                    "device": "0",
                    "device2": "0",
                    "notes": ""
