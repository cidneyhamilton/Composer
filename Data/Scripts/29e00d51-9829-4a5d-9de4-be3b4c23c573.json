    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "5d277be3-d077-471a-a560-846b990fb04a",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "scene",
                                "scopeId": "5c81204b-2c5b-4eb0-9e84-85acb2fc9d13",
                                "scopeFilterId": null,
                                "tags": "safeunlocked",
                                "has": true
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "49e67ff5-e400-41a0-a798-34b63c8942e5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "text": "Sir, may I speak with you for a moment?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "9ac9d1c1-16e2-4485-8ea8-395d51eaa13b",
                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Indeed, as long as you keep it brief.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "98dd60e1-01e4-4dc0-9d90-48a403ce5650",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "49e0d3fa-5706-4242-ae3c-c6add64f2cd8",
                                            "text": "Talk About the Drats",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "991524a3-24a0-421f-8990-2b6fa370667d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The Dire Rat situation is serious, sir. The Warrior Drats are being sent to the castle by the Queen Drat. I think it will take more than killing a few Drats to stop them.",
                                                    "emotion": "0",
                                                    "device": "0"
                                                },
                                                {
                                                    "id": "812d839f-b873-4f47-bcf1-d30ec0520f0c",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "80",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0f90f74c-4291-4075-8713-67c1a6291208",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": null,
                                                    "text": "What is it that you suggest?",
                                                    "emotion": "4",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.showMenu",
                                                    "id": "6398ec4e-ae6a-4d0f-8733-e37713d481fc",
                                                    "options": [
                                                        {
                                                            "type": "options.text",
                                                            "id": "63bdfada-bfdf-4582-ad53-3542e9846501",
                                                            "text": "Volunteer to Stop the Drat Invasion.",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "33d46cbf-3701-458c-89a5-e446997de363",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "I intend to find the Queen Drat and stop the invasion.",
                                                                    "emotion": "0",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "71d76a38-c78e-41fd-a71e-dca96329c73e",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": null,
                                                                    "text": "How very heroic of you... and foolish. You are a Rogue, not a Paladin.",
                                                                    "emotion": "4",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "17a64364-ff27-4aca-a22a-9c1d47fed268",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": null,
                                                                    "text": "I do hope your skills are equal to the dangers you will face to do so.",
                                                                    "emotion": "0",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "9181189f-8b78-408d-8924-e1ccac9bd738",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": null,
                                                                    "text": "Death is the ultimate failure for this class.",
                                                                    "emotion": "3",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "473d378c-cd97-475a-a122-2a2400688fd1",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Death won't cause me to fail this class, sir.",
                                                                    "emotion": "1",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "72fcb5a1-caaf-406c-a093-7cbce37252f7",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": null,
                                                                    "text": "See that it doesn't, Herr O'Conner.",
                                                                    "emotion": "1",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.changeReputation",
                                                                    "id": "cd14a487-300e-4688-975a-67f7ef413fc3",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "amount": "5",
                                                                    "change": "1",
                                                                    "notify": true
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "6",
                                                            "ExitMenu": true
                                                        },
                                                        {
                                                            "type": "options.text",
                                                            "id": "e8d69131-f8ac-4efd-90eb-75ee42871786",
                                                            "text": "Suggest Lady Moira Stop the Drats.",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "1bc9a312-ab53-4198-b68f-5febdb59cea8",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Lady Moira, the Paladin instructor, needs to know about the Queen Drat. Then Lady Moira could stop this invasion.",
                                                                    "emotion": "0",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "e0bd8f7a-7632-4819-808b-46aca4f8ea44",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": null,
                                                                    "text": "I suggest that you speak with her about the matter. Your information could be quite useful.",
                                                                    "emotion": "0",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.changeReputation",
                                                                    "id": "d1c51a36-c1c4-4c8f-b7b5-bed065be834c",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "amount": "1",
                                                                    "change": "1",
                                                                    "notify": true
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "4",
                                                            "ExitMenu": true
                                                        },
                                                        {
                                                            "type": "options.text",
                                                            "id": "75edc154-8e42-436b-bda5-cb6eaac06fae",
                                                            "text": "Suggest the Master Stop the Drats",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "01e2a6af-06d6-406f-916f-4ec657407644",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "You could do it, sir. With your skills, you would have no problem stopping the Drats.",
                                                                    "emotion": "0",
                                                                    "device": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "04f79c36-c97a-450b-ac7e-bbff84ee9cc4",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": null,
                                                                    "text": "Indeed. However, I will point out that I am an instructor rather than a student at this university. It is the students who need to prove their skills, not I.",
                                                                    "emotion": "4",
                                                                    "device": "0"
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "4",
                                                            "ExitMenu": false
                                                        }
                                                    ],
                                                    "AutoAddDone": false,
                                                    "Unique": true
                                                }
                                            ],
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "scopeFilterId": null,
                                                    "tags": "DratQueenInfo",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.hasActiveQuest",
                                                    "propId": "677a9ff1-7927-4c91-9d08-f79ea52cf698",
                                                    "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                                    "questStatus": "0"
                                                }
                                            },
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "ff1457ed-e628-4d9a-9070-a7991b888513",
                                            "text": "Talk About Passing the Test",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "18568006-9f17-4714-993a-9719b20a7fce",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "So what did you think of my disarming skills there? Pretty slick, yes?",
                                                    "emotion": "1",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bf933cf8-10c0-4ffe-b7d1-733dc5a6e2fc",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": null,
                                                    "text": "You successfully disarmed the trap. I expected nothing less than that.",
                                                    "emotion": "4",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2f7ae404-217f-4f06-bb58-dd2ac7b8a019",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "text": "That's it? The least you could do is give me a Sir Goot or a clap on the back for a job well done.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bd1b7f9a-6e4f-4d58-9a53-9ae70b701005",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Herr O'Conner - you have successfully passed the Advanced Tool Use test. However, you are failing the mandate to 'show proper respect for authority.' Try not to push me too far.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fe63e2df-2b25-419f-9658-45125649e7b8",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "text": "Yes, sir.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1266db3b-4104-4dbb-a673-317c3f5ad61d",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Sehr gut, Herr O'Conner.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "2c221cf1-fa4b-4c8f-9470-c2217368bc16",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "80",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "5ff3b266-0e29-493b-a510-5f70b1ef26c0",
                                                                "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "6",
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "afdfcc8c-fafc-44ac-b390-31a2f76c680d",
                                            "text": "Say Something Snarky About Sosi",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "414ff674-e86e-461e-a5f3-302963b35d03",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "So, sir, shall we make a wager that I have taken fewer moves to disarm the trap than 'Herr' Sosi will take?",
                                                    "emotion": "1",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a0fc16a5-c41c-4ec1-9307-7620ba09d013",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": null,
                                                    "text": "That is not a wager I care to take. The odds would not be in my favor.",
                                                    "emotion": "4",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2de5afdf-e42d-423d-8c67-b51d136b5a90",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "<grin>",
                                                    "emotion": "1",
                                                    "device": "0"
                                                },
                                                {
                                                    "id": "2e23e402-f343-499c-a26e-812499dd5e58",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "80",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "88fac5c3-9a17-4177-90ad-456c74d0a537",
                                                                "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
