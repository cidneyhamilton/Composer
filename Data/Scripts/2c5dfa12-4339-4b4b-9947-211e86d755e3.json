                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "befeab84-a54c-4502-aeb8-775d077e812d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The plaque reads \"Ratatoskr - Battle Squirrel of the AGL\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7308270a-6093-4437-8d24-b8cb4c432f95",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Under this grave lies a thief\nWhose life was exciting but brief...\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a83c8e24-8c95-40d5-a38c-899ade3bd090",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"She robbed the wrong king\nand his sword it did sing...\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "79aaf33a-b9f4-4f36-b947-b800a2796d13",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"When it sliced her as thin as a leaf.\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "1e54bc4a-5691-4ca7-baeb-16eada935f09",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Rookie mistake, that one.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "4423e2fc-1274-4723-979c-a4323f37517b",
                            "text": "Remove the Plaque",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "96d33d0b-f475-4160-8f2c-7147d6c25ed5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It's tempting to steal this old plaque...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
