                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "a5e6f184-26cd-4ed9-86de-fbf2388fdfb8",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "56100280-4a0d-44ca-ab65-a4b6602700d9",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "text": "We'll talk later.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "80c12b98-ea6a-41c5-af4d-6c4be7315fae",
                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Maybe... Poobah games tend to run late.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
