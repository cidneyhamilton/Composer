                            "text": "Talk About the Captain's Skeleton Key",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "d3b7e2ab-53f2-488c-ac41-a7e927605299",
                                    "scope": "scene",
                                    "scopeId": "cf64f464-f1cc-4cd9-8125-3d1c2d8d45f9",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "TalkedAboutPirateKey",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8c225b4a-c54a-4ef7-ba0d-8ded379a9d79",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "text": "If you are looking for a key to finding your pirate treasure, this might be it!",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
