                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                    "actorId2": null,
                    "text": "Does anyone have any questions while we work?",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
