    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "00c1c74e-7e30-4adf-93bc-85db80072979",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.changeTags",
                    "id": "c3e1edd6-1ddf-42a3-af53-4afaf453685e",
                    "scope": "actor",
                    "scopeId": "6378f038-f4cd-49aa-8357-dcb46a978684",
