                        {
                            "type": "options.text",
                            "id": "345ca1bb-cdcf-4224-b6fd-5dc27a0cf2fe",
                            "text": "Look at the Books",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "28a8c326-b968-4c00-8ace-9ca2bf7114c3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I guess the Minotaur likes to read. He's amazingly literate for big nasty brute.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
