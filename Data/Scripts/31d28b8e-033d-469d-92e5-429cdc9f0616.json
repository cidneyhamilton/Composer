                                {
                                    "type": "nodes.setCameraTarget",
                                    "propId": null,
                                    "actorId": "5ffe0a3d-d152-412f-bc75-480092ca6256",
                                    "time": 0,
                                    "translateX": 0,
                                    "translateY": "2.0",
                                    "translateZ": 0,
