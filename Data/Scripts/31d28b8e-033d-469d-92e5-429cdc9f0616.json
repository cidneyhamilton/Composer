                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "232d238c-53e0-4ea3-b19a-074f7cecbe8b",
                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "actorId2": null,
                                    "text": "I trust that you all have taken the opportunity to master the basics of picking locks. With the technique in the instruction manual, you should be able to pick the basic locks on houses and small chests.",
                                    "emotion": "0",
                                    "device": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "97ba12a0-90d3-4d55-be1f-d193e9b20735",
                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "actorId2": null,
                                    "text": "For the more complex locks, you will need a combination of this skill with more advanced tools -",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
