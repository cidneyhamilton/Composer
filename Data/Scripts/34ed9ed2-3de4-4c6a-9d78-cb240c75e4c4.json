                                                    "id": "988c9aa7-bf7c-4da4-a391-0c84ba47ea14",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's nice knowing how to use all these things. It gives me a real sense of power. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
