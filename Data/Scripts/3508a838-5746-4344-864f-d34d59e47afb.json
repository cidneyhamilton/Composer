                            "id": "74831aed-9fa2-42da-ba49-b0f195904b3b",
                            "text": "Look at the Statue",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "9765439c-026e-4e2f-8fb9-ae3df4a26915",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Whoever says they don't appreciate the craftsmanship of this statue must be lion.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
