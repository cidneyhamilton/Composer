                            "id": "679b2d2a-1cae-42e1-8b93-d4ce8a572cb6",
                            "text": "Steal the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b3b3b860-b776-4633-86be-3976ea7002fe",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Aeolus barely leaves me enough room for my desk, there's no way I'd find the space for this one. Not that want it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
