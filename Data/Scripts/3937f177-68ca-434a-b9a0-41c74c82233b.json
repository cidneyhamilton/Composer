    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "dd34aecf-d273-4671-bb1e-8798a54df3af",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "8de213cc-15fb-4644-867c-6bb798d4e9cf",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "c3223c32-5173-4374-ba89-93f843b1ad31",
                            "text": "Look at the Relief",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "d59c1f67-9c0d-49f8-b32f-39fbe42a9eef",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Well... he's as handsome as he is smart.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "c1fbf367-f095-4b7a-8932-37289ca875d3",
                            "text": "Poke Fun at the Relief",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "Poked",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.nodeCycle",
                                                    "id": "f7ebc6f8-e7da-42f9-83d9-86f4b49e6e36",
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "053ca3b9-0eb4-4f35-b876-7d9ba96a3252",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "I used to think you Goons were a pain in the neck. But now I have a much lower opinion of you.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "474818f8-fac6-492c-9529-0d656d3662f9",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "You have the intellectual capacity of a small soap dish.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "1a55be77-2e18-49af-a95e-57cac468eec2",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "I don't think your belt goes through all the loops, if you know what I mean. And I bet you don't.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "d4773afa-2434-41f7-87ca-02baab11e5ba",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "You guys are proof that evolution <i>can</i> go in reverse.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "b249c7dc-ef29-4109-ac6a-fe71ec81be50",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Your lights are on, but nobody's home.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "9f88007b-103a-4028-b5ec-b3b1b6ba8ccc",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "You are one sandwich short of a picnic.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "1e21ecc1-05ff-4905-8fb4-b1da2e43ace8",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "If I hold my ear up next to yours, I bet I could hear the ocean.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "c5e9eb60-e893-4805-9cff-61c8ec9e1d61",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "On your family tree, you're the sap!",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "30107209-e66c-4d26-af6c-f4a8e06935f3",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "I love what you've done with your hair. How do you get it all to come out of one nostril like that?",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "b7837bc7-5b96-4101-a9cb-9543976bdeb2",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Y'know, brains aren't everything. In fact, in <i>your</i> case, they're nothing.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.branch",
                                                            "sections": [
                                                                {
                                                                    "type": "nodes.branchSection",
                                                                    "expression": null,
                                                                    "nodes": [
                                                                        {
                                                                            "type": "nodes.speak",
                                                                            "id": "c1fe8115-eba5-4adb-9919-02a9cbe29ece",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "text": "Is it true that you Goons conserve toilet paper by using both sides?",
                                                                            "emotion": "1",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
                                                                        },
                                                                        {
                                                                            "type": "nodes.changeTags",
                                                                            "id": "2ce0203e-97b2-494b-84bb-30d39e59eb4e",
                                                                            "scope": "target",
                                                                            "scopeId": null,
                                                                            "scopeFilterId": null,
                                                                            "tagsToAdd": "Poked",
                                                                            "tagsToRemove": null
                                                                        }
                                                                    ]
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "88296d5e-aa4d-494a-b9a3-e512c4f793d7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It's no fun if he doesn't get offended.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
