                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "320db9ed-35b4-48ec-84bf-e1828166222a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Would you believe that I'm the fire inspector and I'm here to inspect your fire?",
                                                    "emotion": "1",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4985c519-45a2-4eb6-84e6-4b31d4e82809",
                                                    "actorId": "9f5d3ba1-4925-4ddb-9e75-d5304fec4f25",
                                                    "actorId2": null,
                                                    "text": "A fire inspector? At this time of night? I don't believe it.",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
