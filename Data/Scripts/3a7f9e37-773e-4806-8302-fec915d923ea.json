{
    "type": "script",
    "id": "3a7f9e37-773e-4806-8302-fec915d923ea",
    "name": "Interact with Door to Great Hall",
    "sceneId": "af912e02-46a1-4d88-a547-59f3d2093c7b",
    "propId": "2803ef7e-c038-4f9d-8727-12ef359d9856",
    "actorId": null,
    "trigger": {
        "type": "triggers.interact",
        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "dfd28db0-901e-4977-9bce-edb9dac9b301",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "1be08de1-a7f6-4084-8c03-b41ff82a4db9",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "0c5e87f6-e2ea-4fe8-8163-8816985d6028",
                            "text": "Look at the Door",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "eefc76d3-9e1c-47fe-905e-66044f56c646",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I think it's the only way back up to the main floor of the castle.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "bd78764a-dcf2-454b-a46e-6468b8eecf15",
                            "text": "Leave the Room",
                            "nodes": [
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "d2a3e768-690b-4932-b8f4-4eb61f0b3d64",
                                    "scriptId": "a1004657-984b-46ba-8870-9550cbfb4b35",
                                    "entryPointId": "dfd28db0-901e-4977-9bce-edb9dac9b301",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                },
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "a7da7bc7-1867-4962-a41b-22dc7a44c4a7",
                                    "scriptId": "2ea5b2fe-28c2-4b9f-81e9-6dc2a5c4bcdf",
                                    "entryPointId": "dfd28db0-901e-4977-9bce-edb9dac9b301",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                },
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "b4adab26-2890-4a7b-988b-1788df28612b",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "2309e1c7-12dc-4ba8-bd2b-2bfdafa97ca0",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "There sure are a lot of stairs in this place.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "ecb902ad-b0e2-47f4-a321-0bfcbd1af547",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Up and down... up and down...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "ac6b0839-5c00-4b41-889e-68ec15eab8cc",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I get my exercise around here.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "d4db2b0d-43e1-40b9-a7e9-5c5e59136110",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Here I go again.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "a3953eb6-60d9-45bd-b102-8998b7fba45c",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Once more, I stare at the stairs.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "b7c325bb-772f-4338-913c-34aa9cd421c6",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "All this stair climbing is becoming <i>Un-stair-able.</i>",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "911db711-399d-47aa-82d0-9b24de12f0a3",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "If I ever open a school store, I'm putting it on the first floor...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "046e4938-a690-4e6a-b19c-7fce62ad8cd4",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Who left all these stairs around here?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "8325f779-1b07-4895-a6f5-9c32dbe672ce",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Just a few more... then I can collapse...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "744c1cfa-9b26-4a3b-915f-a4c188ee80e3",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "It's like they're forcing us to work out...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "4fd71250-b67d-45ec-9f95-909a9233619f",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I think I've developed an allergy to stairs.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "5d540fb4-47a6-40d7-8572-7973ed2068c0",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Gotta get Gregor to install a ramp.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "bff87961-6c85-4f6b-863f-258e840e2415",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Whose bright idea was it to come down here in the first place?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "44ecc0fa-1fa4-4957-ba38-d33e0915b8d0",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "These stairs are giving me nightmares.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "0befaa12-f809-4f5a-9203-b9ffdf55410e",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I don't suppose there's a shortcut...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "27b44f70-6712-41dd-ae8a-e921ccf78b11",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Sure, <i>Gregor</i> never has to climb these stairs... what does <i>he</i> care...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "72fb6d2e-f0b3-4c36-944a-caf421a760d8",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "One of these days, they're going to find my body in one of these stairwells, dead of exhaustion. Then they'll be sorry.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "71b0da15-6e5a-4212-a5ae-045da9377b3d",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Either these stairs are getting steeper, or I'm getting older.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "b5f2afb7-3abb-4e82-bdee-d7a8c7e50548",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I'd pay someone to carry me up.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "8776e99b-67f1-4335-9831-58b25fed8ef2",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Oh, right... stairs.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "3f5504fc-cc53-47c0-9335-602bca5d03c9",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Every time I climb these stairs, I close my eyes and imagine I'm napping.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "58a3ec61-1fd3-457d-b046-46e6d6d68324",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "It'd be easier to just stay down here forever.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "05bacffd-8cf4-4e35-9328-9a73b2ecad09",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Is this what they mean by achieving higher consciousness?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "23c97944-d7f3-4a15-96b3-f060f4e52835",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "What I wouldn't give for a good levitation spell...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "25172836-e314-4dc7-9c23-f5c5fa1d7be8",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "This castle needs a big label out in front that reads, \"Warning: Stairs!\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "8150aca3-877e-4451-a496-e61254dbc5cf",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Aww, stair-climbing... my new favorite hobby...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "e7dee480-96fa-46ba-8818-d17119b4fa94",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "What I really need is a jolly stair-climbing song.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "45c6e869-6415-4e7c-aaba-99d70515bee8",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I wonder if these stairs can feel my seething hatred.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "847a7552-e3ab-4030-bc49-e942a694d6cf",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Never trust stairs. They're always up to something...",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.changeScene",
                                    "sceneId": "40f18665-4cc2-4acd-b17e-5189e68b7a33",
                                    "spawnId": "c10ac42a-7bed-4bad-a3d4-085aca40186e",
                                    "fadeTime": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
