                                {
                                    "type": "nodes.speak",
                                    "id": "417e06cf-9004-434f-ad9c-14924fdb9286",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "There's nothing of interest behind this banner. I'm not surprised; in back of these banners would be the last place I'd hide something. Too apt to be accidentally exposed. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
