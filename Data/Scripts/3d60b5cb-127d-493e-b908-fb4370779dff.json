                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "My heart is broken. I'll never be able to sleep with this pain.",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
