                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "Good night, Aeolus. Try to get some sleep.",
                                                    "emotion": "0",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
