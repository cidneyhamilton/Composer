                            "text": "Read the Book on the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "5bdf38cb-afce-4278-84d4-eb3d28793779",
                                                "sceneId": "47ebdad9-eb30-4230-b1b2-b576e5ccbe0d",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.playSoundEffect",
                                                    "soundEffectName": "book_open.wav",
                                                    "wait": false
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "57467823-bf9d-49fa-b675-225b76f99a0e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's the essential \"Little Black Book of Chartered Accountancy.\"",
                                                    "emotion": "0",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7c6c1eb9-1270-4a49-a376-516796aca234",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I am <i>so</i> glad I'll never be an accountant. That stuff is dangerous! It could bore you to death.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.playSoundEffect",
                                                    "soundEffectName": "book_close.wav",
                                                    "wait": false
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0e02bc0f-0a6d-4f68-abb5-c2e157f5936a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It might be an interesting read, but for now I'm in the dark about it.",
                                                    "emotion": "0",
                                                    "device": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "5",
                            "ExitMenu": false
