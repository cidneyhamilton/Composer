                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "34579b42-8e0c-4c1a-a61c-497c4862f20f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "That makes things so much brighter.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
