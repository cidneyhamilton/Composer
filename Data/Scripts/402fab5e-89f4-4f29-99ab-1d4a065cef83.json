                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8fc7ab00-8726-4c61-b9ad-35a7522fba78",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can either turn the light on or stay in the dark like I've been for a long time.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
