                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "5bdf38cb-afce-4278-84d4-eb3d28793779",
                                                "sceneId": "47ebdad9-eb30-4230-b1b2-b576e5ccbe0d",
