    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "81d2f3f1-dbe9-428b-b35a-4b290e30f660",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.speak",
                    "id": "b77bf48f-bc21-4bec-8207-0f6fc727d135",
                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "Look at her over there - she's like some goddess of the forest, beautiful and awesome. I want to worship at her feet!",
                    "emotion": "1",
                    "emotion2": "4",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.speak",
                    "id": "7ed1c3cf-0791-4e95-812c-ea96fd06213f",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                    "text": "Who, Katie?",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.speak",
                    "id": "74ec3372-02b4-4cd1-b64d-23fdfa32d73c",
                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "No, not Katie. She's no match for the wild loveliness of Esme.",
                    "emotion": "0",
                    "emotion2": "4",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.showMenu",
                    "id": "c0742cd5-37e8-4bc3-b519-c8b2a89ec4fa",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "3497538a-96c5-4fc6-b51c-ea92faa808ad",
                            "text": "Ask About Aeolus",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "cb43b705-c470-44ba-ad6e-61ea47aebf75",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "So you've got a crush on Esme now?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "514f60b6-aec0-4357-bbc6-520cf4125cdf",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "'Crush' can't describe the overwhelming emotions that fill my heart whenever I look at my feral goddess!",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.hasActiveQuest",
                                                "propId": "bae259d2-1f5a-4258-a3ba-77edf21392ba",
                                                "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                                "questStatus": "7"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "63c2b55f-5735-41e8-8aa6-8150dd38b87f",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Please, Shawn, I know that you are friends with Esme. Can I talk to you about her later - in our room?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.showMenu",
