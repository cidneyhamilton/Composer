                                                    "options": [
                                                        {
                                                            "type": "options.text",
                                                            "id": "2b9bea40-972e-4bdd-b3ec-246c15c9d654",
                                                            "text": "Agree to Talk",
                                                            "nodes": [
                                                                {
                                                                    "id": "32c915a9-16b9-428f-9091-0bf4598b6592",
                                                                    "type": "nodes.skillBranch",
                                                                    "skill": "Charm",
                                                                    "target": "60",
                                                                    "success": {
                                                                        "type": "nodes.block",
                                                                        "nodes": []
                                                                    },
                                                                    "failure": {
                                                                        "type": "nodes.block",
                                                                        "nodes": []
                                                                    },
                                                                    "notify": true
                                                                },
                                                                {
                                                                    "type": "nodes.changeReputation",
