                                                            "text": "Waffle",
                                                            "nodes": [
                                                                {
                                                                    "id": "6866b26b-481d-4c73-bd6c-21207af3db6f",
                                                                    "type": "nodes.skillBranch",
                                                                    "skill": "Smarts",
                                                                    "target": "60",
                                                                    "success": {
                                                                        "type": "nodes.block",
                                                                        "nodes": [
                                                                            {
                                                                                "type": "nodes.changeReputation",
                                                                                "id": "b60a4a25-f11b-4674-b5f6-cbcdd512b81c",
                                                                                "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                                                "amount": "1",
                                                                                "change": "1",
                                                                                "notify": true
                                                                            }
                                                                        ]
                                                                    },
                                                                    "failure": {
                                                                        "type": "nodes.block",
                                                                        "nodes": []
                                                                    },
                                                                    "notify": true
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "ade5c773-f1c3-4a97-a1b9-859fea1ae6f7",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                                    "text": "I have a lot of things I need to do tonight, but I'll try to get back to our room before it's too late to talk.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
