                                                                                            "nodes": [
                                                                                                {
                                                                                                    "type": "nodes.speak",
                                                                                                    "id": "6f66133a-881a-412c-adfb-d04e164f1185",
                                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                                                    "text": "I mean, a nice <i>candelabrum</i>! A <b>nice</b> <i>candelabrum</i>! A <b>nice</b>... <b><i>NEOCLASSIC</i></b>... <b><i>CANDELABRUM!</i></b>",
                                                                                                    "emotion": "0",
                                                                                                    "emotion2": "0",
                                                                                                    "device": "0",
                                                                                                    "device2": "0"
                                                                                                },
                                                                                                {
                                                                                                    "type": "nodes.speak",
                                                                                                    "id": "49872eac-6594-4122-a8f5-42013c1c851d",
                                                                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                                    "text": "Don't you mean \"neoclassic<i>al</i>\"?",
                                                                                                    "emotion": "0",
                                                                                                    "emotion2": "3",
                                                                                                    "device": "0",
                                                                                                    "device2": "0"
                                                                                                },
                                                                                                {
                                                                                                    "type": "nodes.speak",
                                                                                                    "id": "dbace6a8-6583-4511-9e63-19e559321962",
                                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                                                    "text": "<b>ARRGH!!</b>",
                                                                                                    "emotion": "3",
                                                                                                    "emotion2": "1",
                                                                                                    "device": "0",
                                                                                                    "device2": "0"
                                                                                                },
                                                                                                {
                                                                                                    "type": "nodes.changeTags",
