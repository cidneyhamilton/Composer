                                                                                    "id": "77b0c390-27c3-4f37-84d1-df0e56d5bd79",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "text": "Ermahgerd! He's not here to stop me! Candelabra, candelabra, candelabra! It sounds right to <i>me!</i>",
                                                                                    "emotion": "1",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
