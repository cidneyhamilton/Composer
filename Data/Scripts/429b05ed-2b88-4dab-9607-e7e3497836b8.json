                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "Yeah, those things are pretty Ermahgerddamned vicious!",
                                                                            "emotion": "0",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
