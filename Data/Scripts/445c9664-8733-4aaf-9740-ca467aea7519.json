                    "actorId": "5ffe0a3d-d152-412f-bc75-480092ca6256",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "Conner - I want to speak with you. Clear your place and come to me.",
                    "emotion": "0",
                    "emotion2": "4",
                    "device": "0",
                    "device2": "0",
                    "notes": ""
                },
