                    "options": [
                        {
                            "type": "options.text",
                            "id": "5f1fae08-7964-46f2-9e14-055e14c3de9f",
                            "text": "Examine the Houdini 42",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "93b6ee33-5e20-4b52-9665-acf5c162a8b9",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Look at this thing. Not only can it help me with locks and traps, it can also be used as a corkscrew and a toothpick. It's the ultimate answer for all my tool-related problems.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8a2e08e9-b477-46a8-9d21-ade3b3fe39cc",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Now how much would I pay?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "1b0aa4a9-d4ff-4fff-b28c-fc50502b4928",
                            "text": "Pick your Teeth with the Houdini 42",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7a18c8d0-09ca-4837-9beb-a4eab52280b3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "OW!",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "27469cd5-0074-4281-b5b1-3449d2381386",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "8d67bd5e-99d3-412a-a1a0-fafae45eca2b",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Consarn it!",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "e4695899-8fa6-4c88-bf15-7194d3915e68",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Ahh... that's better. Piece of meat stuck between my molars.",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "0c93b43e-e9b1-44a3-940e-e84ed9ae0ae5",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "My gums are bleeding! I don't know why I bother brushing my teeth once a month!",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "e78a4d6e-a8d7-4d8c-86c4-35f85d1cf9ab",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Why doesn't this thing have a warning label?",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "c8d60663-805f-4782-99eb-d4860f278d85",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Pointy!",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "ee765b30-6b6e-440f-8d2c-56bf1a601d45",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I think I chipped another tooth!",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "1998265f-e6e2-4486-aa03-7b59eb88c9c7",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I jabbed myself with the corkscrew!",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "c7737cbe-c6ad-4149-89b8-1d9ad4228ace",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Why do I keep doing this to myself?",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                }
                            ],
