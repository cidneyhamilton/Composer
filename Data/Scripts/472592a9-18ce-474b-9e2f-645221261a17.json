    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "52e680dc-2031-4f97-8a19-dcdaa690a1d9",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "937624b4-305a-43a0-b2a0-558eac779bb5",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "821e7859-6710-4dbb-afbe-2c6649932f19",
                            "text": "Look at the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.showCloseUp",
                                                    "imageName": "EmilyFramed.png"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "bb40770c-a419-4add-b635-e2a52187a6be",
                                                    "scope": "prop",
                                                    "scopeId": "21668a1a-c982-432e-9843-a0fa2fd09da9",
                                                    "scopeFilterId": "298ff758-e1ce-477a-b9d4-da614b74072d",
                                                    "tagsToAdd": "Examined",
                                                    "tagsToRemove": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "36bdfa26-a455-4e51-a31d-ce32019a295b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "She looks like a nice person, but there's something melancholy about the painting. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e01873c9-00cc-4cbc-b02f-b3ab8b9bec1a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The plaque says \"Emily Kerson, Healer of Caligari. Her medical skill and gentle kindness helped to heal the wounds of the Dark Sardonia war. The only wound she couldn't heal was her own broken heart.\"",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.hasActiveQuest",
                                                                "propId": "d446df63-7edf-4c75-920a-f75fa60781f5",
                                                                "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                                                "questStatus": "0"
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "5e4e7a89-52f6-460e-9df5-bf341d227c98",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "This has got to be Patrick Ratana's lost love, Emily... or at least a painting of her.",
                                                                    "emotion": "4",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "03c47eed-2aa1-4c55-8656-9bb3ad445715",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Sad, they hang side by side here, but never see one another.",
                                                                    "emotion": "2",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.hasActiveQuest",
                                                                "propId": "d446df63-7edf-4c75-920a-f75fa60781f5",
                                                                "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                                                "questStatus": "1"
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "3cadd558-fc68-4bdd-86bc-9ea4da5cbaf0",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "She looks as though she is at peace now, or more likely that's just my imagination. It's still the same painting.",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "3a2f4519-032b-4e62-9646-d2ad6765f5c8",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "No wonder the painting seems so sad.",
                                                                    "emotion": "2",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                },
                                                {
                                                    "type": "nodes.hideCloseUp",
                                                    "waitForSeconds": 1
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9f2f306f-7671-404b-982a-5ded9f6e27e4",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It looks like a painting of a woman, but it's hard to tell.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "96aa9f2c-7fee-4c7b-9a15-262217b62679",
                            "text": "Look Behind the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.invokeScript",
                                                    "id": "8db97670-7664-4144-97be-430342e37dca",
                                                    "scriptId": "b034c7b9-29f5-46ab-b66a-3a39f340b33b",
                                                    "entryPointId": "52e680dc-2031-4f97-8a19-dcdaa690a1d9",
                                                    "currentScope": "Scenes",
                                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                                    "currentActorId": null,
                                                    "currentStoryEventId": null,
                                                    "currentPropId": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a0a19883-5cc1-4789-bf1f-4d3710232aca",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "<cough> Dust, dirty, and more dust. Clearly this place needs a maid or a butler.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9476796e-6822-4dd9-a733-b7e375028b69",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "There could be a treasure hidden behind this painting, but I couldn't see to get it. Good thing that's so unlikely.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "afbe2d31-1948-43c7-b7ed-4d318df7e7e0",
                            "text": "Give Emily a Rose",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4911ccf7-af0c-4bf7-92ba-f13cd2d77f89",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "If only I could give Emily a rose, maybe I could put Patrick's spirit to rest.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a15c1738-b746-4341-9b63-a9bf279edf8f",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I don't see any way to give a rose to her, though. It is only a painting.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inInventory",
                                                "propId": "6b99d059-b486-4547-abbc-8b3169478a39",
                                                "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b56eabeb-359f-4ab2-8ce1-375ee0bf0963",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Assuming, of course, that I even had a rose to give her... which I don't.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": {
                                "type": "expressions.hasActiveQuest",
                                "propId": "d446df63-7edf-4c75-920a-f75fa60781f5",
                                "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                "questStatus": "0"
                            },
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "cc53f702-1d43-4568-bc87-4cce32f2995f",
                            "text": "Steal the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "885fd2d0-2785-452d-9323-b7d1cbb781a7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I think that she is better left alone. Then again, that may have been why she died of a broken heart...",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "146db3d1-8721-4ea1-a6c8-e042ca33e187",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "No sense in taking something when I can't see enough to tell whether it is worth taking.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "8d60acb1-22b7-43f9-a5e4-6c27dc390e15",
                            "text": "Sense Magic on the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "311fc35a-6311-4176-bc14-c1e70cfc3320",
                                    "scriptId": "533e1ce1-13d3-439f-8544-dbdf808d30c4",
                                    "entryPointId": "52e680dc-2031-4f97-8a19-dcdaa690a1d9",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "1e76348b-2b8d-4fc0-9deb-611ecc65ba2c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The only magic here is the artist's ability to capture a person and a mood with paint and a brush.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
