                        {
                            "type": "options.text",
                            "id": "a4e2dc2e-05cd-44bb-927a-72cc632ee47a",
                            "text": "Tell About Joel and the Wraith",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "4a97704c-524e-419b-9a37-00b3b858918f",
                                    "scope": "actor",
                                    "scopeId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "WraithTalk",
