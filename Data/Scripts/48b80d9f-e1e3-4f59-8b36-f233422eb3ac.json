                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "My day away from school was most profitable, thank you. I trust your day off was equally valuable?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
