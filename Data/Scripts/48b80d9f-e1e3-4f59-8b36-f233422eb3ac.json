                                                        {
                                                            "type": "options.text",
                                                            "id": "60b37e40-56f9-4855-99f6-4096dec794e8",
                                                            "text": "Say Something Snarky to Mock Sosi",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.changeTags",
                                                                    "id": "48eb5622-7b6a-4fd9-ba56-9e3e5166bdb5",
                                                                    "scope": "target",
                                                                    "scopeId": null,
