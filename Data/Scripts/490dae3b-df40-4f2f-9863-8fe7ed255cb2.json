            "nodes": [
                {
                    "type": "nodes.showCloseUp",
                    "imageName": "Catacombs_plaque_magicuser_1.png"
                },
                {
                    "type": "nodes.speak",
                    "id": "3b36bfc8-cb4e-4d60-b02a-51d6cbb9dda3",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "actorId2": null,
                    "text": "The plaque reads...",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.speak",
                    "id": "59ef6aeb-cd21-40a8-99c8-113fc90f0d73",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "actorId2": null,
                    "text": "\"There once was a teacher named Ray,\nWhose cooking was terribly fey.\"",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.speak",
                    "id": "b8356143-2a4a-4356-a866-c41ca7b09ef9",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "actorId2": null,
                    "text": "\"The food always was scorched.\nNo one passed the first course.\"",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.speak",
                    "id": "d6b85635-ebc2-4cbe-825d-cf185c96f1eb",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "actorId2": null,
                    "text": "\"Even the water was all burned away.\"",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.onceOnly",
                    "id": "59666035-a8f4-4857-b90d-8f0e083e73fb",
                    "nodes": [
                        {
                            "type": "nodes.speak",
                            "id": "07fa5f17-1665-4f3d-957e-11fc87c27b8c",
                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                            "actorId2": null,
                            "text": "I'm glad he isn't the cooking instructor for Hero-U.",
                            "emotion": "1",
                            "emotion2": "0",
                            "device": "0",
                            "device2": "0"
                        }
                    ]
                },
                {
                    "type": "nodes.hideCloseUp",
                    "waitForSeconds": 1
                },
                {
                    "type": "nodes.playSoundEffect",
                    "soundEffectName": "magic_twinkle_loop.wav",
                    "wait": false
                },
                {
                    "type": "nodes.turnActor",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "propId": "15532050-fe54-4ead-abd6-43b2aabc60c4",
                    "wait": true,
                    "scope": "prop",
                    "targetActorId": null
                },
                {
                    "type": "nodes.placeActor",
                    "actorId": "bb406688-04e4-4c93-aaaf-52084b6f975e",
                    "spawnId": "15532050-fe54-4ead-abd6-43b2aabc60c4",
                    "pose": null,
                    "behaviour": "Ghost",
                    "immediate": null
                },
                {
                    "type": "nodes.onceOnly",
                    "id": "87f1da84-17dd-4cc9-89f7-fb4a0e19b957",
                    "nodes": [
                        {
                            "type": "nodes.playAnimation",
                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                            "animationName": "JumpScare",
                            "wait": false
