                        {
                            "type": "options.text",
                            "id": "18c89f27-3ef5-4c16-b770-0060e2406ba3",
                            "text": "Look at the Bed",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7fa32775-2b96-4ab4-8f7e-d70314657ef0",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Much plainer than fancy-pants Sosi's bed. Very neatly made with hardly a wrinkle.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "prop",
                                                "scopeId": "9f375b7c-1e93-4cf8-aa20-1906ea25f529",
                                                "scopeFilterId": "a64f567d-8023-48af-a8e2-5d876e34cb90",
                                                "tags": "itchybed",
                                                "has": true
