    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "d9bc9a8b-46b9-4b2d-a8ad-5546a0d31027",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.enteredScene",
