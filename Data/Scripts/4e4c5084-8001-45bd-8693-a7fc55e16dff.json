{
    "type": "script",
    "id": "4e4c5084-8001-45bd-8693-a7fc55e16dff",
    "name": "Dining Hall Ifetaya 03: Work Offer Again",
    "sceneId": "bf8b9872-cbdd-47c6-997c-1cd3ad91bc1e",
    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "42a2f75d-8e37-45c4-ac07-f60a9526a5f5",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.speak",
                    "id": "fbb76642-d978-418a-972b-cb73f6086db8",
                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "Ek'abo, Mr. Shawn-Shawn. Do you come seeking work? There is plenty work for you here.",
                    "emotion": "1",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.showMenu",
                    "id": "a194f349-5566-4c9e-b514-4a1f3b22b2a0",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "42feba6e-58df-4778-b3b9-cb79eadb4f12",
                            "text": "I Came to Work",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "e8904e31-507d-492b-b08b-f7618783aa01",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "I am here to work for you.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "39a3f8e3-479b-47a1-9956-f06aa333fe1e",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "22b65524-8e65-4663-838e-a5da7f366c24",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Hard work rewards itself - maybe not with money, but with satisfaction. Good for you.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "2cddb909-2169-4c05-af2c-2310f9eef3fa",
                                    "scriptId": "6d1f461f-865a-4ee5-9965-0f2b2076b3e1",
                                    "entryPointId": "",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "bf8b9872-cbdd-47c6-997c-1cd3ad91bc1e",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "scene",
                                "scopeId": "bf8b9872-cbdd-47c6-997c-1cd3ad91bc1e",
                                "scopeFilterId": null,
                                "tags": "WorkedToday",
                                "has": false
                            },
                            "ignoreChildAvailability": false,
                            "minutes": "1",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "3222af09-0409-4860-b213-cc825ee1fcaf",
                            "text": "I Came to Chat",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "42afe213-8475-44c7-b20f-b6c30b171927",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "Actually, I just came by to chat a bit with you and Riki.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "e7c54166-778f-47c9-9f79-1065a5716341",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "2673176c-6017-450f-85be-074cf65f1640",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Well, then, let us talk.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "042702b5-cdce-4695-9698-a93e32d5678d",
                            "text": "I Came to Do Both",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "3d72ec68-5685-464f-8e87-35ee5f97c505",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "I just came here to chat and then to go to work for you.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "344ef1b5-67b6-4911-9a4f-544360740159",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "amount": "2",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "419d2a75-9746-4612-87bb-2c4316fc9cf4",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Very good. Let us speak.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeTags",
                                    "id": "646f9b8c-28ee-471a-9002-b9959a9581d5",
                                    "scope": "actor",
                                    "scopeId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "ReadytoWork",
                                    "tagsToRemove": null
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "scene",
                                "scopeId": "bf8b9872-cbdd-47c6-997c-1cd3ad91bc1e",
                                "scopeFilterId": null,
                                "tags": "WorkedToday",
                                "has": false
                            },
                            "ignoreChildAvailability": false,
                            "minutes": 0,
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": true
                },
                {
                    "type": "nodes.showMenu",
                    "id": "8639fb0e-60b7-4abd-8ee3-85559e753315",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "6d376783-7690-429a-9c6c-0b28cdb674c7",
                            "text": "Ask About Ifetaya",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "eed4b3ad-ce20-4424-aa1b-ba2b7d320dd5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "So where did you come from before you came here?",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "5cc1476e-6291-4220-b91f-793044fc157b",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "amount": "2",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "ce6a9409-66bc-48c7-996f-fee92022ef8c",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I have done much traveling to different lands, learning the ways of people and the food. Just before coming here, I taught myself the cuisine of southern Bellefrance. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0341bd78-d9e5-41e0-8751-131f18368b17",
                                    "actorId": "4cc48e56-7f61-4887-ad13-4bf879f29b21",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Belle people be silly. They call Riki pet! Don't be letting Riki learn.",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "f6a60399-efe1-485a-ba19-e0e4480c5a19",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Riki did not like that the Chefs of Bellefrance forbade Riki to be in the kitchens. I think the Chefs, they did not like it that Riki knows more about cooking then they do.",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4b36855c-6a7e-47f2-8ae8-29fe0c280bee",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "4cc48e56-7f61-4887-ad13-4bf879f29b21",
                                    "text": "Next time we travel, Riki, do not tell the chefs better ways to do what they do. Listen to learn. Teach only those who be wanting to be taught.",
                                    "emotion": "1",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "8",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "c9654d16-d1af-41b0-bb6f-18d7ad5fa6d4",
                            "text": "Ask About Riki",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4c0f8e36-66cd-46b7-b366-1a4087078b88",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4cc48e56-7f61-4887-ad13-4bf879f29b21",
                                    "text": "Riki, do you like to cook, too?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "cbc565f2-da64-4205-8cf5-49bb1ab1aa77",
                                    "actorId": "4cc48e56-7f61-4887-ad13-4bf879f29b21",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Cooking too dirty. Fur, she gets dirty. Riki like teaching. Riki, she make others get dirty.",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "1f7e5a5d-d75a-4918-a575-347d32bca216",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Riki supervises the students in the kitchen. She is good at telling people what to do.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "b103da16-69ac-42a9-9c63-cbc280f87253",
                            "text": "Ask About Rumors",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b6acb004-9848-4321-87f3-2ec878db5ca1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "Hear any interesting rumors going on at the school?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "68105811-10d3-46c1-8054-8e7982bcf704",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "He who gossips to you, will gossip about you. Gossips, they are no one's friend, so I do not listen to such tales. Nor do I speak them.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "47a54b2b-70f5-4970-b3c3-5988f68c82a1",
                            "text": "Go to Work",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4b18bc05-19b2-41e1-a59c-83c8723f1c2d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "I'd like to go to work now.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "6c029e9f-510e-485e-b5a7-aac641938767",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Good. With work, the sooner started, the sooner finished.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "0e8f02f1-4a59-496c-8b6b-58588e3244c2",
                                    "scriptId": "6d1f461f-865a-4ee5-9965-0f2b2076b3e1",
                                    "entryPointId": "",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "bf8b9872-cbdd-47c6-997c-1cd3ad91bc1e",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                "scopeFilterId": null,
                                "tags": "ReadytoWork",
                                "has": true
                            },
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "1f98c52c-d59a-4feb-a004-9082a6911af3",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "8bfcb6e5-ef66-4873-8239-b27dada44d04",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "Goodbye for now.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "541d82be-8abc-4b34-8926-461f187161d9",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Mari e ni'gba mi, Mister Shawn-Shawn - I will see you later.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "447e39d9-7a68-4272-9a24-911462282bc8",
                                    "actorId": "4cc48e56-7f61-4887-ad13-4bf879f29b21",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Bye now.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.endScript"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "3",
                            "ExitMenu": true
                        }
                    ],
