                                {
                                    "type": "nodes.speak",
                                    "id": "ac64d3ab-928c-4691-890e-5c3f4d714284",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I can barely read the inscription at the base. It looks like a name... Garrett Thunderberg. I have no idea who that was.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
