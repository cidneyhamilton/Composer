                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "But I'm not superstitious. I'll only cower a <i>little</i> bit.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
