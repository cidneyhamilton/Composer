                        },
                        {
                            "type": "options.text",
                            "id": "6a2e7be9-2580-425a-a733-9dc0e2ec47e3",
                            "text": "Flirt With Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4ec295b8-ce82-46c3-8b34-1ce4abcd44c7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Would you like me to be your hero?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4c4dd7ce-1631-4e56-8a1a-7812952f6cca",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I am not damsel in distress. I vill be my own hero.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "722d4531-aabe-4060-8613-bdcc004226c4",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
