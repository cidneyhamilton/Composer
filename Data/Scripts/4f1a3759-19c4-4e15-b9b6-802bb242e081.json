    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "6ab040c6-d74a-4719-8c1b-7b20dd63ad3e",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "bf9051e6-6af9-42db-a08e-2774dcd2317e",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "e53ccded-de67-4657-8ea9-839820c9fa16",
                            "text": "Ask Esme About Heroes",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "6e309945-954c-4cf0-b5ae-6685eff6a2df",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "What do you think, Esme? Are you a hero?",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "528e9533-c7c1-464c-8be8-41f30ee1160c",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "No. I have not done good heroic thing. But maybe I vill do good thing one day. Maybe I vill be hero.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "7f61abbe-5c29-4661-8407-b9509f31bbac",
                            "text": "Talk About Heroes",
                            "nodes": [
                                {
                                    "type": "nodes.showMenu",
                                    "id": "c66e94a1-fa06-46c6-9495-e8cf5a9742f0",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "e82a053f-3db8-42b6-a5e8-5fcfdce246bb",
                                            "text": "Say Something Charming About Esme and Katie",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cfc4b089-53d2-4826-acfe-429d439203ac",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "I think we all can and will be heroes someday. We just haven't needed to be heroes. When the time comes, we'll all rise to the cause. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "484e5570-6f17-439f-9bc7-023467b7324b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "You'll be a great hero, Esme.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f597f51c-d826-4655-9c7e-053cd439249a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "text": "And Katie, even you will be a hero someday. Just wait and see.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "52f85a9d-2a6d-4f0c-b717-5a0f95f30747",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "52",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "4fe960c7-a4c4-46f2-b47b-b2e00b5596f3",
                                                                "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "53d98dc3-8e74-4535-a301-327047c95b6c",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "amount": "1",
                                                    "change": "1",
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "4",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "65040f4a-311a-4650-9309-41a0151a5435",
                                            "text": "Say Something Clever About Heroes",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a25ab3bf-629b-474b-978e-491cbdf78fe3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "Just because Rogues look after their own interests doesn't mean we can't be heroes. Even Paladins do things for their own benefit... like saving the world. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1c320e3e-78f9-41d1-9e65-57bb94e9acef",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "If the time comes to save the world, I bet that each and everyone of us Disbarred Bards will do whatever it takes to save it.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "43391f8e-22f5-4c2d-87f3-4964b4826d5c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "Except Sosi, of course. He'll be too busy saving his own skin.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "55dbfbc3-05e8-4cdb-b92a-185b6eccc813",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "52",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "8810a9f2-bc5b-4583-962a-1e533591b742",
                                                                "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "4a1a88c2-2cd8-4f39-b6a4-3f079d7ee618",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "amount": "1",
                                                    "change": "1",
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "5",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "6358c65e-78e8-4aa9-ab71-5b9100bf697a",
                                            "text": "Say Something Cynical About Heroes",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a1beeb67-6223-422c-9d7d-c29624753e72",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "Everyone does things for their own selfish reasons. Heroes are just better at pretending that what they do is for someone else's good.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b61f7259-1cba-4b6d-aec7-4ee3c2a59c18",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "text": "So of course, Rogues can be heroes. Who else is better at disguising their true motives?",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "5e8f2732-1047-4f58-b2ea-f8284d2fed70",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "52",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "591278fd-ea80-4685-9468-0a1fa380d689",
                                                                "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "1e63be45-c26f-4bc6-a246-046bfbd264c6",
                                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "amount": "1",
