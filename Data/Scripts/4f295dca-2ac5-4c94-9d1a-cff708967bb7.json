                        },
                        {
                            "type": "options.text",
                            "id": "6945001b-7fc7-41e7-aca3-381859438548",
                            "text": "Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "ff155f6b-84ee-4187-bf6d-f8f9aadad06b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "text": "Thank you, sir. It's always interesting to talk to you.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "c83321f4-31e0-4426-a244-dbb84c069d44",
                                    "scriptId": "8f2ac6a0-3073-44b0-b814-fdfcbc24ab9d",
                                    "entryPointId": "6cac2b88-9657-4bd2-aa9d-8314949d75cd",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "72ba3e9e-6ef4-450c-8012-d371228f1bcd",
                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Take care, Herr O'Conner. Do not let the dangers catch you unwary or unarmed.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
