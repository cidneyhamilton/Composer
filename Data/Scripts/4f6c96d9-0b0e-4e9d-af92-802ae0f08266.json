                                {
                                    "type": "nodes.speak",
                                    "id": "7fd77fa6-7359-4766-9af7-3c15c0119f5f",
                                    "actorId": "6367aa71-9355-4199-b390-d30a6ef23c02",
                                    "actorId2": null,
                                    "text": "Purrrrr",
                                    "emotion": "1"
