    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "b843d5d7-1d93-42ed-8277-390934d5b98b",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "e67937b6-f1f5-41dc-b2d7-0ec9d1ea00fc",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "41d4a6d0-afae-46e6-ab6b-1ce07e88378f",
                            "text": "Greet the Librarian",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b36f9912-1581-475a-96c4-f6e57625f503",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "Hi there, how are you today?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "94f9ac37-c81c-42e7-b611-65461353d9ae",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I am the same as always.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e66db9df-6a20-4154-bb47-b149ffd09802",
                            "text": "Get to Know the Librarian",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "752f07ff-e218-4222-87d1-a66be762d8be",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "So what do you think about working in a castle? Pretty cool, huh?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "094b78c4-5b70-4e06-b144-2d9a79c58e04",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "By the literal interpretation of your question, it is about 20 degrees cooler inside the castle than the ambient temperature outside of the main structure.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "3f30acb3-f084-47f4-9bc3-208b6f05a638",
                            "text": "Ask About the Library",
                            "nodes": [
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "777467e4-797f-4e97-9433-8aa5fea0d20e",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "9c824f13-caec-43d4-9278-11d449e5218e",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "How many books are there in this library? Must be hundreds of them.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "49f43268-4290-4b03-869d-611ff43a965c",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The library currently contains 6375 unique reference books.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "45155ee8-5715-4730-aaf4-4b9a022a3352",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "Sounds like it contains every book ever written.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "bfa6712e-035a-487c-9c0e-33b500a56846",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "That is a gross underestimate of the magnitude of literature. This library specializes in books of historical facts and philosophies, with a scattering of educational manuals added by the University staff.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "62294350-7d48-4b53-8f16-d6fa68b3c62a",
                            "text": "Investigate the Librarian",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c1a91802-ebca-4caa-93dc-f00d5b4ab1d8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "I hear that you were the librarian here even before there was a Hero University at this castle. So, is this castle your home or something? Who ran this place before the University?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "03d0f9b0-e8f2-4c73-bf67-91bfd2df5e59",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "My personal history is not a topic of conversation. If you desire to know the history of this castle, you may consult the history book upon the middle podium.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": {
                                "type": "expressions.hasActiveQuest",
                                "propId": "3cc7cb21-110a-49b5-8bfe-bab34a5c41d4",
                                "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                "questStatus": "6"
                            },
                            "ignoreChildAvailability": false,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "d152d58c-812f-4b03-b0a0-d8b819dfe3a7",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "fb5721fd-62d8-4183-a665-6db9c186ff77",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "Nice talking with you.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d97dd561-e26f-4f30-afa4-5d2442704c2a",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Indeed?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": 0,
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
}