                                                                            "id": "413b57a2-b937-45e0-ad0f-04b92e888c96",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "Think of all the ashtrays and candleholders I could <i>sell</i>.",
                                                                            "emotion": "1",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
                                                                        },
