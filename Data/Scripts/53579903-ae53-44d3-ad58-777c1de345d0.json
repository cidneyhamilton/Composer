    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "1f0b6cdd-735a-47fe-a616-fc1e5b52717d",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "0939d379-33f5-4ec9-be6c-be20d1ce2088",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "d2412bb3-5c2b-4be0-90d0-821f05f63521",
                            "text": "Examine the Wardrobe",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "5b7e3eb3-f376-42d5-ba0e-fc0aff26ade8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "This is a really solid wooden wardrobe and it has my name on the plaque. It's clearly designed to store all the expensive treasure and gold I gather at this school.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3f95ab93-b184-4faa-8202-e13d5eda97c2",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Assuming, of course, that I can gather any expensive treasure and gold around here. If I stole something, this would be the first place Terk would look for it.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "72ed6470-35d0-4358-8833-30255fce2532",
                            "text": "Open the Wardrobe",
