                                                    "id": "b14ce88a-d64c-4c61-b60c-bb1d14a065de",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It looks like a trap... A giant-sized mouse trap. For giant-sized mice, I guess. Guess they must have had rodent problems back in the day.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
