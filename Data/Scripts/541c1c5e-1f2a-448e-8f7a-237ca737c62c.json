                                                    "id": "d0790029-b795-499f-8de8-29cbda14b76a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I'll just take this dusty thing away for safe keeping - for keeping me safe from unwanted rodent attacks.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
