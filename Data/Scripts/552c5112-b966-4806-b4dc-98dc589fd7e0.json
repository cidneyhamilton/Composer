                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b230d14a-c6c6-4259-96d8-53118783b073",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "text": "Good night and good luck!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0a5800ce-278b-4dbd-ad92-2e90e957e963",
                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'll wish you luck, too. Just not quite as much good luck as I get.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
