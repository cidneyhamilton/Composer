    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "652943e4-8650-4164-bf20-c08d51d7615e",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.enteredScene",
                                "sceneId": "ea6c0ba0-0d23-4c0d-80ed-b8611588a665",
                                "scope": "DuringCurrentEvent",
