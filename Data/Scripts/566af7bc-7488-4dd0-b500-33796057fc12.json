                },
                {
                    "type": "nodes.addJournalEntry",
                    "id": "e7f8f4b8-9e59-4659-af74-1636b618f36b",
                    "name": "Science Madness",
                    "notify": true,
                    "description": "Is Dr. von Braun a Mad Scientist? No, he's too jolly for that. I think it's more likely that 'you don't have to be crazy to work here, but it helps.'"
                }
            ]
        }
    ]
}