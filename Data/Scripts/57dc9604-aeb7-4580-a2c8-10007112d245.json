        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "7bc0e5f3-8bdc-4a55-aba9-de7bd054a855",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "90c762fe-6d23-4bc3-ba66-750e9ad30f5f",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "70276978-596b-4216-bc0d-5fc8fbc17f43",
                            "text": "Look at the Mast",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "db2daee8-9c40-4bf1-ad7a-09104e108590",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Someone had the bright idea of hanging a lantern from the mast. It's sure coming in handy, since there's not much light in here.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "66ca736c-8552-447b-967e-32520ed351f2",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I wonder who that mast man was, anyway? I wanted to thank him!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "36c0c2f7-ac57-4618-ba46-fa20cece87fc",
                            "text": "Climb the Mast",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "abfc4c40-e057-444f-8bb9-a881a000a802",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'm not being paid to be a rigging monkey, I'm being paid to be a student.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "81154432-74a7-4c87-b265-b1e9d7fe2016",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "And I'm not even being paid for <i>that.</i>",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "71a9ca44-f823-4f48-9d86-d05144ecda2d",
                            "text": "Carve your Name in the Mast",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "prop",
                                                "scopeId": "de895c54-7058-4e18-8219-70d385f4aa54",
                                                "scopeFilterId": "91e73978-0245-4bb2-8c83-eaa7f3954455",
                                                "tags": "CarvedName",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a8373358-8819-4109-b87a-a811693a7647",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Now it says, 'Shawn was here!'",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "36548304-7ad7-46fc-bc92-b586e979c01d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "If I carve my name again, it'll say 'Shawn was here! Shawn was here!'",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "df53b541-0b96-4625-b7ac-b4b55a91ad6a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "But there's only one Shawn here. So that would be inaccurate.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
