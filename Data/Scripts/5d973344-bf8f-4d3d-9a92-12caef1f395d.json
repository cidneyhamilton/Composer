                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b3efb21e-1274-4d39-9e6f-2ac38cfac089",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Velyana. I'd recognize her anywhere. Especially with her name inscribed under her.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "ffd8e723-cf14-4825-bc18-c7afb400c605",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I don't know how true to her likeness this relief is, but she looks like someone who could see into the depths of the Darkness and not flinch.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
