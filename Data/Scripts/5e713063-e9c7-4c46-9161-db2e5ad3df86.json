                                            "type": "nodes.changeInventory",
                                            "id": "7fe04f33-6a16-42c1-a5c2-e0f97ad60d21",
                                            "propId": "646cea66-c6c6-4dba-b093-6ec5b7e987a5",
                                            "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4",
                                            "change": "1",
                                            "count": 1,
                                            "notify": true
                                        }
