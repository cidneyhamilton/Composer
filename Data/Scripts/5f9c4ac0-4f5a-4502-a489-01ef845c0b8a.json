                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "75a41267-169a-48af-8982-84f3a6f6da49",
                                            "text": "Look at the Bookshelf",
                                            "nodes": [
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "scene",
                                                                "scopeId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                                                "scopeFilterId": null,
                                                                "tags": "GerhardPresent",
                                                                "has": true
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "9764c333-0298-4182-9349-1fb788a947c6",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "text": "You know, if I tossed this hourglass to you, we could see how time flies while we're having fun.",
                                                                    "emotion": "1",
                                                                    "emotion2": "4",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "d1d5824b-30b9-477a-a77f-45bc12beacf0",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "That would truly be a waste of time.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "a1635ef9-9a2d-444b-b51d-bf199f862e07",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Two bookshelves in one room? I always knew that the Master was shelfish.",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": 0,
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "70c4b303-b76b-49c3-abb1-2058470103ce",
                                            "text": "Take a Book",
                                            "nodes": [
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "scene",
                                                                "scopeId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                                                "scopeFilterId": null,
                                                                "tags": "GerhardPresent",
                                                                "has": true
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "d34ab205-46e9-425e-b6d7-a2ca142945c0",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "You are not, I hope, imagining that I might not immediately notice the abrupt absence of one of my books. That you would even consider removing one speaks volumes about you.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "074fb190-8dc9-4d33-ab66-414882224b85",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "text": "To the contrary. I wouldn't want you to consider me spineless.",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "c46dde28-ff03-4e85-a7be-cc0cc77c464b",
                                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                    "actorId2": null,
                                                                    "text": "Indeed. It remains to be seen whether you are bound for glory or for infamy.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "78874bcc-3d4e-4b22-8773-777e469fa70d",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "If I were to get caught... and there's little doubt that I <i>would</i> get caught... I'm sure the Master would throw the book at me.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": 0,
                                            "ExitMenu": false
                                        }
                                    ],
