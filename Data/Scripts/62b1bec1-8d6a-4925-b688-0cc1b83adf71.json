    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "210b695e-e9ce-4026-b259-7bae71ca3e8e",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "1bc84ea9-d7f5-4b3b-b865-5d8d0df9b067",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "b2470f6f-c778-4237-90be-2c25ce66c519",
                            "text": "Look at the Shelves",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0d1474fd-c4b1-42b7-a156-37f53644795e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "That hardly deserves to be called a bookshelf anymore. Mostly, I see a lot of firewood.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f2cc9547-5994-4f75-aa83-fe34c0385d3e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's a set of shelves, but I can't even begin to see what's on it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "ed394fb3-794e-42c1-b4cb-d8691b85f5fe",
                            "text": "Search the Shelves",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7b547538-9973-44df-8df9-7d506c80879b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Let's see. There are a couple of books: <i>The Primo Strategy Guide to Mumblety Peg</i> and <i>Declutter Your Kitchen One Firkin At a Time</i>. I can see why nobody's missed these.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bea873bc-7e23-4143-8b7b-f64b62b435c3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "There's a couple of small lockboxes here, but there's nothing in them but dead bugs. Lots and lots of dried, shriveled, dead bugs. Maybe there was something tasty in these boxes at one time.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9511466e-4a02-4669-a1d2-ea2b7237076c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Most of it looks like junk. Candle holders, an old pestle, a death mask... some of these are so dusty, I can't tell <i>what</i> they are. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "target",
                                                                "scopeId": null,
                                                                "scopeFilterId": null,
                                                                "tags": "BookTook",
                                                                "has": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.comment",
                                                                    "message": "Only one copy of the Paladin book.",
                                                                    "commentor": "Cidney"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "155f4444-f0e4-47ae-8483-81b909143be9",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Oh, there's a really old book left here - <i>Way of the Paladin.</i> Looks like this Paladin book lost its way.",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.changeTags",
                                                                    "id": "f1dfb86b-6017-403e-b23c-0fdfbf7bfe4e",
                                                                    "scope": "target",
                                                                    "scopeId": null,
                                                                    "scopeFilterId": null,
                                                                    "tagsToAdd": "FoundBook",
                                                                    "tagsToRemove": null
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "371e59fd-f2c3-4732-9630-c2eee3d83bd1",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can't make out what any of this stuff is. It's too dark over here.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "f3af41c9-9502-4e34-a316-b77effb62dbc",
                            "text": "Take the Paladin Book",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "dffcdd89-f097-4785-94ff-0add0a017e04",
                                    "scope": "target",
