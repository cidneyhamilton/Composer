{
    "type": "script",
    "id": "62e5bc45-0f69-4369-bb48-0893add973e3",
    "name": "Dining Hall Conversation Day 16: After Thief",
    "storyEventId": "78421f39-bc8e-4f07-bd53-f6e3c31eadb9",
