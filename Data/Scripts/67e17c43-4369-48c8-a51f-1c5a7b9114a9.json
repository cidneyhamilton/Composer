    "name": "Sosi RR02_SosiStruckOut",
    "storyEventId": "e62d490b-a8cc-4450-af80-21e90bbfc903",
    "sceneId": "84006d08-8e6d-4558-b9b5-101f3c3054fe",
    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "e8c3da90-c2ef-44ab-aec0-86b897991a86",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "e5b86d4e-9595-455a-8b72-b794befd59a1",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "deca9467-202e-4f22-99c7-d35088c2618c",
                            "text": "Ask About Sosi",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                "scopeFilterId": null,
                                                "tags": "ShawnTool",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d7107450-80bf-44ff-a42f-268617fb4870",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Good evening, sir. How are things going for you tonight?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "233362e7-2103-4969-9460-447f3ca30e03",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I don't care to talk right now.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9984396e-6977-4f04-b7ef-3d1074780a06",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "I'm sorry to hear that, sir. I so enjoying talking with you. I find it... exhilarating.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d3f77715-8f7b-4474-85ff-2e2cdbb1d997",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "What's this, Caesari? I'm surprised to find you all alone. I thought you were meeting with Esme this evening... ",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9c669153-feba-448c-8faa-9667993ae33b",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "This is none of your business, O'Conner. Leave here!",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f2d217fa-6883-40f8-aead-2b94090f88de",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Hmmmm... I thought I made it clear that I don't follow orders... particularly not yours, Caesari.",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e4569e98-3f91-4e1a-b03d-3eabe9a417c1",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You are playing with fire, O'Conner. You will be burnt.",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false,
                            "notes": ""
                        },
                        {
                            "type": "options.text",
                            "id": "c2de02b0-5cff-45a8-bfd8-e25e2313648c",
                            "text": "Talk About Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "fcce92b0-5b5e-4ef4-8fb9-9414f9ac8ba8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "text": "Aren't you meeting Esme here later this evening? She mentioned something about that at supper.",
                                    "emotion": "1",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0",
                                    "notes": ""
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                "scopeFilterId": null,
                                                "tags": "ShawnTool",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9ebbeab3-bc60-4951-9f51-c9d37e2e00e7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Esme was really excited about seeing you. You clearly have a way with the ladies, sir.",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1af80c0b-fed0-44d6-adab-654d6940d217",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Get out of here!",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d491e026-8636-4bc5-a413-41f285a66f71",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Oh, of course, sir. I wouldn't want to spoil your evening with Esme!",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "670d735c-a6a7-48a5-bc6c-b7c5aa2ad49c",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "amount": "2",
                                                    "change": "0",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "af66e76d-4fb0-43f7-9d48-069c74788cb0",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Then again, I just saw Esme in the Courtyard looking like she wanted to strangle someone. Guess she must be thinking of you.",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6af205ba-2fa9-4e61-9ea1-2935e0b125a7",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "This doesn't concern you, O'Conner.",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "da6b65e2-460d-4c1f-b7c3-3529c07bb5f6",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "What, it doesn't concern me that you hurt Esme's feelings tonight by being a jerk - is that it, Sosi? Guess again. Don't try this crap around here again.",
                                                    "emotion": "3",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d458808b-0780-46d7-879a-43b116fe36d8",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "What are you going to do about it, Conner?",
                                                    "emotion": "3",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c3d1d56a-163d-4a89-908f-64c2d3771f2f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Let's see... my roommate is a bard. He likes to make up songs all the time. I can just hear him now singing a song about you...",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5f99e9aa-7b45-4072-aad5-61ece58c7686",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "<i>O come all ye maidens and list to me prate,\nAbout Caesari Sosi and his sorrowful fate...\nHow he tried to kiss an unwilling miss\nAnd now he's unable to go on a date.</i>",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bd6ff454-4422-4ae4-a2bf-f76f09f92498",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "I'm sure everyone will be singing that song around here... If you ever try it again.",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.incrementTime",
                                                    "id": "f3daca3a-97e9-4f65-95ad-ca23af589790",
                                                    "days": 0,
                                                    "hours": 0,
                                                    "minutes": "7"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false,
                            "notes": ""
                        },
                        {
                            "type": "options.text",
                            "id": "0b251869-729e-47b3-8846-724b253173c4",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                "scopeFilterId": null,
                                                "tags": "ShawnTool",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8e72142c-4bb8-4690-900d-8f3d3287016b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Good night, sir, and have fun on your date with Esme!",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a11d36c8-4c2d-4bf2-b833-7bddee19d55f",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "...",
                                                    "emotion": "3",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f7ae842f-d450-462a-8e4a-d14c7515a76f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "I'll be watching you Sosi - whatever you do.",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6d64a451-d232-437b-b3d7-7019db8ebace",
                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You will pay for your impudence!",
                                                    "emotion": "3",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8a598e52-370d-4f3c-878f-02344ea45b6e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "Ta-ta for now!",
                                                    "emotion": "1",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0",
                                                    "notes": ""
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.invokeScript",
