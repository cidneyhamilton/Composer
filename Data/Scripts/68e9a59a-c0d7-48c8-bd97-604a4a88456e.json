                    "commentor": "Cidney"
                },
                {
                    "type": "nodes.showMenu",
                    "id": "dd1f578f-d24d-4799-a096-c53be2a4ec6c",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "dea15155-3f67-4e39-905b-ef00292ca943",
                            "text": "Challenge Sosi to Billiards",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "94e13dc0-1843-41e7-a843-256cec8728ad",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "text": "Sure... wanna play a game of Billiards with me?",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.comment",
