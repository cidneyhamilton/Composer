    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "e1ec21de-69b4-4dcb-996e-b571816d694d",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "ae1fd713-abd4-4ff5-9681-103de2f42f04",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "16966036-35ef-41b0-a43c-da89188269e2",
                            "text": "Ask About Sosi",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "0296052a-5bde-4cd0-a653-cdee4007947c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "text": "How is it that a man of your rank and stature gets stuck here at this school with the likes of me? Isn't this a bit below you?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0aad72a3-1ddb-4622-8158-d3a028fdb4f8",
                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Yes, being stuck at this school is an insult to a person of nobility, but we endure what we must. You will never know the machinations behind why I am here, so do not ask.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "a21c3df2-0dc2-4688-908b-d837a1adce27",
                            "text": "Ask About Games",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "ea1e52f0-4472-4d27-8898-e168344afac5",
                                    "scope": "actor",
                                    "scopeId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "Games",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "2b59bad7-2c12-4d00-b99e-58fc0ecd42e6",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "text": "What games do you like to play in the Rec Room?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "de7cefca-b540-4ef1-8cd3-31bfcceb31ca",
                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I am very good at everything I play... Would you like to see how a master at billiards plays? Try challenging me to the sport.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.comment",
                                    "message": "Moved options to invoke script; player should use \"Challenge to Game\" option from main menu.",
                                    "commentor": "Cidney"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "48f56940-520b-440d-a4e1-e48277e1f5f6",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "f5924ed5-355b-479d-9577-f296cbb8e351",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "text": "I'll see you tomorrow.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "f6821486-3657-4731-b4ec-a9f528344ab2",
                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Perhaps so. But I can always hope for the best.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        },
        {
            "type": "scripts.entryPoint",
