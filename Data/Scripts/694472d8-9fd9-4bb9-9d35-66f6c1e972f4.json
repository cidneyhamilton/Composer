                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "As long as you're just talking about cards or darts, then maybe we will.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
