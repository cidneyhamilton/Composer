                {
                    "type": "nodes.showMenu",
                    "id": "1262b2f4-63df-4e1d-911e-1c63af304711",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "ea008757-8c6e-410f-9b72-b01f8d4d7562",
                            "text": "Look at the Chair",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "52325fe3-e97b-470c-bfad-48d3dc280f74",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "That's a pretty grand-looking chair.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "70eb88fd-8a48-4374-ba6d-bc62bcaa617f",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I wonder if sitting is more comfortable than floating.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "f1cb6eb9-f0f9-4e5c-b181-f6364deb91ea",
                            "text": "Buy the Chair",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "bought",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0c6b0599-78b0-4b50-88f3-d9967fb7c90e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "Is the chair for sale? I'm not making a serious offer, I'm just interested.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "000c702a-cb17-4371-b342-50ca13a14020",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "No.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8d5eb0f7-05eb-4c8b-b81f-cd3bbeb6cff5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "Any special reason? I mean, ghosts can't actually sit, can they?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "222f2852-3ff4-4e0b-988e-5d571536c832",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I don't think so, but I'm trying ta learn.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ac0cef47-d34b-4f82-a940-7d08026adec9",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Everyone needs a hobby. It's not like I have all that much ta do here.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "891edbde-764a-4fe7-89f6-ccb0b134ca01",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "bought",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "478cb037-9377-4e6a-bfcd-367d9459e44b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "You're firm on not selling that chair?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e865117e-3fca-43d3-a102-3ab71e95cc52",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "I'm not firm on <i>anything!</i>",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "95c33f81-03d6-42cd-abc7-dc648196e966",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "But I'm still not gonna sell it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
