            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "50bf07f8-5ef7-4dd9-93da-bd754a6d52fa",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "b629c067-85db-4551-a29f-16589c91500a",
                            "text": "Look at the Couch",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "9334a5c1-d593-4cb2-b47c-846811ad6ffb",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Looks nice and comfy. I suppose people meet here in little chat groups, or to hold informal forums. That must be why it's so free of webs.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "ba38d29c-d9c2-4193-b8fa-1be974db7426",
                            "text": "Search the Cushions",
                            "nodes": [
                                {
                                    "type": "nodes.playAnimation",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "animationName": "KneelDown",
                                    "wait": true
                                },
                                {
                                    "type": "nodes.playSoundEffect",
                                    "soundEffectName": "couch_search.wav",
                                    "wait": true
                                },
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "a044613c-d0d6-49bf-8312-10ec7487e8bb",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "bbb196c9-46cb-482b-b0c0-1c02033eefc0",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Ouch! Who left a hatpin in there?",
                                            "emotion": "4",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "159a29d0-9e2d-4c29-bbdf-24bd231189c2",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Not a single coin. Must be a lot of cheap people sitting here.",
                                            "emotion": "3",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "5b6f33c5-0d90-42c5-8194-71ebe7b531bc",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "It's a half a cookie. Don't mind if I do!",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.playSoundEffect",
                                                            "soundEffectName": "paper_rustle.wav",
                                                            "wait": false
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "54a2b28f-174c-4779-9167-b02eca3d873f",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "It's the last page of a book. Looks like it was ripped out. Just think... somewhere, there's a book that can never be finished... ",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "bd8da78c-6467-4a56-a499-b81fd9cf278a",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Ecch... it's half a mouse. That raises all <i>kinds</i> of questions.",
                                            "emotion": "2",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "5b39300d-10b9-4259-86f0-7a01d72119b9",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "It's a small, hard, bumpy, block with the letters L, E, G, and O. What the hell is it? ",
                                            "emotion": "4",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "d2ee6c46-a53f-427f-9f14-a49039826a7e",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I don't feel anything else in there. Ah, well! ",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "8369d458-cf47-4060-aa5e-3e6312ba79df",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "I just keep thinking about that little brick I found in there. It must've been from the \"loge\" of a theater, only I think they spelled it wrong. ",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.branch",
                                                            "sections": [
                                                                {
                                                                    "type": "nodes.branchSection",
                                                                    "expression": {
                                                                        "type": "expressions.inTags",
                                                                        "scope": "prop",
                                                                        "scopeId": "82bc5149-ca28-4c12-9292-57e8efe40146",
                                                                        "scopeFilterId": "981ec865-3e1b-4f3e-bb0d-11df2bc7d593",
                                                                        "tags": "ReadErmahgerd",
