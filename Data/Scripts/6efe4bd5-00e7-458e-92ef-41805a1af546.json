                                                                                "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                                                "amount": "15",
                                                                                "change": "1",
                                                                                "notify": true
                                                                            }
                                                                        ]
                                                                    },
                                                                    "failure": {
                                                                        "type": "nodes.block",
                                                                        "nodes": [
                                                                            {
                                                                                "type": "nodes.speak",
                                                                                "id": "5ff32a6b-7aae-4e11-b03b-cd58e6277f53",
                                                                                "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                                                "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                "text": "That folder should be right in the file cabinet. Mortimer is too fanatical to leave it anywhere else.",
                                                                                "emotion": "3",
                                                                                "emotion2": "0",
                                                                                "device": "0",
                                                                                "device2": "0"
                                                                            },
                                                                            {
                                                                                "type": "nodes.speak",
                                                                                "id": "380b52b4-4dfc-4013-95f9-28aba6d4a428",
                                                                                "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                                                "text": "Maybe Sosi stole it first to make sure his secrets stayed safe?",
                                                                                "emotion": "0",
                                                                                "emotion2": "4",
                                                                                "device": "0",
                                                                                "device2": "0"
                                                                            },
                                                                            {
                                                                                "type": "nodes.speak",
