    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "6f9f6afe-e50e-42f0-a6d2-193bfefbedea",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "e73d888c-2934-41d4-9dfd-29d86ba289c0",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "e0350f24-e176-4ad9-8cf7-71e1a360388b",
                            "text": "Ask About Gregor",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c41decab-5174-4267-83d4-56a121434722",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "I guess you don't like the Warrior Drats much, do you?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "502c56a0-a69a-4bbf-b9fc-252c26596d7f",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Warrior Drats are nasty. Warrior Drats chase Gregor when Gregor comes to store. Warrior Drat bit Gregor's tail!",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fc86c461-c646-4755-bc39-6f61097aed6e",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Warrior Drats are tough and stringy.",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e212ede3-ee44-4906-8cfe-ac6a4920dec4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "Does this mean you won't buy Warrior Drat parts?",
                                    "emotion": "0",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "2ddea9b2-cd54-4c6d-a324-a8a7acfe9de4",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Gregor wants Warrior Drats dead! Gregor will pay for all dead Warrior Drats!",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "07f3fe96-bac0-4c9a-9de8-4cfea828eba9",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Warrior Drats can always be used in stews.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "88f4d0b2-b558-4ebb-bcac-28d6e5402038",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "amount": "2",
                                    "change": "1",
                                    "notify": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "7",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "a860a863-7fb6-4aa1-a32f-44c926a3ebab",
                            "text": "Ask About Warrior Drats",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7d4c38a5-7586-430e-8982-b0ac7e1f1e35",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "What do you know about the Warrior Drat invasion?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fa69bd7c-53e5-4417-9004-fa46a05c9208",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Warrior Drats all over castle! Not in Caligari, not like this. Something made Warrior Drats angry. Many Warrior Drats are in Wine Cellar now. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3f9e2efa-0495-4029-99f6-43127ea68692",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Poor Gregor fears for his life when Warrior Drats are around. Warrior Drats think Gregor is yummy.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "0e143b9d-25cd-4531-af6b-56cc596804a2",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "60fa0d7b-b9fa-4227-a033-2139a1da9c03",
                                            "text": "Say Something Reassuring",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "22cdef1d-3906-414d-b781-e22d715bf561",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "Don't worry, Gregor. I won't let the nasty Warrior Drats eat you.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "39dbbf6f-8814-48e7-b26b-d9e058a104da",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Gregor knows Master Shawn is Gregor's hero! Master Shawn will save Gregor! Gregor is most happy!",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "07dbd4e6-a44a-4b5b-837a-9feba25bdfb0",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "id": "f7a08cf0-df10-44bf-8dfe-005aca0f67b1",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "80",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "9bfe49d8-3138-46a2-a774-a281e3f03421",
                                            "text": "Say Something Clever",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c45ee9d0-65f3-406e-ba11-1e59f752aea3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "This is a school for heroes, Gregor. Don't worry, someone will stop this threat one way or another. It's the heroic thing to do.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "da517cb3-8283-423e-8a53-a846701adb7c",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Yes, yes, Master Shawn is right! A hero will save the day for Gregor! ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c26c0d29-06f3-42c5-ad09-dc65ffd14135",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Gregor is so glad Gregor is here at the Hero University!",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "4b75c9ae-ac4c-45a5-a5e8-1d9eb4517acf",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "id": "031df7c7-3391-4434-90a7-5ac7c19f8d4e",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "80",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "4",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "ad37945d-e3d1-4561-8f5b-0d5a620cb6ce",
                                            "text": "Say Something Snarky",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "89a69dcd-001b-4c1c-b8c9-1a1cec5b8efe",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "So you were chased through the halls by a Warrior Drat... Does that mean a Drat was driving Gregor Ratty? You were in 'Dire' straits from that Drat when it bit you. It's a woeful tail.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f83b5991-33dc-4698-8e83-69b96e878dd5",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "My tail still hurts from it, even now.",
                                                    "emotion": "2",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "088b1210-1ad0-40a2-9d7b-0055e4c5b584",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "80",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "edffc13f-f9f2-4198-9e7d-2c62e8b8fcee",
                            "text": "Ask About Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c16de6e5-1426-4013-8a9b-af703c01294b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "Where is Esme now?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "da9a4ef6-6214-4f0f-88ac-08f0676abae6",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Mistress Esme is so very brave! Earlier Mistress Esme was in Wine Cellar all alone hunting Warrior Drats. ",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "c5019852-a6b6-4c97-894a-376143a468e5",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Mistress Esme told me that the Warrior Drats are coming from the room next to the Dangerous Door room, but the Good Mistress cannot get to the room because the door is blocked.",
                                    "emotion": "2",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "f34444da-265c-479d-b1e2-882b5072b7b4",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "17765ff1-bbef-49bf-b921-8dfc0d42c3e2",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "See you later.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "41febdf2-133a-413a-824f-db7ea6abc0c3",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Please be careful, Master Shawn. Don't let the Warrior Drats bite your tail... er... backside.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
