                    "options": [
                        {
                            "type": "options.text",
                            "id": "8444439a-e2d3-4145-90fc-cfc83d8d7e2c",
                            "text": "Look at the Clock",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "bc628362-d2ca-4221-9187-1120935b0387",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Hmm... this clock isn't working. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.actorPresent",
                                                "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                "hasNot": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9a322f1a-f087-4482-afb1-75a074854264",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "Hey! Did you know your clock isn't working?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5614538c-d536-48be-8cf6-8be61446f986",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I did indeed. It is meant to remind you that if you're looking at the clock, <i>you</i> are not working, either.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6800875c-700d-4fed-ac64-b1a809498d37",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I wonder why it's here, then.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "de0e0d42-0c6b-440f-a10b-0d4ff3213e80",
                            "text": "Look Inside the Clock",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c5b6ddb7-15a6-4c21-8907-120343c22289",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Nothing in here but clock parts.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "prop",
                                                "scopeId": "46016640-5006-4ae4-8be2-7e411927277c",
                                                "scopeFilterId": "981ec865-3e1b-4f3e-bb0d-11df2bc7d593",
                                                "tags": "FirstTime",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "38604fab-06f7-4aa4-8ed6-ebe5e501acbd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Unless... let me see.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.playAnimation",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "animationName": "ReachOut"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "959dee96-262e-4af3-b40c-e72b3ae21e66",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "AHA!",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b78f99bc-b32c-4b42-9a19-174dce3c0332",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Wait... no...",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.playSoundEffect",
                                                    "soundEffectName": "HeroU_VaseBreak_04.wav",
                                                    "wait": false
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e3f69478-e74a-4e3e-b5da-dadca18057cc",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "OOPS...",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "eb406bda-074f-43db-a740-ac152266c8b4",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Under closer examination, I see... nothing but clock parts. I think I'll just stuff everything back in and close the door... if I can.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.actorPresent",
                                                                "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                "hasNot": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "a1af52aa-9dc6-46ae-8a48-d0a7a6adfc9c",
                                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "I would tell you to sit down and read a book, but given the delicacy which you just displaced, I am concerned for the book. ",
                                                                    "emotion": "3",
                                                                    "emotion2": "4",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "d3d049c6-a786-4f6e-a3b9-145c7e1fb32c",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                    "text": "Sorry! It's all part of my... investigation.",
                                                                    "emotion": "3",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "3ebaf656-445e-4a5f-8959-471043fd8806",
                                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Please restrict your investigation to things outside of the library from now on.",
                                                                    "emotion": "0",
                                                                    "emotion2": "2",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
