    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "1064780a-c0bf-45f4-b684-0fe2a110a50e",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "84fe6009-2f14-43a1-b89a-f016e15f1429",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "797faa41-7bea-412d-9ede-419797f96937",
                            "text": "Ask About Gregor",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "5f502e18-9ac0-4610-ad34-61199630195d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "I see that you are in a good mood today.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "bcfe0d3d-c406-456a-bb3c-f3049b4a1c5d",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Yes, indeed, do you see these fine Drats that Mistress Esme has brought for Gregor! The Ratties will feast tonight!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "51c00643-bc9c-499a-b20c-11daad0ea001",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": null,
                                    "text": "Of course, if Master Shawn were to go hunting tonight, the Ratties would feast even more!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "78ca258b-83cc-4693-8c7b-b41f9207e05c",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "amount": "2",
                                    "change": "1",
                                    "notify": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e199a565-6a5c-41d8-923b-a45264440b00",
                            "text": "Ask About Esme",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "scopeFilterId": null,
                                                "tags": "E03talkedEsme",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.comment",
                                                    "message": "If Shawn speaks to Gregor before Speaking to Esme.",
                                                    "commentor": "Esme Ignored"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "eb1f39b7-596e-4a0f-a979-559a4421f77b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "Does Esme hunt here often?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.actorPresent",
                                                                "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                "hasNot": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "f6eaaf3e-6745-4fa0-9855-bd2d8c11a3dc",
                                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "I am here. Do not speak as if I vere not here.",
                                                                    "emotion": "3",
                                                                    "emotion2": "4",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d13339a7-ae52-41dc-b290-975300648925",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Oh, yes, Mistress Esme hunts all the time for Gregor. The mistress knows exactly how to catch the tastiest of Drats.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.actorPresent",
                                                                "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                "hasNot": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "20f40da6-ccbf-4805-872c-e808a8372fe0",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "text": "I'm sorry, Esme, I didn't mean to act like I was ignoring you.",
                                                                    "emotion": "2",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "53432250-f509-4962-99d8-3d9403b2b107",
                                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Is okay. Is just that many peoples try to ignore our people - and ve do not like to be ignored. ",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.changeReputation",
                                                                    "id": "dd1d2274-d834-4240-a5ca-384b054faa8e",
                                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "amount": "1",
                                                                    "change": "0",
                                                                    "notify": true
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "soldDrats",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.comment",
                                                    "message": "This flag is set when Shawn sells them to Gregor",
                                                    "commentor": "Selling Drats"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "eabc9bee-8044-4a8b-8288-19c7f4359ce5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "So does Esme catch better Drats than I do?",
                                                    "emotion": "0",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b378f06a-f0e7-45bc-862b-25f7d2499685",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Master Shawn catches very good Drats, yes indeed. Mistress Esme catches very good Drats, too.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3c387987-67c4-4497-a8d1-32812af9a031",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "Dead Drats look kind of gruesome, don't they?",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.actorPresent",
                                                                "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                "hasNot": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "6bd9bcc8-7534-4c54-94da-f9cf15dcfb26",
                                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "<shrug> They are just Drats. Drats are not lovely things.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2600b1e7-0b07-40c9-800c-6e40242184d3",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I think they look luscious!",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "00ec19f3-87af-419f-aa2d-c12923482a1f",
                            "text": "Ask About the Store",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b4077501-11b1-472c-8344-fc751a46803b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "So what's the best thing for sale at the store?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b2540c73-51ac-4bf6-8b73-f1e5b96e6325",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Gregor has many, many fine things for sale. Master Shawn would look very fine wearing the Official School Uniform. Gregor will be happy to sell the Uniform to the Master.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.actorPresent",
                                                "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "hasNot": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e29f68b5-6c22-4459-83e6-d00e36893bc1",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Gregor is right - you vould look better if you vore suit and tie. You vould look respectable.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8ccb3968-6fb5-4c43-9a5d-b569b29bff78",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "Would you like me to look more respectable? ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d84c8f2f-1da1-45a4-94d2-025608794262",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Respectable is good. You do not have to look like peasant. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "6",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "45d1d59b-bd9a-44db-91a8-60cda12cec65",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a212b209-a377-46a4-94ab-462f0ab27075",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "Goodbye for now.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d68aaba4-1cac-4f47-af05-b1da24608186",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Perhaps Master Shawn will be hunting Drats for Gregor? Gregor hopes so.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
