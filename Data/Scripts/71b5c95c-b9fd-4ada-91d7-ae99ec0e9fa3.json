                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.propStatus",
                                                                "propId": "45d81e68-ecca-4dff-8823-9e46a461b952",
                                                                "sceneId": "298ff758-e1ce-477a-b9d4-da614b74072d",
                                                                "status": "Visible"
