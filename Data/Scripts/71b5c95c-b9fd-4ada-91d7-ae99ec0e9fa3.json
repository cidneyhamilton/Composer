                        {
                            "type": "options.text",
                            "id": "df919853-2206-4c7a-bcf0-6b9c19603e6b",
                            "text": "Look at the Book",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4f501649-2f3c-4695-ad84-40a72376f0f4",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's a little creepy to see that book sitting there open, as if somebody was <i>just</i> here a moment ago, reading it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
