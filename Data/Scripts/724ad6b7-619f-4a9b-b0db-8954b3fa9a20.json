                                                            "type": "nodes.speak",
                                                            "id": "7e0cbefe-cc00-49d7-9fcb-d64645643657",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "You are the apple of my eye... or perhaps, my belly.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
