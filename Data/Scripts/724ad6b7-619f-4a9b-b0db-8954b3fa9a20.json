                                                            "id": "df155779-6d93-446a-93e9-fb1a7bd05dd5",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "I'll just take the plumpest, juiciest one I could find. Too bad I had to touch all the others to do it.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
