                            "id": "adae353a-1d24-461f-a817-797fa2e0bbe4",
                            "text": "Look at the Apples",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "8c3fd0df-f6d2-47af-8f28-2d117cfa321c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Chef must've set out this bowl of apples. They look great, brightly polished and fragrant.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "9b907005-4a7e-489f-a9d6-a600225d5362",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I've been known to polish a few apples myself, and quite handily, too.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
