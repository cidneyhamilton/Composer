    "trigger": {
        "type": "triggers.interact",
        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "18467e08-5093-426f-981e-ec1499ffbe3d",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "42787c73-7087-4b61-93e0-41a8d69e45c6",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "4d6eb469-7b3f-491e-928e-5989b264ccf0",
                            "text": "Look at the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "5a74affa-aee3-4642-9426-627f71ccf646",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It <i>looks</i> like a basic desk, just like the others. But I know firsthand that it's actually a pillory in disguise, using the shackles of \"rules\" and \"etiquette\" to confine me for hours on end each day...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "91ba48b6-d733-4bfd-86a6-00a12352ae12",
                            "text": "Move the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "3800e56a-d660-4f71-8a19-e49420652472",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Let's see, I could move this over on top of the Master's desk and pretend that Sosi did it...",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "68ac1079-c5bc-44db-8130-8cf140e03bc3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Assuming, of course, that I could pick this desk without straining something... ",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "c065ba19-da1a-4e69-aa1a-ad1fbb4810aa",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Eh, at times, practical jokes aren't all that practical. I guess I'll just leave this desk where it is.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "15162ef1-2936-45b4-b87d-25af89342bd2",
                            "text": "Sit at the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "679c4e2a-0034-43b7-96b7-336e86e59341",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I sit here far too often for far too long as it is already, thank you very much.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
