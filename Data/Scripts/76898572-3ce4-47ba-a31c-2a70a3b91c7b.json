                    "options": [
                        {
                            "type": "options.text",
                            "id": "baa4a1ed-e11f-471d-bcd1-bdde249e1a58",
                            "text": "Ask About Joel",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                "scopeFilterId": null,
                                                "tags": "JH9",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "0c3238ca-c255-4294-b457-78fe5cf88335",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "JH9",
                                                    "tagsToRemove": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7f33a593-c071-47a5-b318-f6ecc6fcac10",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "What's up with you?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "af9c1f23-85ca-4c59-87b3-9e68475ad712",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "amount": "1",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "92ef973d-204f-4b26-9235-8abf4174caec",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "My time spent in Caligari on our day off was most useful to me. I have learned some very unusual things about the area... and picked up a few juicy rumors. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8e1bda12-8045-4756-89f6-fc24da869a7c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Don't you get bored hanging around here all the time?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b35a3457-439e-4b0a-9eb3-47c0dc18b6fb",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I am never bored at the prospect of selling my wares. Patience is my virtue, you see.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "0440361f-3b57-43b2-a370-e88f1bb243a5",
                            "text": "Ask About the Trap Tapper",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "9a9a9e9b-93a3-4b0f-b69c-0833d2d508bd",
                                    "scope": "target",
                                    "scopeId": null,
                                    "scopeFilterId": null,
                                    "tagsToAdd": "jiquestion2",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "67e6b350-f9e9-4b11-990a-d58bf7258bdc",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "So what is a Trap Tapper?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "be065de0-edfa-4aa7-978c-4ad2bd2bd3d4",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I am so glad that you asked that... A Trap Tapper is a tool used for manipulating minute, complex machinery, much like that found in safe locks or mechanical devices.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7032d2e3-4533-4d6a-a910-b97c712cf3e6",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "It is the go-to tool for disarming traps. I believe you will find it most useful for any extracurricular activities you might choose to pursue.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fbb1c1e8-2969-444f-a217-0b9d8e0aed73",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You simply insert the Tapper into the keyhole of a door or chest and then adjust the dials of the Tapper until the trap is disarmed.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0cb8402e-c3ce-4f6a-9b01-9f0d277e4512",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Of course, there is always the possibility that you will set off the trap rather than disarm it, so you must use the Trap Tapper with caution. We wouldn't want to blow ourselves up, would we?",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "8",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "ad67da7c-7917-4ae7-8d84-3565a51ab993",
                            "text": "Ask About Rumors",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "f4f9c8df-99aa-48e2-9513-0a1a6bc0e53c",
                                    "scope": "target",
                                    "scopeId": null,
                                    "scopeFilterId": null,
                                    "tagsToAdd": "jiquestion3, jiquestion4",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "90756948-2579-42d2-86c9-0583585c906f",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "What is the best rumor you've heard recently?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3f5ecc18-77f2-4fcc-8ded-b43d9520d159",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "There's a very interesting rumor that Miss MacMichael is the daughter of the notorious Captain Mac of the Lucky Lady. It seems that she may be the pirate's daughter.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "270d0ff4-7b1e-44d2-877a-abd3a9c59852",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "However, I have been unable to verify the rumor. Miss MacMichael was rather unpleasant to me when I brought up that subject.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e02f1501-1002-4d6e-bbc9-47e43a42c6e6",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a863e67d-bd01-482b-9799-ca75843052e4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "See you later.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "99ac9911-50ab-4b74-9bc8-1a5079753ebd",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I will look forward to talking with you again.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
