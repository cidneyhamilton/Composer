                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Why bother? I've got the coin. Now I've just gotta get outta here in one piece.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
