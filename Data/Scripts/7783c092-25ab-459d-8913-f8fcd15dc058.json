                                                    "id": "e5d53ea6-3040-4897-8378-b4cf9e38cc22",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I've already tried that. I don't want to get a reputation as a trash-picker.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
