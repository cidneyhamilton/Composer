                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I've already looked there. I wasn't impressed.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
