                                                    "id": "09d14847-6e9f-4daa-a722-88390dc8322d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I'd much rather have a reputation as a no-good, low-down thief.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
