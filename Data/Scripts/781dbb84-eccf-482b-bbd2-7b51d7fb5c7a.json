    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "ab078eee-572a-448a-ab30-043af7b99751",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "2827ea2f-73a1-4f79-8c94-cf9b9082720c",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "05bd30ad-0b0b-42c1-9f63-f7f37eabedef",
                            "text": "Look at the Chair",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "abbb5f47-2c2f-4753-9b85-5dbbc762ac04",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Strange place to leave a chair - in the middle of a crypt-lined hallway in the middle of a secret hallway. I wonder what it was for?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3c8e22d1-4876-47a2-aeed-6af0d2a858bd",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Secret meetings with only one person?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "ee48686d-d7cb-4305-892d-b4c95b281ae1",
                            "text": "Sit in the Chair",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "14e73a36-95fb-4d2f-bcf8-61a10534572d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It's filthy. I'd spoil my hindquarters.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7ed30f4a-b234-46e0-82fa-661d621259dc",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'm serious. You never know <i>what</i> you might run into down here.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "056cdc85-2c11-41cb-8394-4be1b46ab4fb",
                            "text": "Steal the Chair",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b7e78a1a-9419-41e4-90b2-78d15e0bbaba",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Of all the things I could use here right now, chairs are very low on the list. I can always come back for it if I need it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7d25d3e3-907b-45a0-8b29-fd695ec583ab",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "But I won't, so I won't.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
