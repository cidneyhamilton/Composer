            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "4275ac10-200f-4092-b9e9-b5362c0177cc",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "0624eb88-d7e7-4273-b661-736e04e2e958",
                            "text": "Look at the Cabinet",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "84464ad4-fca6-4155-a4eb-8ceba0508a31",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "It's one of those cabinets that doesn't have any locks on the doors. I like those!",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "0",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "87bf6cd2-5376-4dbb-848b-6fc122159404",
                            "text": "Search the Cabinet",
                            "nodes": [
                                {
                                    "type": "nodes.playSoundEffect",
                                    "soundEffectName": "cabinet_open.wav",
                                    "wait": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "501a4325-ce53-4c65-b839-3ad72ec73a31",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "There's nothing in here except for Famous Adventurer-branded souvenir merchandise... keychains, snow globes, bookmarks, that kind of thing. I don't need any of it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.playSoundEffect",
                                    "soundEffectName": "cabinet_close.wav",
                                    "wait": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "42ed55cb-7656-4c1a-8df6-a3956b581ce4",
                            "text": "Smell the Flowers",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a935f14e-cec7-41f8-b5cc-14683f57ee28",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "That's odd. I smell bread.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "05233528-3f9c-4673-b229-f6794bee7070",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Where I come from, that's a very, very bad sign.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "1584951d-45a4-43ed-b229-9f84a2c7be3d",
                            "text": "Take the Flowers",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "f180da15-9cd3-45ad-8c2d-3352cbd95527",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "None of these flowers would make suitable presents for anyone. They're already past their give-by date.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
