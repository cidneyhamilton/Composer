                            "minutes": "2",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "3528603f-b289-47c5-a6b6-24c0b5a916ed",
                            "text": "Flirt With Katie",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a97fd8bb-edc7-45d7-b2bf-e08c9705490a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "text": "Are you willing to take a chance with me?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.reputationComparison",
                                                "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                "operator": "gte",
                                                "value": "75"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ae3a183a-13e6-4f0a-a89d-809dd2200d53",
                                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "That depends... are you willing to risk venturing in the Sea Caves with me?",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "83767409-6486-4af7-abdf-063b0130cb9d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "text": "I'll risk anything for you.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.addJournalEntry",
                                                    "id": "8a074114-1e70-46fd-b46c-9ab08b0defd7",
                                                    "name": "Woo Hoo! Date With Katie",
                                                    "notify": true,
                                                    "description": "Katie agreed to have a date with me tomorrow in the Sea Caves. I don't want to miss it!"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e91d3527-dfa5-4026-810d-f298c1129276",
                                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Okay, meet me at the base of the stairs in the Sea Caves tomorrow after breakfast and we'll spend the day together...",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "a3001ba4-15d3-46d8-9fd2-363963a5d810",
                                                    "scope": "actor",
