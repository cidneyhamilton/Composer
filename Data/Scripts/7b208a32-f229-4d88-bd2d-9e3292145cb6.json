                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "ef18f136-8368-47f7-80a3-6c6443fd27d6",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "aab8a14e-ec39-4bba-8dc0-c5ec352e5b3e",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "text": "Good luck, Katie. May the sneakiest, most deceitful win.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
