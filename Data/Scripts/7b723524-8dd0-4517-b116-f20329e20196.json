                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "25dc0e51-1073-4bba-bf78-5d2f065ffb31",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "There's nothing for me to forage back there. Not even a wild mushroom.",
