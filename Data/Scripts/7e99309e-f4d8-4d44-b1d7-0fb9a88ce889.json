            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "34ed1359-39a2-486e-8d18-31fb268c2fae",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "8f2ec91f-55f5-40bb-8992-ecfc6a5422d0",
                            "text": "Look at the Suit of Armor",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "422a60b5-1dae-46a3-a70b-683dc5ea8b30",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "He looks so forlorn standing there empty, with nobody to kill.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9d80327e-0401-4842-8b48-4b6b96d7aaf4",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's a suit of armor, I guess. Hopefully, with nobody in it. And no<i>thing</i> in it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "6c57e278-ec8f-4026-8df1-071c4edc07c4",
                            "text": "Look inside the Suit of Armor",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "aafcd688-7d65-43ae-886a-84d7db7416b9",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I don't see anything inside it, but there's an unpleasant smell inside. Might be a dead mouse or something. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bba20b5b-7989-4447-92ea-84b4b3853156",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "In fact, I really <i>hope</i> it's a dead mouse, and not something larger... or something not quite dead!",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c6a29d6c-6f83-47e8-8f31-7f8a5fae24b5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I'll have to wait 'til I have some light. I'm not messing around with anything I can't see.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "f9611867-a2a0-45b0-8375-2a9b4eeb3761",
                            "text": "Wear the Suit of Armor",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1cc83b06-a203-4338-b82b-a94a04fb62df",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I think what I'm already wearing is perfectly appropriate for skulking around hidden hallways!",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3e6dae0e-dbe0-4fad-aabb-df3ce00dcd36",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I don't like wearing hand-me-downs.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
