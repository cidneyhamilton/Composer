                                            "expression": {
                                                "type": "expressions.actorPresent",
                                                "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c4d32440-9868-4f22-ae12-83933ffd7620",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Thank you, Master! Gregor does his best!",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
