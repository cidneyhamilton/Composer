                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f98599a1-8e30-42f2-957e-12287caf6a16",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Well, except for that big messy pile of crates over there.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
