                            "id": "b12a67b8-ee9b-4280-b76d-571d28194e39",
                            "text": "Look at Store Counter",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c1bfae03-72fe-4c56-a891-51b0481cb9c4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The counter's absolutely bare. They keep this place neat as a pin, I must say. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
