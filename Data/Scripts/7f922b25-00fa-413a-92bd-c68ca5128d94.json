    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "1947e108-e949-4287-a189-a4e9f2df5113",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "0ff39862-98aa-440b-ad13-ee8b43de7b2c",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "eab69ec8-8c30-4866-ad01-d5b8ec3b0567",
                            "text": "Ask About Joel",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "769288fc-58ba-45de-8972-ec06bde3101a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "How are things going for you?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "353f9706-5b7a-4ff7-a88d-e6f576798615",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'm fine. Very fine. Everything is going very fine.",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "a2b6ea80-2ad9-4b23-b765-78bb3c174aa2",
                            "text": "Ask About the Map",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "a6fa5261-84a3-4384-a968-ae9d0cbad532",
                                    "scope": "target",
                                    "scopeId": null,
                                    "scopeFilterId": null,
                                    "tagsToAdd": "jiquestion2",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e826f7a1-43a1-4084-b100-8cd9e8c65c94",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "You wouldn't know anything about the theft of Katie's treasure map, would you?",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "bf451b5c-728b-4d41-a55f-c48112f418ad",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Who, me? A stolen treasure map? I have no idea who would do such a terrible thing!",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "44b6cbe5-f86a-45f7-a0cb-c3209e776f6b",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Such a theft would break the rules of the Disbarred Bards class. Such a thief might well be expelled from the school. ",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a395ab44-c98c-495f-8209-8400434e7c9c",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I do not wish to be expelled from here. That would be bad.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "4503cd5b-cab5-4bd6-99b7-3fafe93fbaff",
                            "text": "Ask About the Shop",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "53d7ca58-49ab-4dee-8aa3-849361c0d5f2",
                                    "scope": "target",
                                    "scopeId": null,
                                    "scopeFilterId": null,
                                    "tagsToAdd": "jiquestion3",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "23fc8b8b-2e36-48d5-8ef7-e64ba2be8e9e",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "You got anything new for sale today?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0fb3e923-1937-41fc-9de2-6fc2da8deeb9",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Sale? Oh, um... yes, of course. ",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0d8fd81a-027f-4143-86c7-a34d6eb4d11c",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I have a few samples of itching powder today. It can be used as a weapon. When thrown at a monster, it distracts them enough to make them less likely to hit you.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b4b06574-e921-4d55-a4da-2bb0a30a56ad",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Of course, it can always be sprinkled upon a friend's bed as a prank, not that I'd recommend such an act, mind you. While it is amusing, it is rather unpleasant for the victim.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "6",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "db7ef008-6f9a-444b-9ec7-0bb0fd2a5c1c",
                            "text": "Ask About Rumors",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "dc33c8c6-e547-4695-ae9b-276a61a212fa",
                                    "scope": "target",
                                    "scopeId": null,
                                    "scopeFilterId": null,
                                    "tagsToAdd": "jiquestion4",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e86b66b6-c075-4459-9c2d-7650946b74f3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "Did you hear rumors of Katie's map before today?",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "eda46709-ec6b-46a9-acb5-d4b359876fce",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I... uh... just happened to overhear Katie talking to you about the map yesterday in the Dining Hall, but I assure you that I was unaware of it before then.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "64f4b430-6587-4f4d-a839-e7e000e8981e",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Miss MacMichael certainly seemed most upset. I do hope she does not suspect that I would do something like that. I would not like to face her wrath.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "dcb87659-4695-4ced-a4e5-20c3eec0b826",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "727718ba-4246-45e2-9355-58ea865e09b5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "Talk to you later.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "5a33fda3-409c-4e08-a163-f8e85a2415dd",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Yes, of course...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
}