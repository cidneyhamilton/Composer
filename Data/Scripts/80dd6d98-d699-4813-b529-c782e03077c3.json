                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "c9ae5b6f-7e41-44d4-947f-4dfc547d5a67",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "This huge book's falling apart. I think it says, <i>The Recipes of Nostradamus</i>. Oh, right... he's the guy who wrote thousands of really obscure recipes. Or something like that.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "79727eca-ac9d-44fe-8c49-4d1084af6e2b",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "Looking through this, I can't understand any of these recipes. But... I'm sure he didn't have someone like me in mind when he wrote this. There's no way he could've predicted that a guy like me would read this book.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
