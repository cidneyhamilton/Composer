                        {
                            "type": "options.text",
                            "id": "b29d1941-436c-4ebe-9a2e-f8f05696481c",
                            "text": "Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "da741cdc-545c-446e-a061-163f6d673a29",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "text": "Good day to you, sir.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "2b4e5eed-93ce-448c-bce2-38039afcc60e",
                                    "scriptId": "8f2ac6a0-3073-44b0-b814-fdfcbc24ab9d",
                                    "entryPointId": "6cac2b88-9657-4bd2-aa9d-8314949d75cd",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "eceae806-6f55-4cde-b517-c73b1c924fd9",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "53d185f9-3d8c-4059-b068-552b492450e1",
                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Make the most of the time you have... this semester is almost over. Good day to you.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
