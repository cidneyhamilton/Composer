                                                                "compareTo": "true",
                                                                "sceneId": null,
                                                                "propId": null
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.changeTags",
                                                                    "id": "8b48212c-16c3-4072-bb87-abf4ffa6fb7e",
                                                                    "scope": "actor",
                                                                    "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "scopeFilterId": null,
                                                                    "tagsToAdd": "StudiedBookMoxieWill",
                                                                    "tagsToRemove": null
                                                                },
                                                                {
                                                                    "id": "b357ba55-4616-4b99-b3e6-a0d580895efe",
                                                                    "type": "nodes.improveSkill",
                                                                    "skill": "Moxie",
                                                                    "amount": "5",
                                                                    "notify": true
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "85b192df-4b1a-4f07-8536-c9377d2f231a",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Well, before the book started going off on how Paladins use self-discipline and Will to cast spells and other paladin-y stuff, the book talks about showing self-confidence and believing in one's own abilities. Attitude and confidence are important for everyone.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "2638532e-6b3a-431b-a232-7a6020e1f317",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "I'll check it out some other time.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
