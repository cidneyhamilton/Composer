                            "id": "c70b299d-229a-47a0-8776-b3d9665fe627",
                            "text": "Dive into the Pool",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "85f93e23-9a4e-4553-849d-e1c0d8c76e0d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The water looks like it's only a few inches deep here.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
