                            "id": "dc061fa2-9110-4fca-af43-b423f05f01a1",
                            "text": "Flirt With Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "33a14a6e-dd3e-4ffa-9b35-edf8c748670f",
