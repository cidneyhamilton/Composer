                                                                "scope": "target",
                                                                "scopeId": null,
                                                                "scopeFilterId": null,
                                                                "tags": "KnockedTwice",
                                                                "has": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "704e8949-b018-4b5f-aaab-249d7d733f43",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Surely the Gods were merely <i>testing</i> me! Surely if I knock on one of these OTHER barrels, my prayers will be answered!",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0",
                                                                    "notes": ""
                                                                },
                                                                {
