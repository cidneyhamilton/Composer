                            "text": "Look at the Barrel Rack",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "0b73c2c6-b123-4507-ba16-6b5826070321",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "They must've aged most of their wine right in this room.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0",
                                    "notes": ""
