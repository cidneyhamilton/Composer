                                                    "change": "1",
                                                    "count": 1,
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.playAnimation",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "animationName": "KneelUp",
                                                    "wait": true
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "70f502e1-2209-466d-91d8-99b219a05ce6",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "gotwine",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        }
                                    ]
