    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "d79ef759-97ca-4e4c-9fc6-b4ab8280523b",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "91788b3e-b8cd-4339-9021-d1eeb11072cd",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "7f9e1b36-73dc-4411-a94c-3de32309dccc",
                            "text": "Look at the Pallets",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "9f0a18f9-9449-4218-932a-a2de06307008",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "They look sturdy. But I've seen much better ones.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8c20dd4c-c587-4743-a862-1c6cd7017bc5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "For example, I really loved the pallets of Versailles.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "b2a8114a-9d31-4949-b323-295a7a6a5383",
                            "text": "Search the Pallets",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "d0c9ac0a-5d85-4261-aa9c-0873d88c6d95",
                                                "scopeFilterId": null,
                                                "tags": "ProachDead",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "af6c8693-8cf3-47c9-bc0a-344796148f91",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I don't have time to search this at the moment... ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "scene",
                                                                "scopeId": "f6710e8e-d7d6-4401-9124-28d089cd9d71",
                                                                "scopeFilterId": null,
                                                                "tags": "sawProach",
                                                                "has": true
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "48e7f18c-01f8-4910-9e36-3080142be8d8",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "That Giant Proach could catch me - and that would be unpallet-able.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "01ea2638-a5ac-4f4e-bae6-670a0672e37b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Hey, there's something stashed away here - it looks like a bottle of wine!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "dbb2b5bc-fd0b-420c-b1c7-3c51c72d54fa",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "So why would anyone hide a bottle of wine in a wine cellar?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.playAnimation",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "animationName": "KneelDown",
                                                    "wait": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6fed1996-01d6-4346-b616-9268adc4a846",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It says it is <i>Dryad's Kiss</i> wine. Must be pretty special stuff.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeInventory",
