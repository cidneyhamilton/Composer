                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can't think of anything else to say to Esme right now.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
