                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "eafe2b18-b559-4c1b-a267-f2bd9b996276",
                                            "text": "Say Something Charming",
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeTags",
