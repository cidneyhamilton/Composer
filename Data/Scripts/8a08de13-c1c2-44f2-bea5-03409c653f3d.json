                                                    "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "text": "Since you know everything that happens here, I know that I'd like to get to know you better.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
