                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                                    "text": "Well, Sugarplum, I came here to treat you to more of my sweet smile and honeyed words.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
