                                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "I sincerely hope that was merely an aside to nobody in particular, not a direct question for me to answer. I'm not a reference librarian. Research is good for you.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
