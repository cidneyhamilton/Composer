                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4c78dfdf-89f2-4d6a-8b5b-7f88e78cc863",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The plaque says that this is \"The Famous Explorer - Who Boldly Goes Where No One Has Gone Before!\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
