                {
                    "type": "nodes.speak",
                    "id": "9d64a1e7-e5fb-4bc8-a552-99c8c31f74a4",
                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "Yes?",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.showMenu",
                    "id": "592cc7e7-1042-4393-b73d-d4a7b39e84dd",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "f8958d58-d821-46fa-973f-ce0a68357fc1",
                            "text": "Ask About the Master",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "3035b8a3-98a4-47b9-8dcf-ecbffc9933e0",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "text": "You originally came from Germania, didn't you?",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "9d2dede0-4e80-4072-a5dc-8dbca2cadb07",
                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You are correct. However, I do not desire to discuss my past with you at this time.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "d89175d5-3193-48e6-991b-554761cc78a2",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "40b7bca7-d2aa-45b3-8540-12cc78879cc0",
                                            "text": "Say Something Polite About Germanish",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b453f0de-39c2-4a7d-9255-9908fdeae433",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "text": "I'm sorry, I wasn't trying to pry. I was just curious because you sometimes speak Germanish.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b8d669de-1d06-4563-98f0-7e1f0e9b32c2",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "text": "Ja, I hope to expand your vocabulary... or at least your interpretation skills.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "8f46c2ca-eac5-4987-9e96-09dd35cb2618",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "40",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "1e19960a-37c5-4671-b1ef-9b2e82a596cf",
                                                                "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                "amount": "2",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "4",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "ef5f418c-69d0-400b-af63-aaccdde0f68a",
                                            "text": "Ask Something Clever About Germania",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6d03dde3-1e3a-462d-9b8a-ca3563d96665",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "text": "I was just curious about the nation of Germania. Is it true that they don't have a king or an emperor?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "29cfa046-58fc-4555-9eb7-5ab306b39f39",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Germania is composed of twenty duchies all united by their mutual hatred of one another.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "98d9b81d-65e5-41c7-a0db-210becd947c5",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "40",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "5726d9a8-584a-416c-a554-02322426136e",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "5",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "41293b34-b576-444d-86e0-0f3c5cd64547",
                                            "text": "Say Something Snarky About Rogues",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3cd8458c-3646-41e5-a33c-954f212ef04f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "text": "Are all Rogues supposed to be secretive and mysterious or is that just you?",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c223f478-6513-404c-8c1b-5781002a7cf2",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "We all have our secrets, Herr O'Conner. Some more than others.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "5c1b240c-16c8-4aed-9080-d4679f91c94b",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "40",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "3aa6a8d0-2546-40c7-ac0b-42f36dee822f",
                                                                "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                "amount": "2",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "aa9c94ba-89fd-4d07-9002-e3905800d6d9",
                                                                "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "bd02bac1-7277-4e85-b474-ed2683eb11da",
                                            "text": "Say Something Perceptive",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "42cc1341-230b-4661-9b2d-518979420428",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "text": "Does this mean that at some future point, you will want to talk to me about your past?",
                                                    "emotion": "4",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e6bfb6b7-7429-48d1-acf8-bd4ae44c3be1",
                                                    "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "There might indeed come a time when my past will be relevant to your future. However, that topic is closed for now.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "c4510860-7e8a-4045-b032-e03bfb3c4aea",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Perception",
                                                    "target": "38",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "08b6f202-6d98-4d38-a57a-7a6c6e8fa58e",
                                                                "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                                                "amount": "2",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "4",
                                            "ExitMenu": false
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
                            "expression": null,
