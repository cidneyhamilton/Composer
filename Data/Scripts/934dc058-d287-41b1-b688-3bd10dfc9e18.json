    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "937d27a8-a6c1-44fa-91b6-0c54eb8fa495",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "c0d4988b-1732-4519-8b92-8230ecb74362",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "f890d9e8-25a4-414e-8dc2-b23b02b30b1e",
                            "text": "Examine the Desk Lamp",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "5bdf38cb-afce-4278-84d4-eb3d28793779",
                                                "sceneId": "47ebdad9-eb30-4230-b1b2-b576e5ccbe0d",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d18436a2-8fde-4a76-b92b-99ba5acb7c75",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I didn't get in trouble last time I turned it on. I'm guessing I won't get in trouble this time, either.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0f2a0e58-d05e-43d3-86b1-e5e420b5ef5b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's kinda dim in here. But I remember the switch on the side.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "580305c0-c173-4324-b0a4-7e43820953bc",
                            "text": "Read the Book on the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "5bdf38cb-afce-4278-84d4-eb3d28793779",
                                                "sceneId": "47ebdad9-eb30-4230-b1b2-b576e5ccbe0d",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.playSoundEffect",
                                                    "soundEffectName": "book_open.wav",
                                                    "wait": false
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "37b43c87-6f11-4c04-9e88-dd98ffda5faa",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Oh, yes... <i>The Little Black Book of Chartered Accountancy.</i> How could I forget <i>that?</i>",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7986fcf4-f527-436d-abb6-06a5a8b1dc10",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "If I ever had to read it, it would drive me stark Raven mad. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.playSoundEffect",
                                                    "soundEffectName": "book_close.wav",
                                                    "wait": false
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "44fdc2a0-22d3-40c7-9084-871fbcd98f6a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's too dark to read the text. I think it's better this way.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "33417cb5-6e33-40bb-a225-9c551842b95c",
                            "text": "Check out the Book Set",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "5bdf38cb-afce-4278-84d4-eb3d28793779",
                                                "sceneId": "47ebdad9-eb30-4230-b1b2-b576e5ccbe0d",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b99b6d79-dd96-472e-85f9-98e23a47e038",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It's the multivolume set, <i>CPA for Illiterates.</i> I don't know how you write something like this. It's mostly rebuses.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a1fd4f2c-03e1-4eef-b89c-24cad0cb7dab",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Can't see it very well in this light.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": {
                                "type": "expressions.propStatus",
                                "propId": "5bdf38cb-afce-4278-84d4-eb3d28793779",
                                "sceneId": "47ebdad9-eb30-4230-b1b2-b576e5ccbe0d",
                                "status": "Visible"
                            },
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "2454ce78-612f-4196-ac97-bdf7b8134cf3",
                            "text": "Examine the Pictures on the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "5bdf38cb-afce-4278-84d4-eb3d28793779",
                                                "sceneId": "47ebdad9-eb30-4230-b1b2-b576e5ccbe0d",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5820adc4-8661-403c-b72c-bbbd3fa3cad0",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Pictures of two old women holding cats. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4b9a2e6a-d150-4a01-b5ad-52fa747a3c7d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I don't know who looks more unhappy, the women or the cats.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5e3f94be-e2e7-46b9-bbe7-5153d58f15b9",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can't make out the pictures. It's too dark in here.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "4a8245d5-8a9b-4319-a861-69e627f3e0da",
                            "text": "Extinguish the Desk Lamp",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "49706090-aad6-4839-b708-a52169708300",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Best to turn the light off. I don't want anyone to think the room's occupied.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
