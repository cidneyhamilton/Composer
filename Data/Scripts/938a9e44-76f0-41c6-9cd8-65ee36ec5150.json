    "name": "I11: Drat Invasion",
    "sceneId": "1c60250c-dab6-4aaa-aaeb-b7f4203ccb9d",
    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
    "trigger": {
        "type": "triggers.manual",
