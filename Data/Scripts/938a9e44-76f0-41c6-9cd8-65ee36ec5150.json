    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "b53e800a-8dd9-4ccc-9cfd-e26e2f61c315",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "0fddde03-d900-433c-ba14-110ac4f826f7",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "5ff8303b-50a4-44a7-822e-48eeecc2d802",
                            "text": "Talk About the Dire Rats",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "3ec7237c-65ea-4a23-9e21-697f17a2dbe8",
                                                "scopeFilterId": null,
                                                "tags": "QueenShawn",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e1594539-2d62-4bbf-a240-c45aeeafa481",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "I've taken care of the Warrior Drat threat. You should see fewer injured students for a while.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f3ca7510-ba97-4102-832c-d35c1cf3211f",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You did very well, Shawn! We shall make a Paladin of you at this rate.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "04d96676-b817-4e46-8cd1-22971bd06d51",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "I've been dodging Drats all day. How come I don't see any around the infirmary?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6a8de1a9-fc3c-4c1f-a97d-212d9926e20c",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I've had to take down several of them here in the last hour. Gregor was kind enough to dispose of the bodies. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "5b460ac9-0738-42af-b982-013fe55ce262",
                            "text": "Ask About Drats",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "85ccea15-646d-4c6b-ae6d-c238dc9f39d7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "text": "Why do you think the Drats are running around here?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "acb3582d-bcca-4be2-89d9-3fb0ddcd45d5",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Professor Featherstonehaugh says that there is some sort of magic controlling the Dire Rats in the castle, but she has been unable to determine the source.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e6f72fc7-3451-45a6-acbd-0dd0f3641fa6",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The bad news is that she believes that the spell originates from someplace within the castle itself. There are many mysteries about this place. Please be careful wherever you go until we find the source of this problem.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "3ec7237c-65ea-4a23-9e21-697f17a2dbe8",
                                "scopeFilterId": null,
                                "tags": "QueenDone",
                                "has": false
                            },
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "ae4aa35b-dd85-4bd6-9eec-7df1a9c0e003",
                            "text": "Flirt With Moira",
                            "nodes": [
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "75cc05ed-7498-4e6f-b32d-3fb3e9a3ce96",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "26ebb0b4-7514-4a63-988b-886f82a10635",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "text": "You know, I'd be happy to hang out with you this afternoon to distract you from those vermin. ",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d693a4e2-f832-41a3-aedb-3eb328573630",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'll be fine without your help, thank you.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "a1f9b569-9a8d-4a07-9d80-333f6edbde3a",
                            "text": "Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.onceOnly",
                                    "id": "4dc6a375-7a7f-4ba9-b463-5366ca8b7477",
                                    "nodes": [
                                        {
                                            "type": "nodes.changeReputation",
                                            "id": "ad3efe84-7989-4532-8e2c-51b147d41db8",
                                            "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                            "amount": "1",
                                            "change": "1",
                                            "notify": true
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "97e9a07c-9ae1-469e-abb9-5e2c2776cdc0",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "text": "I'll see you later... don't let the Drats get you down.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "52fbe3d9-f996-41e5-af38-e2c91b0c4b2b",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Try to stay safe, Shawn.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.endScript"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
