                                                    "type": "nodes.speak",
                                                    "id": "1bb607e5-0b32-47fa-acdd-e16d3d75f559",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It might be serving some purpose here that I don't understand. So I'll just leave it. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
