                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "2e784d2b-8a50-4458-a20d-dcef00888c4a",
                            "text": "Lie down on the Rack",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
