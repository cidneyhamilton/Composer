                                {
                                    "type": "nodes.speak",
                                    "id": "72f35982-b4df-4533-95b8-0d510b10acc8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "text": "So the real lesson from this class on mushrooms is that Heroes just want to have fungi?",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
