    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "d26a9121-6673-4128-b887-886e29b35c9d",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "a412d007-a90e-4fa6-9fc6-747c081455c2",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "f7bdf722-3aca-4879-9835-6ab1ae2bc0c2",
                            "text": "Examine the Desk Lamp",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "e87cea39-f378-4c15-b4ab-720b61cc32f8",
                                                "sceneId": "212f8023-648d-45df-8f16-d56a7f081d41",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d9edf8f2-0081-4a7e-942f-c2206ae57a2c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The lamp illuminates the desk nicely. Hopefully nobody will notice that I turned it on.",
                                                    "emotion": "0",
                                                    "device": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2b9523bc-eab7-4cc2-9474-f98af0554ef3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's quite bright, really, a smart modern model.",
                                                    "emotion": "0",
                                                    "device": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "24c7f3db-6c4a-4156-b03f-8663d81efdf6",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The lamp is a bit dim, but I can feel a switch on the side.",
                                                    "emotion": "0",
                                                    "device": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "91680bf9-7f39-49cc-898e-8f829085f41a",
                            "text": "Read the Book on the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "e87cea39-f378-4c15-b4ab-720b61cc32f8",
                                                "sceneId": "212f8023-648d-45df-8f16-d56a7f081d41",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.playSoundEffect",
                                                    "soundEffectName": "book_open.wav",
                                                    "wait": false
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fd2a474f-6763-4a7f-ad41-3046990298d0",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's the essential <i>Little Black Book of Chartered Accountancy.</i>",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7fd63ff8-7286-4b42-951b-f9d6658b989e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I am <i>so</i> glad I'll never be an accountant. That stuff is dangerous! It could bore you to death.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.playSoundEffect",
                                                    "soundEffectName": "book_close.wav",
                                                    "wait": false
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "269c2393-9248-4f01-8ed7-b68728ee85b7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It might be an interesting read, but for now I'm in the dark about it.",
                                                    "emotion": "0",
                                                    "device": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "c87d7687-864e-4cc9-b0d3-4e8d2036a58c",
                            "text": "Check out the Book Set",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.propStatus",
                                                "propId": "e87cea39-f378-4c15-b4ab-720b61cc32f8",
                                                "sceneId": "212f8023-648d-45df-8f16-d56a7f081d41",
                                                "status": "Visible"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1a38b7f9-3063-4bdd-9e6e-541f1ef45de8",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Great, a complete set of <i>CPA for Illiterates.</i> Wait? What?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f8414bc4-2a68-4d1e-837c-a55f786e75fd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can't quite make out the titles in the dark.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": {
                                "type": "expressions.propStatus",
                                "propId": "e87cea39-f378-4c15-b4ab-720b61cc32f8",
                                "sceneId": "212f8023-648d-45df-8f16-d56a7f081d41",
                                "status": "Visible"
                            },
