                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "efdc10c1-60dd-4d26-be56-6cb189428bae",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Nice! That really makes things brighter around here. It will be much easier to find the coin now.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
