                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "d0740e09-c926-4f9c-ac37-c8fbabd51506",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Let's see what booty ye hold.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "3693deaf-ab64-44ce-b0b8-ea728d21d09b",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Pickpocketing has never been easier.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "6dddb8d9-92a6-445b-81de-ff23e88a7076",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Give me all your pirate loot!",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "0a7e33e3-0c82-48f9-b3c8-46cc4ad479d4",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Got anything on ya?",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "ab00b407-48b9-4d4a-af52-2bbce493d9dd",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Sorry... this is undignified for both of us...",
                                                            "emotion": "2",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "bd539089-49f2-4220-a2bd-bf45200dcc5d",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "It's a wonder this purse hasn't rotted through...",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "637f94df-3410-49f6-a561-67f703b78af2",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "I hope I don't get crabs from digging around in here.",
                                                            "emotion": "2",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "e50b6031-b737-49d7-869b-d76dfc3b66e0",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Pardon me... I'll rob you as respectfully as I can...",
                                                            "emotion": "2",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "8f082905-bb8d-4311-9081-78427a41445c",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Any treasures for me today?",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "f3a991da-0884-4123-9fd1-6164b53a2ba9",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "At least I'm just looking for money... I don't have to bring any chunks back for Gregor...",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "e695594c-1259-4e61-b1f2-d564655506e7",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Easy pickin's...",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "ce36b870-16ed-4d16-85fc-feb69161d4e4",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "You're really dead now, so this is totally on the up-and-up.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "f922c4ec-3973-4653-8efd-bdfe19f7e32b",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Sort of pathetic, how these zombie pirates still carry around their loot, though they haven't any use for it.",
                                                            "emotion": "2",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "5be7411d-9618-43d9-9c02-e0dbe1bbbca1",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Mind if I search you for Lyra? Of course you don't.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "3f7c8f78-0d35-408d-8066-d8311b850b56",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "I'll say this for zombie pirates: they sure knew how to save for a rainy day.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "4a7402f0-06cb-4136-87bc-30df816b4663",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Don't worry, you'll never miss it.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
