                                {
                                    "type": "nodes.speak",
                                    "id": "87da48a9-655c-4bf6-800c-e58376bfad48",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "This is clearly a chair for somebody more important than me.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
