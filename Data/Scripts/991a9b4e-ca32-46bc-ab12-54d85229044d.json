                        },
                        {
                            "type": "options.text",
                            "id": "eb2121fc-54a2-4ef8-a3a4-0c95aabc0384",
                            "text": "Tell the Truth",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "22187ef6-45da-4060-bdc9-6d8b5eb35b44",
                                    "scope": "scene",
                                    "scopeId": "905016da-4317-4d06-945d-1357b12b0d73",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "R08",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "6e58ea53-4ea3-4d34-a340-c1667dd067a3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                    "text": "I haven't found the time to do it yet, but I'll get around to it tonight.",
                                    "emotion": "0",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "cdbd6dd8-55e7-4623-8af9-c62af835c0b3",
                                    "actorId": "6378f038-f4cd-49aa-8357-dcb46a978684",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I hope so, Honey, I really want to see what's in that file. I can't wait...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": {
                                "type": "expressions.and",
                                "left": {
