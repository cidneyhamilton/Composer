                    "options": [
                        {
                            "type": "options.text",
                            "id": "be540eb6-5169-4844-a1a3-87151406ed1f",
                            "text": "Look at the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "f5bb98cc-7e90-4cde-8e2a-432d89a632df",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Thomas likes sitting up front. Fortune favors the bold.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "b88f2396-db19-41de-823d-af61da80c1fe",
                            "text": "Move the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "c38ee34a-0a7a-4e7b-8423-005d2b31a1a1",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "c0d39889-6d11-4841-8d87-75862a2282b1",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "There, now it's a bit to the right.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "a38a3e26-bc46-4efb-9d59-01e93226c7ed",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Okay, I moved it forward a little bit.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "41950666-1dc6-427b-8ca8-a2f72e744cf2",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "All right, I pulled it just a hair to the left.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "d715e31b-6a32-4e78-b349-b5dcabb5b726",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "... and I pushed it back a skosh. We're back where we started from.",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "04a2d033-a0a7-43b8-a593-4135ad210f52",
                            "text": "Sit at the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "ebcbfe16-0758-42b3-84ca-338a1ef6413f",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I don't like sitting up front. Even if it's only for a moment and there's nobody watching.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
