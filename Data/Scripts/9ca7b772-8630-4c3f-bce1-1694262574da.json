                                    "id": "8d5a9427-3468-435c-b73e-7a9af8879661",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Nothing worth finding. I think it's too wet here by the fountain to grow any of Moira's magic medicinal mushrooms.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
