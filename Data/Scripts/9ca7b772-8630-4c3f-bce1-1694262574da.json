                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "ed1bbfb7-8940-431b-9995-dbf000cd51ce",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I <i>love</i> the way the fine spray waves over my face. I try to let it do that every chance I get.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
