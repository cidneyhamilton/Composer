            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "82be93e0-a001-4181-8001-a88affb061f9",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "90625ec2-8b8f-4761-be1b-4628a203d288",
                            "text": "Look at the Statue",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.showCloseUp",
                                                    "imageName": "HansStatueCU.png"
                                                },
                                                {
                                                    "type": "nodes.branch",
                                                    "sections": [
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": {
                                                                "type": "expressions.inTags",
                                                                "scope": "prop",
                                                                "scopeId": "633284d9-8e35-44fb-bd1f-2a91639f7f8e",
                                                                "scopeFilterId": "981ec865-3e1b-4f3e-bb0d-11df2bc7d593",
                                                                "tags": "SEEN_PORTRAIT",
                                                                "has": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "0ba196c3-4dc5-4863-8659-3ec408d164df",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "This statue looks ancient. And the inscription on the bottom is worn almost smooth. I think I can make out the words, \"Sir Hans.\" ",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.changeTags",
                                                                    "id": "d90c1a5b-0a41-4363-a348-e834c4248384",
                                                                    "scope": "prop",
                                                                    "scopeId": "a113771e-41a2-4542-896f-888821c5ab96",
                                                                    "scopeFilterId": "298ff758-e1ce-477a-b9d4-da614b74072d",
                                                                    "tagsToAdd": "SEEN_STATUE",
                                                                    "tagsToRemove": null
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.branch",
                                                                    "sections": [
                                                                        {
                                                                            "type": "nodes.branchSection",
                                                                            "expression": {
                                                                                "type": "expressions.inTags",
                                                                                "scope": "prop",
                                                                                "scopeId": "a113771e-41a2-4542-896f-888821c5ab96",
                                                                                "scopeFilterId": "298ff758-e1ce-477a-b9d4-da614b74072d",
                                                                                "tags": "SEEN_STATUE",
                                                                                "has": false
                                                                            },
                                                                            "nodes": [
                                                                                {
                                                                                    "type": "nodes.changeTags",
                                                                                    "id": "5f90884c-ae6c-493b-b51f-b8dd0e05438d",
                                                                                    "scope": "prop",
                                                                                    "scopeId": "a113771e-41a2-4542-896f-888821c5ab96",
                                                                                    "scopeFilterId": "298ff758-e1ce-477a-b9d4-da614b74072d",
                                                                                    "tagsToAdd": "SEEN_STATUE",
                                                                                    "tagsToRemove": null
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "885c0119-99a6-4974-86a4-b3ca23c046ad",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "He looks so familiar! Where have I seen this guy before?",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "8b6922d4-8813-4afa-9706-0d8efeaa4618",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "This is going to annoy me until I think of who it is.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.playAnimation",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "animationName": "KneelDown",
                                                                                    "wait": true
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "f800ff23-c993-4eaf-9ede-a387c3eff557",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "Let's see if I can make out the plaque.. it's almost eroded too much to make out...",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "28fdbdc8-426d-47e8-93a8-a78c5f97fc98",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "Ah, this is Sir Hans! He's the guy in the painting. I knew I saw him before. Well, at least a painting of him.",
                                                                                    "emotion": "1",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.playAnimation",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "animationName": "KneelUp",
                                                                                    "wait": true
                                                                                }
                                                                            ]
                                                                        },
                                                                        {
                                                                            "type": "nodes.branchSection",
                                                                            "expression": null,
                                                                            "nodes": [
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "859b0134-063e-449e-9436-750cc4e20659",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "The plaque says this is Sir Hans. He must be important to have both a painting and a statue in the castle.",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                }
                                                                            ]
                                                                        }
                                                                    ]
                                                                }
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6639fe30-c86b-4e88-bab5-861e1edace3c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can't make out his features, but I can tell it's a statue of a man. Or a woman. I'm almost sure it's one or the other.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "f8ac864e-fbbc-473f-b439-ecbac06bdadc",
                            "text": "Move the Statue",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "24f0d9e6-f4fb-4aa5-89ca-b02aea6c9241",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I don't know, it looks so old. I'm afraid I might break off an arm or two. A statue without arms would lose all its value forever.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fc033e3a-e5c1-47fb-b389-3d87dcbaa3fe",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I'm not going to try to move this thing in the dark. If I get hurt in here, it'd probably be a long time before anyone found me. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d6964f24-4b6b-49f7-a080-3cbd492d7443",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It'd make a great epitaph, though. \"Here Lieth Shawn O'Conner, Found Rotting In the Cold Embrace of a Statue.\"",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "d2922f89-89d1-4f37-a3de-93559729d0fc",
                            "text": "Take the Statue",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cee65910-ee40-4a80-b14f-b6879e2ef7bd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I knew a man who tried to steal a statue once. After that, whenever he coughed, parts of his groin would bulge out in a very alarming way. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7b7c0d98-5fea-40d0-9a4e-1cf7e963a5d1",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "This is why I'm disinclined to give it a try.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1f5b7e57-8e76-4ac1-af79-59135087cb55",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Trying to take something this massive in the dark is a sure recipe to shatter something.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9e1bc835-9516-4cf5-9271-86e15e176e17",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Like my ribcage, or my skull, or...",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
