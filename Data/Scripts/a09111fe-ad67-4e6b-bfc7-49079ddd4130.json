                            "type": "options.text",
                            "id": "9ed4ec73-f5c4-4d2d-9f3c-dc6052a0a59e",
                            "text": "Examine the Bookshelf",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "54e78958-3d53-4062-b561-d18e207f6a78",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "This section has books like <i>Alchemy for Amateurs</i> and <i>Casting Spells with Zoltar Winkleman</i>. These books must cover Alchemy and Magic.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "46c4f1d8-5311-4f25-ab22-d179a08d802b",
                            "text": "Study the Books",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.skillCheck",
                                                "skill": "Magic",
                                                "target": "1"
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "11bbfbde-cc58-4e3e-9bae-a01390e1ed51",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "These books sound pretty magical. I might learn something here.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.invokeScript",
                                                    "id": "b548e677-dc7d-4b80-9755-8e55141d77eb",
                                                    "scriptId": "e49ddd5a-2b32-4b92-bb4b-9e417cc9a181",
                                                    "entryPointId": "bb81d00a-9368-4baa-86e9-af700f23d888",
                                                    "currentScope": "Actors",
                                                    "currentSceneId": "981ec865-3e1b-4f3e-bb0d-11df2bc7d593",
                                                    "currentActorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "currentStoryEventId": null,
                                                    "currentPropId": null
                                                },
                                                {
