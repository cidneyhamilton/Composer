                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Hold your tongue, peasant!",
                                                    "emotion": "3",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
