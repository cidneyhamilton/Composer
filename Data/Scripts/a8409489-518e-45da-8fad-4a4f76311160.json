                                                    "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Of course not! It's having to listen to your idiotic drivel daily that annoys me.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
