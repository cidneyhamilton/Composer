                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Friends? You and I? Don't make me laugh.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
