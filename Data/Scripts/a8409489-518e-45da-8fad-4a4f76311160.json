                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "That's better, sir. At least you are smiling now.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
