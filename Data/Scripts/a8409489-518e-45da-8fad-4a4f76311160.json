                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "I'm sorry that you feel that way, sir. I sincerely hope that we can be friends one day.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
