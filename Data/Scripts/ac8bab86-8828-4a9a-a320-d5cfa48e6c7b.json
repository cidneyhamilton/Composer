                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "actorId2": null,
                    "text": "Go, me hearties! Show 'em who be the real Scourge of the Seas!",
                    "emotion": "3",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
