                    "notify": true
                },
                {
                    "type": "nodes.speak",
                    "id": "dfd5931b-8da0-4f59-b861-0d0912f595b2",
                    "actorId": "59d4db68-0e1f-4cca-8118-eb49edcf3859",
                    "actorId2": null,
                    "text": "We have come to the last session of <i>Magic for Mundanes.</i> I do hope that you have found this class to be enlightening, entertaining, and useful.",
                    "emotion": "1",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.speak",
                    "id": "00eb2314-b3ab-4223-8dd9-24c68bc2d395",
                    "actorId": "59d4db68-0e1f-4cca-8118-eb49edcf3859",
                    "actorId2": null,
                    "text": "You have all exceeded my expectations with your obvious dedication to the learning of magic.",
                    "emotion": "1",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.or",
                                "left": {
                                    "type": "expressions.variableComparison",
