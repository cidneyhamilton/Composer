                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "9d928105-d165-411d-8dab-58b22724439d",
                            "text": "Change the Sign",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a5f13945-ceed-47b7-a5e1-67a0b7c8e4f4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "That'd be a silly risk. It's not as if anyone ever willingly comes to see him anyway. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
