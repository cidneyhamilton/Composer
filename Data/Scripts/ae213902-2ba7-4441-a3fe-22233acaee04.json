                                        },
                                        {
                                            "type": "options.text",
                                            "id": "df530511-af41-4d99-b2c7-d2c11374ebb5",
                                            "text": "Make a Snarky Comment",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
