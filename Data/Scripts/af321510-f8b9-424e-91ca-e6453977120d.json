                {
                    "type": "nodes.changeTags",
                    "id": "aa5b4552-06de-470f-b4de-ded174edfdc5",
                    "scope": "actor",
                    "scopeId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                    "scopeFilterId": null,
                    "tagsToAdd": "KwirkDratMeeting",
                    "tagsToRemove": null
                },
                {
                    "type": "nodes.speak",
                    "id": "0b29edc4-b758-4862-b296-958b54a2a18c",
                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "It's terrible! We Kwirks are trapped by nasty Drats. Ya gotta stop them, Boss.",
                    "emotion": "4",
                    "emotion2": "4",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.quests",
                    "id": "0516783f-374b-44fe-b8da-88d1e088e4c1",
                    "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                    "propId": "afe1180c-3a68-447f-a74f-b0a0bcf007a3",
                    "target": "1",
