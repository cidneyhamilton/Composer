                {
                    "type": "nodes.wait",
                    "waitForSeconds": 1
                },
                {
                    "type": "nodes.showMenu",
                    "id": "47698a41-426f-4b99-983b-588cbd23f704",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "d53c283f-6bc8-4c01-827c-2031a41db306",
                            "text": "Greet the Kwirk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4c5cd78b-0996-46e4-b0b3-1c533dfc6a80",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                    "text": "Hi, nice night for surreptitious meetings, don't you think?",
                                    "emotion": "1",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "00d88403-570a-49c0-9b0f-12ad5b060762",
                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Nice night? Nice Night? How can anything be nice when any moment now we're all going to be eaten by Drats?",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "7e6231d6-0a26-43ec-8765-336ac99c9f86",
                            "text": "Talk About the Kwirk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c9fb79da-8cb8-4f49-ba9f-4a95a8bc60f7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                    "text": "I take it that things are not going well for you...",
                                    "emotion": "0",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "c0fccf6a-08a5-43b0-b5ef-b1c71a9bdd1a",
                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Oh, things are going just peachy keen - if you don't mind the fact that we're all gonna die!",
                                    "emotion": "3",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "8d9a3e09-0305-45e7-970f-4b664b06ac18",
                            "text": "Give a Gift to the Kwirk",
                            "nodes": [
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "92404209-88d9-4d4f-80a4-2a29570df550",
                                    "scriptId": "83634da0-18f4-4ba2-a9e0-52c2c6aafcb0",
                                    "entryPointId": "43d57ac8-5238-44ba-a5b6-049f5f525df1",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "96064b1c-7fee-41c6-ab46-b8425d3af2de",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                "scopeFilterId": null,
                                "tags": "hasKwirkGift",
                                "has": true
                            },
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "75eb8ebc-c1a0-4f79-af70-5bf944a684f1",
                            "text": "Talk About the Drats",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "2ca37aab-fff2-4878-a5a1-27a743633386",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                    "text": "So, the Drats are bothering you... so why come to me? Everyone around the school is going after Drats.",
                                    "emotion": "0",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e6342d7d-b9a6-49dd-a706-9bf32dab82f0",
                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Look, Boss, it's not enough just to kill a few here and there. The only way to stop them from coming here and eating all the Kwirks - and everything else - is to get the Queen Drat.",
                                    "emotion": "3",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "401b888a-b2b6-4642-888b-22821564c596",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "38423e86-fe58-409e-ac62-215f2cdd4298",
                                            "text": "Ask About the Queen Drat",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9bdf33e3-0818-468a-9f9b-561a2f85e998",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "text": "The Drats have a Queen?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a1a674ba-e8e8-4967-94ef-fd23078f36e8",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Yeah, if you think Drats are bad, you should see her. She's really big and really mean.",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4a5f908e-205b-4dd8-a5ea-66cfad3e7496",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Ya don't dare <i>borrow</i> anything from her area - she'll send an army of Drats after you.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "19fbdc20-6e44-4ac9-a488-d2f2d3862d15",
                                            "text": "Where is the Queen Drat?",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1b8af3d5-0279-4c16-bbcf-0e66b7352724",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "text": "So where is this Queen Drat? Maybe I can do something about her.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c59f6651-f8b7-4724-ad26-78c8d7fe774f",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Yes! I knew you were a good one, Boss. I knew you'd save the Kwirks!",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0f5da95e-408a-41b1-ab14-6e19c5c02f86",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Her lair is near the Wine Cellar, but you can't get there that way. The Drats have blocked off the door and the area around it with a bunch of junk.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f13b6f2b-a3b8-4751-937c-580fba4dc66f",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You'll have to come in the back way, through the torture chamber and the dungeon below. And that means going through the Creepy Guy's room.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "e01f35a2-ed34-4e06-af1f-a0ac29953972",
                                                    "scope": "script",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "MentionedCreepy",
                                                    "tagsToRemove": null
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "27f2c4bd-60bf-4687-9931-f13bf102d1d8",
                                            "text": "How Do I Get to the Creepy Guy?",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "279a5cfb-04fd-458c-8fa7-64af742cfc31",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "text": "The Creepy Guy? That sounds scary!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ddccb666-23dd-4a7e-84d8-fd0845e1fb42",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "He is scary, Boss! We Kwirks don't go near him. But you'll have to get past him and down through the dungeon to get to the Queen Drat's back door.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "571706cc-66ad-4e89-a9ec-664621934b3b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "text": "So where is this Creepy Guy?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3f3dcb83-64f5-4e5c-8822-3268030a3efb",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Yes! You can do this, Boss, I know you can! You get to the Creepy Guy's lair through the Librarian's lair. Just look for the creepiest painting there. Behind it, there's a way to Mr. Creepy himself.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "script",
                                                "scopeId": "96064b1c-7fee-41c6-ab46-b8425d3af2de",
                                                "scopeFilterId": null,
                                                "tags": "MentionedCreepy",
                                                "has": true
                                            },
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": false
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "f2fbb302-eb62-489a-8c23-05daa8ac1f19",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "be6f5b1f-78d1-4198-8aae-1ee9e73160f1",
                                            "text": "Leave It Up to Me.",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cedbbb19-5d86-4417-a8ad-7f207bf650c9",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "text": "I'll take care of the Drat Queen for you. It's the least I can do for making you lose your Finder status.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "51de06e3-d3ff-4165-b33b-e7d1c3f6252e",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "amount": "5",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c30d272e-a1ec-4420-b55a-80d826901c3f",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You'll be the Hero of the Kwirks for this, Boss. We'll tell stories about how brave you were to face death and scary stuff just to save the Kwirks. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "bb5fbbd9-d4f5-45bf-9208-de0f796e8cfb",
                                            "text": "Let Someone Else Handle It",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "99b9dd6e-14a6-44f5-8e07-11aa5a458f4a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "text": "Sorry, but killing drats isn't in my job description. I'll pass this info along, but I'm not going up against a huge monster Drat for the sake of a few Kwirks.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "18e46141-7ba6-461e-9072-a2e656da9790",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "amount": "2",
                                                    "change": "0",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c95aaa8e-0290-49da-b6b2-afd10ebcfc58",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Boss, you gotta save us. Think of the baby Kwirks! They are depending on you!",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "08324bfb-b6e7-433d-900f-af72aa5f07f2",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "text": "Look, it's not...",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c05b9558-b71f-4657-acb3-af7f388172cc",
                                                    "actorId": "d790a30c-ba0a-45af-94dc-c5066fcb20a4",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I knew I could count on you! You're the best human I've ever met!",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": 0,
                                            "ExitMenu": false
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                },
                                {
                                    "type": "nodes.comment",
                                    "message": "We should update the journal here.",
                                    "commentor": "Cidney"
                                },
                                {
                                    "type": "nodes.addJournalEntry",
                                    "id": "a9ad90bb-9da9-410a-b1de-24da3020edbe",
                                    "name": "Drat the Drats!",
