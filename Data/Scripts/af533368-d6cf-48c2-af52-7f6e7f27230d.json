{
    "type": "script",
    "id": "af533368-d6cf-48c2-af52-7f6e7f27230d",
    "name": "Esme RR14_DratAttack",
    "sceneId": "84006d08-8e6d-4558-b9b5-101f3c3054fe",
    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "7dc9d35a-376c-4336-a0b0-44893c635a9c",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "2e86f307-067d-48ac-b553-8701f07ea725",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "e79cd8e2-8d43-421c-b4ad-46b024618368",
                            "text": "Talk About the Drat Invasion",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "scopeFilterId": null,
                                                    "tags": "EsmeProach",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "scopeFilterId": null,
                                                    "tags": "QueenConversation",
                                                    "has": true
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "db209501-48ca-4036-a87f-0145db1f21d6",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "Have you had any luck finding the Warrior Drats?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cfccb1b5-6a03-48a3-ad69-089d23848fb7",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I am still hunting. There is strong smell in Underground Passage near the ladder beside door that does not open. Maybe the area on the other side of door will lead to Varrior Drats.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "scopeFilterId": null,
                                                    "tags": "EsmeProach",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "scopeFilterId": null,
                                                    "tags": "QueenConversation",
                                                    "has": false
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2dcc5454-1c17-4fb0-bdf6-532505525dcc",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "The Warrior Drats are obviously a different kind of Drat, right? And I know this may sound strange, but I think they don't even <i>smell</i> the same.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7193cc0c-0380-4b66-8797-be51c4079ee0",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "The Varrior Drats are meaner than the Vine Cellar Drats... and, to me, they smell a little like the blocked off room at the back of the Proach Room.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "702db1cd-4da0-44a6-aa16-214b9f264dd4",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I am sure that Varrior Drats come from there, but they don't go through Vine Cellar. There must be another vay into that area.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "scopeFilterId": null,
                                                "tags": "EsmeProach",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ea31294c-c2e6-4211-8fe4-c9ca5e8b0fba",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "What, let a slavering horde of vicious monsters keep me away from seeing my favorite people? Some hero I would be.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d823b629-4a91-469a-8f26-f7e6adbbb092",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Are you hero? Is hard to tell at times.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fb6a25f0-4b10-4b28-8863-8ad7e2201b66",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "I'd like to be your hero any day.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "5d4be6a7-c4ac-4c58-9f46-6688f8f08eb3",
                            "text": "Ask About the Drats",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "scopeFilterId": null,
                                                    "tags": "EsmeProach",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "scopeFilterId": null,
                                                    "tags": "QueenConversation",
                                                    "has": false
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3dd1c32a-7a59-40a6-8a4e-8d8186f66170",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "Do you think the Proach had something to do with the Varrior er... Warrior Drats?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8a205bf7-a5f4-40be-aeff-9359284ca43c",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Is strange... I vent down to Vine Cellar and sniffed around Proach Room. Many Varrior Drats had been all over Proach Room, but not one Varrior Drat stayed there. Is like they looked all over Vine Cellar, but did not find it.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bd96c541-67ea-459b-8ce0-b13fdd916e5f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "So... what? They are looking for the Proach?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5f684ae1-b4f3-49b0-a386-345db4e5ad3d",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "No, no... Varrior Drats looked in barrels and shelves... all sorts of small places. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8f6e3316-df28-4f6f-95f0-b11f089bde57",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I noticed the smell of the Varrior Drats in the cellar a few days before they came to the castle. They have not been back down there since.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "scopeFilterId": null,
                                                "tags": "EsmeProach",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0911b456-e0f0-46c6-94c3-bfaabad58436",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "Have you had any luck finding the dungeon?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e7402711-0e79-4494-8f0d-553bca173422",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I am thinking that door in the Underground Corridor connects to dungeon, but I cannot open it. There must be another vay.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "385ddcd9-6408-4c55-a537-0e7b0d2945cd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "So where are all these Drats coming from? They seem to be everywhere in the castle.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1577a9e4-4e3b-4617-a57a-6a7f40e82a92",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "No one has seen them outside. They must be coming from inside castle... maybe from dungeons underneath. ",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7bcb423d-5491-4b08-8bf6-313a1fba3a8e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "What do you know about the dungeon?",
                                                    "emotion": "4",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1e1eeed8-d346-44ad-b0af-3ee72b813fe8",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Me? Nothing. I just know Thomas spoke of them. ",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                "scopeFilterId": null,
                                "tags": "QueenConversation",
                                "has": false
                            },
                            "ignoreChildAvailability": false,
                            "minutes": "6",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e4749f4a-96dd-47e8-b5b5-a320f59cb910",
                            "text": "Flirt With Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "9faddafc-f7b4-4940-a554-09bdd62e89aa",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Why don't you and I take a little stroll around the courtyard in the moonlight? It'll be romantic.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "6b872ff7-da74-4d3a-af32-f1984bf99259",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Oh yes, ve could romantically run avay from Varrior Drats hand in hand. Or better yet, ve could lovingly slaughter them together. ",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "2ca6511c-bddd-458f-80d4-7388dae42f15",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Maybe that's your idea of romance. It is not mine.",
                                    "emotion": "1",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                "scopeFilterId": null,
                                                "tags": "EsmeDatesAeolus",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fa76d903-ea26-483c-836a-3e5ec90ab691",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Besides, I am dating Aeolus. ",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "9077e3e9-2c11-4df8-b080-000d0d5b04ea",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "87de9232-7c04-4546-b19b-a1cc5f5c803d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Well, I guess I'll say good night.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "dd6f780c-c088-4dff-a097-e2fdc3ae8851",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Good night, Shawn. Don't let Varrior Drats bite!",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
}