        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "3539bfe0-4b1a-4db0-aa65-29d812c99f48",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "b6f61725-627c-4ae8-b5b1-5b350cb4c756",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "e48bc385-8a41-426f-8c9e-8c123839d859",
                            "text": "Look at the Banner",
                            "nodes": [
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "7c707569-ba96-4068-88d2-43fe18104e07",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "7ba57292-268b-4c3d-a00b-4101d24e7ebf",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Okay, I feel just a <i>little</i> more rabid about the school than I did a moment ago. Go team!",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "1ad3c689-d1c7-4315-915f-f2b804ab8c33",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "YES! Hero University is the <i>greatest</i> school in the world! Rah rah!",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "2b0ec95d-3af4-4c0a-bfde-dff5d32f875d",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Give me an H! Give me an E! Give me an R! Give me an O! ",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "7eaf2606-63dc-4961-8941-4353ac4b8ca0",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Then give me a U! Give me an N! Give me an I! Give me a V! Give me an E! Give me...",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "cc8d09d6-3ddd-4638-80b7-1b9e66aa83bd",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Wait a minute, where am I? I lost my place.",
                                                            "emotion": "2",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "7fa587fb-0f22-4055-8d85-094760c14207",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Just looking at this, my heart is totally swollen with pride! Unless it's worms, it could also be worms.",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "9ff4c21e-e2a5-4186-ba49-d81e46a8825d",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "It makes me feel honored to be part of this great institution!",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "45de8ec6-1661-460d-8ce4-160a20499d9d",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "If I wasn't already a student, this banner would convince me to become one! If they accepted me. And if I could afford it. And... never mind. ",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "029f95d6-ff9a-46b6-ab8c-29ff4a6927c1",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Seeing this stirs me so deeply... I feel a school anthem coming on!",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "a1cff853-311d-47ca-8f71-56e513c451b0",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "\"Hail to thee, Little Warriors Nursery School, by the shores of the Deadly Swamp...\"",
                                                            "emotion": "4",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "03690ad8-0ac1-4178-a19d-d700a08de4c7",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "text": "Well, it's the only school anthem I know!",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "6878620f-ee71-4f62-aeae-acc3bab836bf",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Next time I need some school spirit, I know what to look at!",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "baaf5fe0-1cb5-4328-9b22-70e65d2c943f",
                            "text": "Steal the Banner",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "e3d4756f-5c58-4b6d-ac33-c6a2a7ff88d2",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The only person who'd buy such a thing from me is somebody from the school. And if they're from the school, they wouldn't have to be a catapult scientist to figure out I stole it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true
                }
            ]
        }
    ]
