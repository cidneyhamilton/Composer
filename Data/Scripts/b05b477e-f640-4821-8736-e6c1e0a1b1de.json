                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Good day and good hunting to Master Shawn! Gregor hopes that the Houdini 42 and the Trap Tapper will be good for the Young Master.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
