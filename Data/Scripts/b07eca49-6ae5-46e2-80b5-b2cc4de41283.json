    "actorId": null,
    "trigger": {
        "type": "triggers.interact",
        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "12619d6b-8d9c-4656-881b-0c008ab86212",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "434ed6fb-b352-4ea7-b881-f8c1839ac5fc",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "14db31cc-5820-4cff-ab2a-e62ba32ac513",
                            "text": "Look at the Bookshelves",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "923b78ae-63da-4dee-8fcf-b059c8601bb3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "As expected, they're full of books.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "543459b6-32d2-4e10-8036-e67442dd713f",
                            "text": "Read a Book",
                            "nodes": [
                                {
                                    "type": "nodes.playAnimation",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "animationName": "ReachOut"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3010d249-b73b-46c6-b99d-6a63617607a7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I can't believe how dry most of these books sound. Look at this one...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.playSoundEffect",
                                    "soundEffectName": "book_page_turn.wav",
                                    "wait": false
                                },
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "606320aa-c810-40ef-abe9-5d966abe1a9f",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "a96b5474-2dbb-4787-8613-90c61278fe09",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>5,000 Shades of Grey, Typed & Classified</i>. I mean, at least it has pictures... sort of.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "5876fa00-fb0c-42eb-a618-b3de52504773",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>Thrilling Tales of Botany and Botanists</i>. I think there's at least a couple of oxymorons hidden in that title somewhere.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "00b50f0d-7557-4f9c-9e09-361f74d466ec",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>How to Spot Extinct Animals</i>. All right, WHAT?",
                                            "emotion": "4",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "36aa096d-5142-42eb-918c-933fc412c3b2",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>The Compleat Formulary of Polishes, Finishes, and Cleaners.</i> They lost me at \"Compleat.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "4c028b80-ec05-43a7-a019-d45588576a26",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>The History of the Cheswicks: Twelve Generations of Lamplighters.</i> How much of a market did they think there'd be for this book?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "c621e79f-dd64-44e6-8315-fee78783bd6e",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>You, Too, Can Squash Bugs!</i> I need a 600-page book to tell me that?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "f50997e4-118e-4dec-ad44-3c196fb62ede",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>The Seven Habits of Highly Effective Nuns.</i> What in the world is an effective nun?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "bb036e09-8a8b-4554-8e69-f73d01f7c40a",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "<i>So You've Been Buried Alive!</i> What is this doing here? Why not put this book in a casket, where it might do somebody some good, along with a nice tiny torch?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "3d2eb6bc-f3ed-44cc-9197-152b8f27afe5",
                            "text": "Steal a Book",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "fb1563f9-433b-4ca9-af03-59a4c22634d6",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Life is too short and uncertain to spend time reading a dull book.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
