    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "26766f83-d875-4b32-9d4f-ffed0a5100e4",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "1b4ce28d-c75d-49cc-94a6-2d494d8f31b5",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "6f2c9179-0066-4d16-97f3-5fec0b490a0b",
                            "text": "Look at the Bed",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "31b2a9ae-5d62-49e0-9994-042afc03c52d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "It's the most interesting bed I've ever seen.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7eef34a2-f821-43f2-9011-4a311653cd1c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The pillow looks - and smells - as if it was stuffed with a half-dozen dead geese.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a4d628a7-cbef-45a7-bfc4-f48cf455d4a7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The headboard and footboard look capable of impaling a platoon of Gogs.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "384e3202-5332-4433-83f6-a234105c4164",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "And that sheet appears to be about 15 thread count!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "63d0099d-e20b-4a78-8590-622f58a46da0",
                            "text": "Lie in the Bed",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "fdfbb8d0-8a25-4bb1-b66e-8090f69d2b3e",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It smells more than a little funky.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0b5149fb-0e7e-4040-b4ee-c643a9ddaee1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "And when someone like <i>me</i> notices it, you know it <i>has</i> to be pretty bad!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
