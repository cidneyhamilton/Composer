                    "options": [
                        {
                            "type": "options.text",
                            "id": "c7d5c14f-2afb-4ee2-89ca-16eb7b7adb00",
                            "text": "Ask About Sosi's Mood",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                "scopeFilterId": null,
                                                "tags": "JSM1",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "82d7ea31-ee55-4791-97d4-aca334c1255d",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "JSM1",
                                                    "tagsToRemove": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "72e05870-2750-4b3b-a866-7692d7d6d713",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Sosi's in a bad mood? How can you tell? ",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "105e70f6-069b-4318-969a-cf4df81702e1",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "amount": "1",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8d17b6ad-736e-4ab5-a31f-3f3348d4b6c2",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "He's always in a bad mood around me. Must be my winning personality.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fa272775-0fb4-463e-a2cb-de59545269c6",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Caesari and I are dorm mates. He can be most unpleasant when he is displeased.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4de0818c-876c-4e70-8b22-f1861e3455de",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Sosi can be most unpleasant when he is pleased, too. That's part of his charm.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "adda9b6a-9d22-49cd-8606-70201fdd2106",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "So how are you surviving Sosi's wrath?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5e60b8a8-7eed-40b4-9cc9-85e7591258e8",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Do not underestimate Caesari. He can be... vindictive.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "7a79229d-ed23-48f5-85bf-982e6299c793",
                            "text": "Ask About Sosi's Meeting",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "d853d836-1199-4cf5-83c0-5ec6586cbd32",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "What did Sosi have to say about his meeting with Esme?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e092e653-e382-4b02-a324-ed6abb9f1fa7",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Caesari will not speak of the matter, but he is very angry.",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4f4439b1-9b19-4bb9-a178-6d91bc96d61a",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "And perhaps, a little bit afraid.",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.addJournalEntry",
                                    "id": "fe1254cc-8c50-499b-8011-a75ce86d6741",
                                    "name": "Sosi Afraid of Esme?",
                                    "notify": true,
                                    "description": "Kyro says that Sosi is very angry about his meeting with Esme. Also - Sosi is a little afraid. Why would Sosi be afraid of Esme?"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8a2b6c81-ee65-48d2-9298-ed379e457809",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "Afraid? What would Sosi be afraid of?",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fc9f275c-b91a-4c73-8b7a-2a510954dfe2",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Of that, I do not know. I had hoped you had more information on the matter than I have. Perhaps if you speak with Esme, she will reveal what happened at that meeting.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "6",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "4eef1092-9603-4957-a474-28a13a3c1c03",
                            "text": "Ask About the Shop",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "382edb18-2bf1-4784-b8dd-44b1140f7fce",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "So what's new in your store today?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a90f7df9-79e6-4ab5-aa58-9ba0361bb025",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I have a very fine throwing dagger that I would be willing to sell to you. It is perfectly balanced and sharpened to a razor's edge. It could be most useful for you, don't you think?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "382fcdba-7081-4fbe-aaf3-9e72220c2107",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "2428c5a3-d935-4477-abdf-a302023381bc",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "See you around!",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "1178beb9-0354-4fdf-87e1-6875e92b9594",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Have a pleasant afternoon.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
