                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "97333279-bdf1-4526-b56b-821c11df521a",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "01069be8-06f3-4621-bb7c-49c3a0e0da50",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                    "text": "I'll see you tomorrow, Caesari. ",
                                    "emotion": "1",
                                    "emotion2": "3",
