                    "options": [
                        {
                            "type": "options.text",
                            "id": "75d4cd58-a67a-4559-b772-dc133555d766",
                            "text": "Respond to Sosi",
                            "nodes": [
                                {
                                    "type": "nodes.showMenu",
                                    "id": "d410cc31-4aa8-4ed8-aba5-70b61a0afb3c",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "1d7fabee-afaa-4d07-bc2f-c0ed61b21586",
                                            "text": "Say Something Conciliatory ",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fd28f74d-2f72-4020-98ce-48f8a1079ba8",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                    "text": "I'm sorry I annoy you so, sir. I know how galling it must be for you to be forced to act in class as if I were an equal. Clearly this entire school is beneath your station. Perhaps you should be the one to go to a better place.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "1ca8720d-9dfb-42f3-bee6-b09ab976206e",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "27",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "0ef0c043-b2fe-44a9-97f7-8dcc553645cc",
                                                                "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            },
                                                            {
                                                                "type": "nodes.speak",
                                                                "id": "2eb6168a-c799-42a8-8fcc-92c9de8b39cd",
                                                                "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                                "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                "text": "You are correct - this place disgusts me. Unfortunately, I must be here for the nonce. So the sooner you leave, the better this place will seem.",
                                                                "emotion": "0",
                                                                "emotion2": "1",
                                                                "device": "0",
                                                                "device2": "0"
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.speak",
                                                                "id": "e6007f4c-0707-4414-b3bb-a17eedca5bcf",
                                                                "actorId": "2ae7a408-9338-4b78-af6b-ba7a28e050ac",
                                                                "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                "text": "Insolent clown! ",
                                                                "emotion": "3",
                                                                "emotion2": "1",
                                                                "device": "0",
                                                                "device2": "0"
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "4",
                                            "ExitMenu": false
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "517e962d-b4dd-4ff4-bfb9-f1d655e5da2b",
                                            "text": "Say Something Clever",
                                            "nodes": [
                                                {
                                                    "id": "c8a9d114-3c3f-43fb-adfc-aae938761894",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "27",
                                                    "success": {
