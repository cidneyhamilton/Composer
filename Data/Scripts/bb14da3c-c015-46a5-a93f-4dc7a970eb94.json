                                    "wait": false
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "53aa8282-e017-42c2-bf74-6e1b442b009e",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "OW!",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "f972f30e-624f-4fdd-bb4e-a71c6a28c32b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Well, now I <i>know</i> I've hit rock bottom!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
