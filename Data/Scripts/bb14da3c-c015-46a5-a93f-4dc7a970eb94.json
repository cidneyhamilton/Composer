                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "16f12045-48f4-4037-93eb-8fd344102d52",
                            "text": "Slap the Statue",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "58e73da6-a81e-4fda-bc71-26448c5eb07b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I'll just smack him on the... uh... backside...",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "026359a7-dd01-439f-99c4-d38cabc30551",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "... I'm sure he won't mind.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.playSoundEffect",
