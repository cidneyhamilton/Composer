                        {
                            "type": "options.text",
                            "id": "d65957bc-62c9-4d13-80fc-f0cc4c341ef8",
                            "text": "Ask About Gregor",
                            "nodes": [
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "cdc2b096-9cc0-4654-a2a1-58da6a6c804d",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
