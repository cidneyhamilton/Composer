                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I don't want to take something I can't see.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
