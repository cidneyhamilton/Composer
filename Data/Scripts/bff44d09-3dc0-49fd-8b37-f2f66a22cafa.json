                        },
                        {
                            "type": "options.text",
                            "id": "2b3d1f08-a486-4631-b93e-a76ace63fef0",
                            "text": "Flirt With Esme",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "dd403f8d-640b-47be-8782-e661db270de1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I'd be happy to hold your hand while escorting you around the school to keep you warm and safe from the Banshees.",
