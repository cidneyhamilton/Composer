    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "67f3a14e-f188-4dcd-b738-43f30f1cafc0",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "1ad6d3aa-4d00-48bf-8b2d-4ec09329262b",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "b5b8b1c8-2407-4c89-b81a-f1caf63ecfd2",
                            "text": "Look at the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "63e089a1-fa90-4ced-ba20-7961504e50f5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "They're in better shape than most of the school desks. Then again, I guess none of the classes in the Library involve knives, chemicals, or kettlebells.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "1055a7a3-863d-4117-8510-0015d09040b4",
                            "text": "Sit at the Desk",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "18cf9ece-7df6-4d1f-ac33-b181d5619177",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I'm sure I'll be spending plenty of time behind these desks. Right now, I'm a man with a purpose.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.actorPresent",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007"
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "prop",
                                                    "scopeId": "2a2bb2c6-c7a0-426e-b70b-c5096b0479ec",
                                                    "scopeFilterId": "981ec865-3e1b-4f3e-bb0d-11df2bc7d593",
                                                    "tags": "SatOnce",
                                                    "has": false
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5a34afaa-7f53-4a02-ada8-9b54a7e22bed",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "We don't allow those in the library.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ae2f09d5-4594-43af-aacc-f740e2d3dee6",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "Wh-what?",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bedd8b3a-ad5a-47a5-8cd5-c3e3082c3a20",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Aquatic mammals of any kind. Not allowed.",
                                                    "emotion": "0",
                                                    "emotion2": "3",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5a8d813f-fcee-447f-b2f8-d5d1add114ea",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "OH!",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9239fa01-c219-48cb-b3f1-aef00d13b312",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "I said I'm here with a <i>purpose</i>. ",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5d2e0899-e30a-45db-b787-1ea7a828e273",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "And I was making a joke. You couldn't tell from my puckish demeanor?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f1e13462-8bf9-4d12-8760-df3fc79ca39f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "I'm... I'm just going to go back to my explorations, thank you.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "397f6e52-21c9-4793-b565-9bbaad391d78",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": null,
                                                    "text": "...",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
