                                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                    "text": "Right. Thank you.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
