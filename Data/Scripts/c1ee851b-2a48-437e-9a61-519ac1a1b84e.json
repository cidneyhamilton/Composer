                        },
                        {
                            "type": "options.text",
                            "id": "25e036c2-68bc-4037-8f52-2a2a199d97e3",
                            "text": "Ransack the Alcoves",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
