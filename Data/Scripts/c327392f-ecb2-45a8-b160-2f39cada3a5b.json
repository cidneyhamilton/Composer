                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "c4404102-151f-4079-8878-33a825fd9415",
                            "text": "Talk About the Final Exam",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "5a2dfe5d-0486-45cd-8409-369a5c49437f",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "Don't worry about the final, Esme. If Gerhard hasn't expelled us by now, he's not going to do it on the basis of one little test.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "ca9fc4bc-d57d-43f2-8ebf-63c8dd485c59",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You are most likely right, but I vant to do my best. I alvays vant to do my best.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
