                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "Are you?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8e3ff6e9-598f-4745-8904-7d51f56feb46",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "No. It's supposed ta be Proach Extract. ",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7a1ee25a-7d18-4daf-85d6-445f817767d0",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "What's <i>that</i> for?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f9f3e163-0ccb-4846-b539-aaaa5d4100f0",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I don't know! I just got it 'cuz I've never seen it before!",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "50e50346-8904-4d48-bffa-8ae41793350e",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "Looked",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
