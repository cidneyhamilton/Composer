                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "02e86a67-1256-48b5-82bb-50dc2e17431d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It's a black bottle. There's no label on it, so I have no idea what's in it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "08a5b1af-0c6d-42be-8509-6037083ac492",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "How do ya know <i>anything's</i> in it? Maybe I'm selling black bottles!",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
