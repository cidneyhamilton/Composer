                        {
                            "type": "options.text",
                            "id": "6632a7f4-f298-49f4-a0cd-a61411674a2f",
                            "text": "Pick a Book at Random",
                            "nodes": [
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "9ac2f7d7-1de7-4817-be4b-dbd1105cd615",
                                    "nodes": [
                                        {
