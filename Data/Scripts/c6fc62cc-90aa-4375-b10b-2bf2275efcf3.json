                            "id": "52ef5833-9062-40fe-8625-bef6c9f969f7",
                            "text": "Look at the Painting",
                            "nodes": [
                                {
                                    "type": "nodes.showCloseUp",
                                    "imageName": "painting_eranaPeace_640px.jpg"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b0f9d6c1-d4f2-4c7c-a533-d3dc64813a19",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The plaque under the painting says, \"Erana's Meadow.\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "5af886c0-a48c-4f5f-ba12-4075f32b3700",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I don't know who Erana was, but she had a beautiful meadow.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
