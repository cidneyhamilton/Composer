                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "7b5586d7-1052-4589-8ab2-fa88629c2e33",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "f5ca7ada-9a18-439e-a272-f8651968a106",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "text": "We can talk more later.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "dac0e4a0-9c9c-4010-9fc7-7e08e1900671",
                                    "actorId": "0b49e2c8-5b7f-4534-bd10-21a0a36a6cbe",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "We can do that. Thanks, Shawn, for being a friend.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
