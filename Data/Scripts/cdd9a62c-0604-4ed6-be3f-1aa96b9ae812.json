                                                                "operator": "eq",
                                                                "compareTo": "true",
                                                                "sceneId": null,
                                                                "propId": null
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.nodeCycle",
                                                                    "id": "8beed94e-5ffc-4897-b1fa-fdd5ad6a5bc7",
                                                                    "nodes": [
                                                                        {
                                                                            "type": "nodes.speak",
                                                                            "id": "13bfd8a8-17a8-47a9-a1a1-d5e1f38ee3eb",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "The book recommends that I use some sort of magical barrier like a rune to protect against the backlash of destroying a phylactery.",
                                                                            "emotion": "0",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
                                                                        },
