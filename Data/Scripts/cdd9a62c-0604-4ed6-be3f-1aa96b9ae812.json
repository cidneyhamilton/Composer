                                                                },
                                                                {
                                                                    "id": "9fb04c94-48c5-4d34-b2ea-d7dcfaaa666e",
                                                                    "type": "nodes.improveSkill",
                                                                    "skill": "Combat",
                                                                    "amount": "1"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "ef349b16-67fa-42a1-8bf3-40f68aeaf727",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "I really enjoyed the <i>stabs</i> chapter, but I wish the <i>jabs</i> chapter had been about quick-witted insults and banter instead of boring old punches.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
