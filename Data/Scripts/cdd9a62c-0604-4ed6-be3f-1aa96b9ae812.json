                                                                            "id": "7e3af9fa-efea-4f6d-b93f-70dba3baaeb4",
                                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                            "actorId2": null,
                                                                            "text": "I'll need a magical weapon and all the magical defenses I can get if I want to defeat the living dead.",
                                                                            "emotion": "0",
                                                                            "emotion2": "0",
                                                                            "device": "0",
                                                                            "device2": "0"
