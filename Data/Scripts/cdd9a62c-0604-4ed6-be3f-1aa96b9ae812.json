                        {
                            "type": "options.text",
                            "id": "7c5a019c-d7db-49ed-a903-ca5bc42ef7ac",
                            "text": "Read \"Defense Against the Undead\"",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
