                        {
                            "type": "options.text",
                            "id": "e17a2a72-6bbd-40ae-bac8-59c11622c73b",
                            "text": "Take the Shield",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "98431d33-ce47-43f5-bf48-e4fff2dbd767",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It's too bulky for me. I can't imagine having to lug that thing around everywhere. Besides, it would probably break if a Drat bit it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
