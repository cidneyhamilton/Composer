                            "text": "Look at the Grate",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b47bc6ed-1431-4ef8-a23d-845435d0c95a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Someone... or some<i>thing</i>... managed to bend these bars.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
