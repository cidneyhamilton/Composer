                        {
                            "type": "options.text",
                            "id": "fc24eac7-e7db-44e8-bdfe-063af7f5ff0d",
                            "text": "Climb the Fireplace",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "ff935874-a4ae-4ab4-a24f-9da54d1bd426",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Maybe someday the Thieves' Guild will teach me how to climb fireplaces and stuff. For now, I'll keep my feet on the ground.",
                                    "emotion": "0",
                                    "device": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e711b317-ded0-462e-8fd2-c65a98c9a873",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Besides, the stones on the mantelpiece feel a little loose.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
