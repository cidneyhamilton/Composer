                            "id": "88f03900-3c19-4d65-97dd-46da5f14686f",
                            "text": "Put Out the Fire",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "cd5bfc47-207f-4a71-8054-f92047e12be8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Oh, sure. I should let everyone know that someone has been in here. Besides, I need the light.",
                                    "emotion": "0",
                                    "device": "0"
                                }
                            ],
