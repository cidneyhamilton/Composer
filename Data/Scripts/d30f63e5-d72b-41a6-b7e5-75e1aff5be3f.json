                            "text": "Examine the Fireplace",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "afb4beb7-09a8-4e50-8ede-39a58b3d3b26",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "There's a lovely warm fire in the fireplace and an intriguing mantel above it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
