                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "803f8f7a-469f-4316-a311-b021895ada1c",
                            "text": "Search the Plaque",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4809a8d3-3d13-4381-90c6-02a3f0b98708",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It's solid and it all seems to be carved from one piece of stone.",
