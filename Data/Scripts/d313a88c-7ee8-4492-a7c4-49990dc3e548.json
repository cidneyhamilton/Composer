                                                    "id": "172dece9-1c91-48e2-b626-444cbcc620bd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Hmmm... there's a book that's fallen down between the plaque pedestal and the wall...",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
