    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "8496874f-6b04-4c78-8278-3ec2c95e827e",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "f45adbc3-e8a5-4949-8a45-3b727727620f",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "4206f2d6-1323-42b3-94cb-70f555217f29",
                            "text": "Look at the Barrels",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "60794972-b8d5-4e4d-aa2f-2e67daf0d6c9",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The one thing they probably <i>didn't</i> use these barrels for is <i>wine.</i>",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "7b702a3d-7534-4cbe-80cc-0f411fb72e38",
                            "text": "Look Inside the Barrel",
                            "nodes": [
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "0832aa57-9fbb-4cf5-a4e9-b57f2b32f052",
                                    "nodes": [
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "5342ef6a-4df0-4777-8ff3-f21116436e51",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "The lid is loose... let me see...",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "f4b2dfc6-9dec-4e98-8bd8-56507f524f43",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "It's full of <i>salt!</i>",
                                                            "emotion": "4",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "b78bb04d-fe27-4d87-8b27-2ab8f51277e1",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "Oh, to rub on the wounds, of course!",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                },
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": []
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "09a0ce5d-bbd6-43f8-9f21-59af86e3517c",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "This one's got a lot of shriveled little graying... hmm... I think they were slugs. And they open up a whole new world of aroma! ",
                                            "emotion": "2",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
                                                    "type": "nodes.branchSection",
                                                    "expression": null,
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "4a5be932-7747-4b7c-ac21-4e49e90fd9cd",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "This one is empty... but it's stained all red inside.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "bb03cea1-6d6b-42ec-b0fe-f43b97aa2a50",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "Let's just say that was wine.",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "d73a6598-f53b-46d3-a9fb-df1772aee7fa",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "It's empty, but it smells powerfully of rodent. They couldn't have kept <i>those</i> in barrels, could they? They'd chew their way out in no time.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "a5057d77-640d-4b35-b2b0-1a502ab47cd9",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Oh, this barrel's full of big flat stones, for pressing. You've gotta love the classics.",
                                            "emotion": "1",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "03d10d2e-9b92-49f8-afe5-20feb1c6eb0a",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Let's see... Huh! Why would anyone need so many thumbscrews? Are they really easy to lose?",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "f342d770-6f91-44aa-89cc-3f4d9499d321",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "I've already looked in all of the barrels. Darn it, I was enjoying the smorgasbord of torment.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
