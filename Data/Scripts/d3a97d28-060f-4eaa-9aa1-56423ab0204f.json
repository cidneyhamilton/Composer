{
    "type": "script",
    "id": "d3a97d28-060f-4eaa-9aa1-56423ab0204f",
    "name": "Bedroom 11: Aeolus loves Moira",
    "storyEventId": "21dd9f5b-dd40-42e8-9d7e-4027f3f4bc34",
    "sceneId": "9897ecb3-4200-4884-867a-2840471747b8",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "7fba5d49-97b3-4a8c-b967-6a427aa69311",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.changeTags",
                    "id": "24073a33-14d6-48a6-b4ac-4833cba13890",
                    "scope": "scene",
                    "scopeId": "9897ecb3-4200-4884-867a-2840471747b8",
                    "scopeFilterId": null,
                    "tagsToAdd": "HadMoiraTalk",
                    "tagsToRemove": null
                },
                {
                    "type": "nodes.showMenu",
                    "id": "d621b8a8-c1ee-41dc-a8bd-b77137ad5328",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "54c8c982-7419-43ce-bc05-b7817c4ad05f",
                            "text": "What's New?",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "961c66c9-bebe-47e6-9df1-daab2903aa04",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "You seem chipper today.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "aaabc002-24ce-4b04-a321-c4a1b5135376",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "487c4c43-ecc3-469c-a217-8ec7434dba67",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Yes! I got hit in the head in Self-Defense class and it's the happiest day of my life!",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4d6ab038-23ab-4dd2-a70b-cbf78a6338c3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "Okaaaay...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "553b4881-a870-4f20-940b-13e448ac06aa",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I went down to the Infirmary and there I met her - Lady Moira Glenshannon, the Maiden of Mercy and Medicine. She healed my wound and won my heart at the same time.",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "44105be0-9242-4c1d-a3c3-8209f3879404",
                            "text": "Ask About Moira",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "73b4526c-ef90-4c2f-8a45-82c70d7f00ab",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "So how did you fall for Moira?",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "ffe04f83-cc20-4f47-8b3c-ee34d42717b9",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Moira is the kindest person I've ever met. She soothed my pain with her touch, then she wiped the blood from my face. I looked into her eyes and they were like emerald pools of love.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "942f84a8-d123-4f98-b761-496dd1ad4eba",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I just had to write this song - ",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a38adea2-b713-45ea-aec2-2943beb33655",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": null,
                                    "text": "Moira - earlier my life was filled with grief.\nMoira - you looked at me and filled me with relief.\n ",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a07f3b20-d96e-49b6-b1d3-9b227a820a5f",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "Now the pain is all gone and the pleasure is nigh,\nMoira Glenshannon - I'll tell you why -\nMoira, it's quite true - I love you.",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a1160bff-67fb-4004-90f5-3aae3d67119d",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Shawn, would you please visit the infirmary and talk to her about me? She just thought of me as a patient today. I want her to think of me as her boyfriend. Please speak with her and mention how beautiful and kind she is... tell her that I love her... Would you do that for me?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "dc8c8041-c57e-40f6-bdfd-4c5a5acee94d",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "86041304-a5bd-4bb2-a1c5-dc5bb18cc9aa",
                                            "text": "Agree to Speak to Moira",
                                            "nodes": [
                                                {
                                                    "id": "c5d9ef37-c551-4cfe-9944-c93407e4bf51",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "30",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b611ded1-a447-45ed-b864-b960055c4561",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "Sure, I could do that for you.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "536240fc-d33a-4c19-a808-fa56b065a549",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "amount": "5",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.quests",
                                                    "id": "6b311f80-a718-4633-aec1-68e7da1913c3",
                                                    "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                                    "propId": "d9b131eb-091d-45ad-ba39-ed93bda720a9",
                                                    "target": "0",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "51369da3-a646-43c1-8932-4e4b068d5321",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Oh, thank you, thank you, thank you! This means so much to me.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.addJournalEntry",
                                                    "id": "bda8a082-d455-4544-8c16-5f939bc69802",
                                                    "name": "Aeolus loves Moira",
                                                    "notify": false,
                                                    "description": "So Aeolus has fallen in love again and wants me to talk to Moira about him. Well, it's the least I could do for a friend..."
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "1",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "d0ded568-071e-42ef-aadd-1be958af17fc",
                                            "text": "Refuse to Speak to Moira",
                                            "nodes": [
                                                {
                                                    "id": "f9a66dfb-4504-4508-9d66-49ad47365b05",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "30",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ed6dfdd9-b975-4053-bce8-afee72d91bb7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "Look, Aeolus, you are the only one who needs to speak to Lady Moira. You've got to talk to her and tell her how you love her. Sing her your new song. That's the only way to make things work out.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2bbafd51-1c50-4a15-a3f8-3fc54deafe99",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "But what if she doesn't like my song? What if she thinks I'm silly?",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "b26c3514-07dc-406e-b521-5dee37f7cfdb",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "036ab4d7-b89c-4768-90f4-c3cf69a3ae3d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "text": "You are a bard. You'll do fine. ",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "44bbd950-b7d3-4839-b2b8-8cbceb63d54d",
                                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                                    "actorId2": null,
                                                    "text": "<sigh>",
                                                    "emotion": "2",
                                                    "device": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "8",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "3d4973f7-fd47-4cb9-9e8c-3cce1b2d27e7",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "305d1457-2831-4c13-add7-790abe5a956c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "text": "Catch you later, alligator.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7db1fcee-efaf-48c1-9636-68ac91959668",
                                    "actorId": "0661a2f7-5493-46d9-ab58-6e9bff9a5f30",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "In a while, crocodile.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
}