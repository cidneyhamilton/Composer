    "name": "Interact with Chen Sun Portrait",
    "sceneId": "298ff758-e1ce-477a-b9d4-da614b74072d",
    "propId": "e4577e8c-a401-443f-abe2-d545ffac5b6d",
    "actorId": null,
    "trigger": {
        "type": "triggers.interact",
        "name": "Interact"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "c4efbbac-b748-467f-9a4e-9d4a674b68b9",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "e5b43d46-2e23-4d7d-a2dc-fc00e6fa2394",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "33e1e3ab-fad4-4ce0-be76-5c6cf173a3be",
                            "text": "Look at the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.showCloseUp",
                                                    "imageName": "Prop_ChenSunPainting.png"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "94789fe8-bb8f-4a34-b131-64c2b3be060e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "This says that it is the \"Portrait of Chen Sun.\"",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "64f7b085-83cb-459a-bf9c-edf438d0001d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "A one-on-one fight with a cheetaur with some sort of polearm? That's not fair! That cat critter looks like it will shred poor Chen alive.",
                                                    "emotion": "3",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.hideCloseUp",
                                                    "waitForSeconds": 1
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "69040033-5951-4763-83b2-87cf1a68ed69",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can vaguely see what might be some sort of monster, and there's someone in the back. I think.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "95a4e6d5-6f01-4c31-bac3-10f656bca6bb",
                            "text": "Read the Plaque",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "32b456fe-0d7a-4e0f-93d6-0780d8ae302f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The plaque says, \"An Adventurer's Guild Exchange Student to the valley of Spielburg, Chen Sun spent a significant portion of his youth dedicated to thinning the local monster population - especially the ones carrying coins.\"",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8458bd15-8714-4f04-912c-9f0bcf536939",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "\"Renowned for his regular nocturnal forays to hunt cheetaur, it was not uncommon for incredulous early rising residents of Spielburg to see him struggling back from the forest just before dawn, overladen with loot and valuable cheetaur claws.\"",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bbe7ae03-3ac2-4302-a9c4-d01cccc72f06",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "\"Chen also spent time in preliberated Raseir, which almost ended in false imprisonment and loss of his riches. After his escape, Chen donated a significant portion of his fortune towards the establishment of Hero University, in the hopes that other young adventurers would have the opportunity to be able to form their own memorable experiences on the path towards becoming a hero.\"",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "30751aef-5806-4232-b79c-823b048a69b3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I hate the guys who can just go out into the forest and saunter home carrying treasures and huge dead beasts. I so want to be like them.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f5cdc038-7d84-47a3-a587-4a3965fa318f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Not a chance. Not in the dark like this.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "8b1fb184-a78b-4a41-89a7-c7dc70f59e53",
                            "text": "Look Behind the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.invokeScript",
                                                    "id": "bee492e4-8f7a-472f-b911-d997e6dea027",
                                                    "scriptId": "b034c7b9-29f5-46ab-b66a-3a39f340b33b",
                                                    "entryPointId": "c4efbbac-b748-467f-9a4e-9d4a674b68b9",
                                                    "currentScope": "Scenes",
                                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                                    "currentActorId": null,
                                                    "currentStoryEventId": null,
                                                    "currentPropId": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "83c8d394-ad2d-4ee2-8e8f-1801809678fb",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "There's nothing back there except very slightly cleaner stone.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "774439b7-e594-4cb3-9bb5-8f92aec1f82b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I can hardly see the portrait, let alone what's behind it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "7180dcbd-4c21-4fbe-8c50-401a052be7d1",
                            "text": "Take the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0d2a5e1a-7285-4d07-88ce-ade0123cd87e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Too ostentatious for my dorm room.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "94a358d8-d902-4b20-84b0-36350abc94b3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I wouldn't consider stealing it until I've had a good look at it. It might be a jester. Jesters scare the crap out of me.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "60adc630-a5d7-457c-8300-7603425b214e",
                            "text": "Sense Magic on the Portrait",
                            "nodes": [
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "2ad14bdd-5d77-4082-82f4-8c5801e4bd70",
                                    "scriptId": "533e1ce1-13d3-439f-8544-dbdf808d30c4",
                                    "entryPointId": "c4efbbac-b748-467f-9a4e-9d4a674b68b9",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "GOT_LIGHT",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ba320a12-ec75-49ec-8a8e-0d2da4682d30",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "No magic... just, I guess, great self-discipline and perseverance! Qualities I'm intimately familiar with!",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "92f328d9-d9d2-4548-ab55-2d4bd3a778ff",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Or will be, anyway. Maybe tomorrow.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "37b6bde5-2765-4758-ab66-f1be6605dd47",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's just a painting. Nothing to get excited about. ",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
