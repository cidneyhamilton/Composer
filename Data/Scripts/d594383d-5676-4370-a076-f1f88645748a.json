            "nodes": [
                {
                    "type": "nodes.showVignette",
                    "vignetteName": "FirstAidClassIndoor.png",
                    "crossFadeTime": 0
                },
                {
                    "type": "nodes.changeTags",
                    "id": "030e97d7-ce07-4ea7-8e05-413fa8e5d9cf",
                    "scope": "scene",
                    "scopeId": "1c60250c-dab6-4aaa-aaeb-b7f4203ccb9d",
                    "scopeFilterId": null,
                    "tagsToAdd": "LearnedPotions,LearnedHealingPotion",
