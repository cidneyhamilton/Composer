                        },
                        {
                            "type": "options.text",
                            "id": "a54a3c13-e4a4-4715-805b-6fdac8bd078f",
                            "text": "Shake a Crate",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "36034f28-0ec0-4722-b0e7-fb7ceb30269b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I want to avoid little beasties. Of which I'm sure there are many, in and around all the crates around here.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
