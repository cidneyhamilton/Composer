                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "About this one thing, anyway.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
