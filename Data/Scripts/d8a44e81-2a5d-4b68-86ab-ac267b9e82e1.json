                            "text": "Steal the Masthead",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "2b461222-cc84-4fbc-881a-0d64ce02c623",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Let's see... I'd need an axe, a saw, lots of time, and a lot more initiative than I have right now.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
