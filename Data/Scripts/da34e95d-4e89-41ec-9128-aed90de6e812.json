                        },
                        {
                            "type": "options.text",
                            "id": "f2895e79-0184-4e0a-ba9b-23ae7ce72dc9",
                            "text": "Play the Violin",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "28915918-4cd4-4eb6-9620-886fc27655a5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I don't know how to play the violin. These hands were meant for a different sort of art altogether.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
