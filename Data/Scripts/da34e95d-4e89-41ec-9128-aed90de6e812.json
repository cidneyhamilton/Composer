                        },
                        {
                            "type": "options.text",
                            "id": "f594d3c4-1136-4b3e-b3ef-7c4ebaa6a6a8",
                            "text": "Look at the Violin",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "66390da4-4a88-4ace-bb69-636a044b9afb",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "This is a beautiful instrument. The little plaque says this was Darla's first violin, and it's the one she's holding in the portrait!",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
