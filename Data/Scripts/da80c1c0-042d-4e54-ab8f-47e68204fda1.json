                            "text": "Look at the Chair",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "59cf6993-f9bf-4200-9666-f092dc272aa2",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It's a very ordinary chair. Clearly not the Captain's. It's for peasants like me.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
