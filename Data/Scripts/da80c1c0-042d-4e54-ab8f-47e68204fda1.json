                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "062c80f8-2e3a-480d-bc7a-a707692a2708",
                            "text": "Steal the Chair",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "3a79f429-cdaf-42fc-b82f-26f253ed3adb",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It's so plain and ordinary, it looks worthless.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "920b14e7-3f8c-4bd0-bc9b-babcd6c30184",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Then again, maybe I'm just looking at it through worthless eyes.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
