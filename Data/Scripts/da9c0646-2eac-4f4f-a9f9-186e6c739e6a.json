    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "1f949f25-33a5-4e84-9c32-d3caf233aa5b",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "2d517ffb-53bd-437a-8d59-285e4a3338ad",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "5a38f707-75da-434c-9371-7545ba7521c1",
                            "text": "Look at the Crate",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "LookedOnce",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ff0fb960-a97f-46da-b3fc-eb53580bb6fb",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "The crates in here have no markings on them. I don't have a clue as to what's in them.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a65e516c-66c2-4b9b-a716-56a745fc2b7d",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Get used ta it!",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "eedea550-d563-4b23-b8fe-e41bf4592546",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "LookedOnce",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "LookedTwice",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "bcb196fe-ab5b-4600-bea3-177cd4be084d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "They look ordinary from the outside.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "30512bdf-bc12-450a-8057-c6fee20075c0",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Just quit yer wonderin' about my crates! They have my things from before I died!",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "49bebd09-71e3-47da-a19a-da6a0f11ca70",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "LookedTwice",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "target",
                                                "scopeId": null,
                                                "scopeFilterId": null,
                                                "tags": "LookedThrice",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2652fa3e-7c9b-4e20-ad09-4a8347bb6eb5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "Why are you hanging onto what's in these crates? What good are they to you, just sealed up like this?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6b83a01a-9695-4a45-81ab-d45a951eb66e",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Just in case I ever become alive again!",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "acc71dbf-481b-4f8e-b785-80626d6780bd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "text": "How's <i>that</i> gonna happen?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6e16b331-3a38-4802-ad7a-03ee177a2160",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I didn't say it was <i>likely.</i> I said just in <i>case.</i>",
                                                    "emotion": "2",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "e4bc60cd-30ef-48cc-b7d3-38c5aec8b110",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "LookedThrice",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b34021ab-75c1-483b-bc56-eb11286b6fe3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "These are Fester's old things from before he died. I'll just leave them alone.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7e496b5c-5b0d-443d-b920-008a682c3ad2",
                                                    "actorId": "feffd7f3-810e-4625-9d22-dafc701d17e0",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You <i>bet</i> you will!",
                                                    "emotion": "3",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "c7d12721-a189-4447-9645-7dd0a040e8c5",
                            "text": "Open the Crate",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "0310e9ee-0042-479d-8b85-d2d4f8e6a1c9",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I have nothing that'll help me get these crates open.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "50283b0d-5d38-464f-9f96-ae4425a0f5a4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "For once, my good looks alone aren't enough.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        }
                    ],
