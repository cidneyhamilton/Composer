                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "79a2cd66-3d16-4989-9ed1-3b1741f22ee7",
                            "text": "Sense Magic on the Book",
                            "nodes": [
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "cd30c73d-3538-4cb9-a74f-c9bb66537456",
                                    "scriptId": "533e1ce1-13d3-439f-8544-dbdf808d30c4",
                                    "entryPointId": "2a84e00f-2f8e-4123-8684-c243dfdc2bf3",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "5f4c53b2-540d-4627-af71-7ede35c6d84b",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a6fc3b73-da3c-49fe-9ece-eb105bca64fa",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I don't detect any magic on it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "705a54fc-aeea-4ade-8cef-bf1c38ab2ca3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I can't even tell for sure whether it's a book, a volume, or a tome.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.actorPresent",
                                                "actorId": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                "hasNot": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "9bdd0c17-81f0-4107-8797-60658d6443ab",
                                                    "actorId": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "...",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.nodeCycle",
                                                    "id": "c8b1e53b-4408-481d-bee7-9e63f90a61db",
                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "2b5a9d49-5229-459d-b1fe-b8ed9a3d6739",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "That's not helpful!",
                                                            "emotion": "3",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "d1a4357b-092d-4137-bd1e-16f40ee4e5ac",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "I'm sure you're doing your best.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "01c0a133-5c59-49df-bd5a-f5122f7fd4df",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "See, I thought you'd switch it up this time, and actually say something.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "4cdc1279-051a-4e12-8a80-7e194166e1ef",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "Stop speaking in punctuation!",
                                                            "emotion": "4",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "f755ec5b-2719-4011-b597-4a51516996a4",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "It's nice to know we're both equally patient.",
                                                            "emotion": "1",
                                                            "emotion2": "1",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "98e980e9-2523-4f4d-aed3-8c6cb803ae9d",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "Is this Frican click-language I've heard about? Or is it just from another period?",
                                                            "emotion": "1",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "19438963-f199-4718-8c5c-63a0001a2f81",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "I don't usually have this much trouble communicating.",
                                                            "emotion": "2",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "b9608177-038b-471d-aaa2-a276960a04b0",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": "f36c57a0-78e2-4aed-a1cf-5e9a67d06751",
                                                            "text": "I've wasted a quarter of an hour doing this? You won't catch <i>me</i> wasting time like this again!",
                                                            "emotion": "3",
                                                            "emotion2": "1",
                                                            "device": "0",
                                                            "device2": "0"
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
