                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b3ab074d-5159-4cf2-b52d-374060523651",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "The leatherwork on that cover could've taken somebody a month. I'm guessing this isn't just another book for children about lovelorn vampires.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
