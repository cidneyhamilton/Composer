                            "id": "eaebcdd7-011a-456e-9990-53280e57d327",
                            "text": "Read the Book",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
