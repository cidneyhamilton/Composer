                                                "scopeId": "d4d31e10-4cd7-4e93-a86b-71352060d4a7",
                                                "scopeFilterId": "0f09f7d6-26b1-438e-aa73-fc5f6065ad9e",
                                                "tags": "BeenRead",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "12cc42cf-cf88-430c-a07a-219c3353cc44",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I know he may throw me out for touching this book, but it <i>is</i> on display...",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1d511a47-c927-4c48-bfd4-2ba7d1592d9a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Hmm. It's all in Arcanum. That's inconvenient.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "61d1f16d-1f80-4090-9592-a88c23f6ae64",
                                                    "scope": "prop",
                                                    "scopeId": "d4d31e10-4cd7-4e93-a86b-71352060d4a7",
                                                    "scopeFilterId": "0f09f7d6-26b1-438e-aa73-fc5f6065ad9e",
                                                    "tagsToAdd": "BeenRead",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e664783d-d12d-41e5-890e-b85ba8dd34cc",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I already tried. It's in Arcanum, I can't understand a word of it.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
