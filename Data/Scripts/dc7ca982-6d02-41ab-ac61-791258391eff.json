                    "scriptId": "b2da7853-1a90-4886-a9c7-6015909fecfd",
                    "entryPointId": "24078100-3eb1-4092-9ab1-04aaa025f852",
                    "currentScope": "Scenes",
                    "currentSceneId": "a8efa640-212a-4108-8251-612933f66abc",
