                    "scriptId": "0acd60ad-eda5-4dd2-a44f-2541a572e6b1",
                    "entryPointId": "be9004fe-d680-4534-8f50-e36e778b13b0",
                    "currentScope": "Scenes",
                    "currentSceneId": "a8efa640-212a-4108-8251-612933f66abc",
                    "currentActorId": null,
                    "currentStoryEventId": null,
