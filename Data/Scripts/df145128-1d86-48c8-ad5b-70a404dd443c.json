                            "nodes": [
                                {
                                    "type": "nodes.addJournalEntry",
                                    "id": "0ae5eb98-4d11-4f53-9e93-bf19799c1ac5",
                                    "name": "Poobah Pot",
                                    "notify": false,
                                    "description": "I would like to play Poobah tonight in the Rec Room, but I don't have a lot of Lyra right now. I may need to make some more before I play Poobah. I don't want to run out of money in the middle of the game.",
                                    "notes": ""
