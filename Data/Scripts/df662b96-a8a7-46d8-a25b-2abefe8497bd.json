                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I'd like to talk to you about what we did with the Giant Proach...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
