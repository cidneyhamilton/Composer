                        {
                            "type": "options.text",
                            "id": "7d219398-2d29-4ad6-9e5b-8a78e6791b2b",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "8d04c13b-02c5-4369-82ec-bdf9a93a6f71",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I'll see you tomorrow!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
