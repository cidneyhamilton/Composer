                    "options": [
                        {
                            "type": "options.text",
                            "id": "1a1e207b-1353-4982-9a87-9f69058654da",
                            "text": "Examine the Clutter",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c9c885d5-c097-4d22-9586-1961ca81fcd3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "The clutter is mostly school work, I think. There are poetry books and sheets of music, both stained by what appears to be some unidentified lifeform growing from a neglected stein.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4a7cff20-db52-4830-9ceb-41b9140f30dc",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Aeolus should know he has to clean things up. This bard's all rhyme, no reason.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
