{
    "type": "script",
    "id": "e32150ab-287a-4acf-8104-ff4779fd8667",
    "name": "RCC10 Joel Midterm",
    "sceneId": "3256b6d8-50b9-4764-b016-01ea538ea55f",
    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "f50d457c-f13d-4055-912a-a498f45face5",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "05ec34da-b123-4947-b02e-0cbb88a2b9df",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "e9b212dd-b031-4b86-af67-a4a601754527",
                            "text": "Ask About Joel",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                "scopeFilterId": null,
                                                "tags": "JM10",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "dd0c292a-1156-4930-9d5c-545f8bd83370",
                                                    "scope": "target",
                                                    "scopeId": null,
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "JM10",
                                                    "tagsToRemove": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "11fd313c-6271-469a-834a-9f63995b2cb1",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "What kind of investigations are you doing?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f460e3bf-ab13-49c8-af58-5c6c5a339ae3",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I am on a quest, so to speak. I am searching for a particular statue of a raven. It is said to be hidden somewhere on Sardonia.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.showMenu",
                                                    "id": "2c556c24-187c-4226-81ad-c0a0a1cdd4b0",
                                                    "options": [
                                                        {
                                                            "type": "options.text",
                                                            "id": "20aa4bfe-84f4-45b1-b8f4-00b2a62d2860",
                                                            "text": "Ask About Joel's Quest",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "c6d384ff-4f0a-40e0-921d-7391d2588b3a",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                    "text": "Why are you questing for some bird statue?",
                                                                    "emotion": "4",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "62d2f382-5aba-4d04-be09-875a51cd9415",
                                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "My late father, may he rest in peace, spent his life searching for such a statue. Alas, he searched in vain.",
                                                                    "emotion": "2",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "1a6ec8e2-8c0a-46d3-b21d-0587ecc93c1c",
                                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "Finding it would make my father so proud if I were to recover it - in his name, of course.",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "4",
                                                            "ExitMenu": false
                                                        },
                                                        {
                                                            "type": "options.text",
                                                            "id": "64a13977-7e82-4765-8253-8cc4195f410e",
                                                            "text": "Ask About the Statue",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "b938b361-fd6c-4efc-8a9d-294d025dddae",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                    "text": "What makes this bird statue so valuable?",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "33c6ab50-b846-40ec-a8a8-459cadc840c6",
                                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "The statue itself is of little monetary value. It is purely sentimental. ",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "fa5e48b1-790c-4c61-8b1e-fb8411edee94",
                                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "text": "If you should happen to come across such a thing in your explorations, I would be more than happy to compensate you greatly in return for the statue.",
                                                                    "emotion": "1",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.changeReputation",
                                                                    "id": "42b71b0b-34cc-4fa6-941d-9ef3aef8e2ee",
                                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                    "amount": "1",
                                                                    "change": "1",
                                                                    "notify": true
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "4",
                                                            "ExitMenu": false
                                                        }
                                                    ],
                                                    "AutoAddDone": true,
                                                    "Unique": false
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e65fb1a9-a6c0-4fa1-bdde-abdb8813c9c7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Are you worried about passing the Midterm?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "200af144-ebf6-4930-b5c7-225d6046067b",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Of course not. It will be trivial. It's only a matter of saying the words that the Master wants to hear.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "129eeb3d-7c2b-4f93-bf6c-19cb99d6a49d",
                            "text": "Ask About Odds and Ends",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "d8422ba7-c321-45aa-b54d-70d44161fd0c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "So what sorts of odds and ends are you selling today?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "644934fa-0d53-477c-ae9a-3df2b0ab7923",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I have acquired a more powerful version of the Boom trap - the Big Boom.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "639adf2b-f88f-412c-aa96-3a6e7dc8bd5e",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The Big Boom does a lot of damage - I wouldn't advise being near it when it goes off. It could be... messy.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "9",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "585cb10e-f548-43dc-8d35-5888951f7fb5",
                            "text": "Ask About Midterm",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7ad11fa0-d900-4f3b-991a-72e53bb6a386",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "What's this about a Midterm break?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "6aef3bbf-c3fc-4a19-b874-0813616c1b59",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "We will have three days off school to do with what we like. ",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "6675c5d3-552a-4996-a0e4-98148f4d4cff",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Of course, first we will have to face the Midterm exam. I am certain that the Master will test us thoroughly upon his lessons.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "72096677-ecd4-4b22-8424-a6a9c582af36",
                            "text": "Ask About Rumors",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "b1f77993-aad1-4b8d-b5a3-79901a35adcd",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "Found out anything interesting recently?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "86c64acc-3037-46d8-bb94-09060322c367",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I have heard the rumor that years ago, there was a Raven Statue in Sardonia that united all the Thieves of this island under one guild. Unfortunately, that statue has not been seen for nearly 20 years.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "d40989cc-cf84-486d-8f47-f509455f23b4",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "958cfc0e-a328-4c21-9056-48d6f2897588",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "See you later!",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "5da79cd4-f461-40c4-a5ba-fb38dd186aa2",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I very much look forward to doing so.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
}