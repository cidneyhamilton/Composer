{
    "type": "script",
    "id": "e35af347-bbee-4efa-973b-94121f696830",
    "name": "Joel RR15_Poobah4",
    "storyEventId": "e27eeea1-0257-4cec-91c6-510d968c94ae",
    "sceneId": "84006d08-8e6d-4558-b9b5-101f3c3054fe",
    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "ee1da4a6-4892-4cbe-9441-8b9525096b9e",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "c4b825c2-456f-4ec1-ba1c-90bdf65dce45",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "97e72ac6-8023-4e1a-83e8-d70c6194e324",
                            "text": "Ask About Joel",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "JoelPoobahCheat",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "CheatingSosi",
                                                    "has": true
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e9109df4-04bd-4bfa-b9bd-5b2b9781b52f",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "You are clearly enjoying the thought of giving Sosi what he deserves. Do you think we can pull this off?",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "21506f8d-57ba-44b2-aa30-fc90197dcdc6",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Caesari is aware of my... skill... with a deck of cards, so he will be cautious when betting against me. However, I believe his antipathy towards you will overweigh his caution.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "JoelPoobahCheat",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "CheatingSosi",
                                                    "has": false
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c1e752aa-a2e9-4fc7-81f1-475910bc3631",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Are you really going to cheat at cards to get back at Sosi?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "88f4f4a2-4ad6-49d6-b017-d2810c30686c",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Oh, no... I would never do such a thing to my dear friend Caesari...",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "efc49ed4-0a8d-444a-abce-87e5844269ce",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Let us just say that I will make certain that the odds that Caesari will have a winning hand of Poobah are rather small.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "baea8c8a-9550-4024-bb05-5bada9e3a743",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "You think we can beat Sosi at Poobah tonight?",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3eb2e325-fe47-4922-8fee-1178bc764cc6",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "The odds of winning will be in our favor this evening.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e61d7979-b2e0-4f37-ad4f-0e304c144891",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "What makes you so sure that Sosi will lose?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "39b9aaf6-d16a-4801-b4cb-0355b49abfcd",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Let us say that up to now, Caesari has been extraordinarily <i>lucky</i> in his hands at Poobah. Sadly though, Caesari's luck runs out tonight.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "08c9f00a-95e7-43de-a04d-aaff045ab226",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "RiggedGame",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "860334ba-f6cd-4a49-b803-48abf3bda7aa",
                            "text": "Talk About Poobah",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "JoelPoobahCheat",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "CheatingSosi",
                                                    "has": true
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3bf00570-12ba-4053-8454-fd379b8f972a",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "So how are we going to accomplish the worthy goal of winning all of Sosi's ill-gotten gains?",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2803f441-909b-4f95-b3f8-4ac35f9aa770",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You can rest assure that whenever I am the dealer, Caesari's hand will be good, but your hand will be better. You should bet accordingly.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "JoelPoobahCheat",
                                                    "has": true
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "CheatingSosi",
                                                    "has": false
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "85c11e8a-ed3e-48b3-92a8-174fecddb4bd",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Has this Poobah game been rigged from the start?",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "96b2a585-5912-45d0-9e6e-ffbf7200b401",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "No... no... I would not cheat against my friends...",
                                                    "emotion": "2",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a648b7d8-5d5b-43b4-a9a4-01f7db84b4a1",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It is that I did not always play to my fullest advantage against Caesari.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d383b893-1ba9-4cc5-bf60-eaf303159c5d",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Are you suggesting that we cheat at cards?",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "85743e24-de84-484e-a1e3-32fa4fd72486",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Do you think that I would stoop to such a vile thing?",
                                                    "emotion": "4",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c1142ace-c791-4214-be87-a6856eaa8860",
                                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It is only that the playing field has been leveled, my friend. Caesari has always depended upon my support. Tonight, he will only have my enmity.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e98ff4d0-7d8c-4b5c-9248-161d3506887e",
                            "text": "Talk About Sosi",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "dce1e4be-d67d-49e4-98c3-d81fb8c4488b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "Does this mean that Sosi cheats at Poobah?",
                                    "emotion": "3",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "ec5807c6-2e12-4d23-b118-415c977ea12b",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "<shrug> Clearly, Caesari takes advantage of every opportunity to change the odds in his favor. ",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7030d9a9-cf0a-4565-927f-ad87a9ca3218",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "However, Caesari is not as clever or skilled as you and I are, my friend. Poobah isn't just a matter of what cards you hold. It is all about how you play those cards.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fb44f259-85fb-448c-87b1-669903c42785",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "For once, Caesari will not be able to bully his way to winning.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "cc9b5456-87ef-41b2-93fe-4dc86bcbc827",
                            "text": "Start the Poobah Game",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "d60b085b-ad86-4f7b-b239-e8a7dca6335c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "Shall we begin this game then?",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "739a54dc-5eb0-46e1-b6df-563850196cde",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "There is nothing I'd rather do than see Caesari's face when he loses his money.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.invokeScript",
                                    "id": "9864e328-2085-4012-98f7-d68921e329ec",
                                    "scriptId": "f1562fb6-8b4c-46c5-8ca1-596c961abf64",
                                    "entryPointId": "0039f70f-9837-4acb-bcd2-84296e8d2077",
                                    "currentScope": "Scenes",
                                    "currentSceneId": "b5e12b26-529b-41c8-b6cf-a7688b177694",
                                    "currentActorId": null,
                                    "currentStoryEventId": null,
                                    "currentPropId": null
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        },
                        {
                            "type": "options.text",
                            "id": "801b6e44-41b3-49cb-a75c-4ac8bf7b96f7",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "11a83693-8742-4bc8-bb8f-b5036c3b022e",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "I'll talk to you later.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a3839bb4-f50d-43f2-9cfc-4a2213a8acd2",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Indeed, I hope the game will begin soon.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
}