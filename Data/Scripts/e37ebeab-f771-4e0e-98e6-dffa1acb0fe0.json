                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "e2659c9b-2f18-41fa-9767-e5f21ecd94a5",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Now that I've defeated it, there's nothing left but a simple skull and some wings.",
                                            "emotion": "2",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "91d2a97b-4fa7-4c52-b45b-cdddf0118450",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Maybe Gregor will buy the wings for Skatter Stew, or something equally unappetizing.",
                                            "emotion": "2",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "511f6e78-3f24-43af-a2b6-bd8b75aec7d7",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "Wait... there's something inside the skull.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "536405e5-605a-4fe3-ae14-e9854cd11438",
                                    "nodes": [
                                        {
                                            "type": "nodes.branch",
                                            "sections": [
                                                {
