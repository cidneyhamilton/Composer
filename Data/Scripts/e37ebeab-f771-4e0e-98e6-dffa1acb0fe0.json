                                                            "id": "b91d394b-d52d-4000-a37f-b94cd1a083c6",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "There are 8 Lyra inside the skull.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
