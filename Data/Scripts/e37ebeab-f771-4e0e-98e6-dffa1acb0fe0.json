                                                    "nodes": [
                                                        {
                                                            "type": "nodes.speak",
                                                            "id": "cdd4fda1-3495-4544-9880-c2d41549c44a",
                                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                            "actorId2": null,
                                                            "text": "There are 10 Lyra inside the skull.",
                                                            "emotion": "0",
                                                            "emotion2": "0",
                                                            "device": "0",
                                                            "device2": "0"
                                                        },
                                                        {
