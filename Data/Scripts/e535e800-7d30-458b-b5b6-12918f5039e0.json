                        {
                            "type": "options.text",
                            "id": "332866df-a1cb-4713-94e7-ee5acd193602",
                            "text": "Study the Big Book of Tool Use",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "55325e48-b9c1-4929-be27-6a5fde478d9d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "This reads like a child's book with big words, pictures, and a smiling \"Mr. Lockpick.\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
