                                                    "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                                    "propId": "be64d025-2e36-4027-887b-af503112424f",
                                                    "target": "0",
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": true,
                                            "minutes": "6",
                                            "ExitMenu": false,
                                            "notes": ""
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "52f5f0bc-507f-48e0-b517-dd1e0b06e83b",
                                            "text": "Mock the Ghost",
                                            "nodes": [
                                                {
                                                    "id": "f6e7c67d-c994-4b9e-9238-eefd74a19525",
