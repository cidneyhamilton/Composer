{
    "type": "script",
    "id": "e6c5ea2d-6f01-42eb-badf-0c41b737d031",
    "name": "Dining Hall Conversation Day 20: Proach",
    "storyEventId": "dc120962-4525-4b62-b0de-9fc26fb189e4",
    "sceneId": "a8efa640-212a-4108-8251-612933f66abc",
