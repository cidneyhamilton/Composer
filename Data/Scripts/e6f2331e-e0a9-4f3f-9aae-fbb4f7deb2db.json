                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2ce2abd7-2c24-41ed-ace2-77ba0e32436b",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "It's a deep hole. I can't be sure, but I might just be seeing a stalagmite surrounded by water. Looks like some sort of platform down there... or an open mouth!",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
