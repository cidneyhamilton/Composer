                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "'tis sweet of you to say that, Shawn.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
