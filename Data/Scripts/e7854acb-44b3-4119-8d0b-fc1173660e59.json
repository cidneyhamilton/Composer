                                    "notify": true
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "53028dfe-386a-466f-a96f-a4275df0019d",
                                    "actorId": "a104063f-f97b-49da-a22a-4104403e86e0",
                                    "amount": "2",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d814893b-dc64-4711-99e3-e15df5e7dfc6",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a104063f-f97b-49da-a22a-4104403e86e0",
                                    "text": "So what you are saying is...",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "c278a9eb-0a6f-44fb-b3f8-b6fbb755fd1a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a104063f-f97b-49da-a22a-4104403e86e0",
                                    "text": "For a heated fray when in melee\nThrow the Flambe away.",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0d38a47d-3f9a-4121-9435-7713dfe61e5b",
                                    "actorId": "a104063f-f97b-49da-a22a-4104403e86e0",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "That is a reasonable paraphrase of what I am saying, yes.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": true
                        },
