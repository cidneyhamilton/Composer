                        },
                        {
                            "type": "options.text",
                            "id": "27437a9c-b232-4b7c-aa2e-ea5366c2baaa",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7c3490c7-2db8-45b7-92e8-d224b1b09de3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "Have a good night, Thomas. Sweet dreams.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                "scopeFilterId": null,
                                                "tags": "PrefersGuys",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5044f4ae-117f-49df-9569-80f65d9937c4",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You, too, Shawn. Thanks for everything.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "525c074e-8a30-4d4e-a8e7-2af2a4de7f6a",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I'll be seeing you in my dreams.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        },
        {
            "type": "scripts.entryPoint",
            "id": "f300a55e-2aee-45dc-acd5-fa40a8223225",
            "name": "Snuggles",
            "nodes": [
                {
                    "type": "nodes.speak",
                    "id": "aa2e1f8d-6e80-4cef-8ba6-abdd16c7fa04",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                    "text": "Why don't we spend a little time now getting to know each other better?",
                    "emotion": "0",
                    "emotion2": "0",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.speak",
                    "id": "33594adf-0173-4938-be44-258b058a3c91",
                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "I'd really enjoy that. It's so nice not having to hide from you now.",
                    "emotion": "1",
                    "emotion2": "1",
                    "device": "0",
                    "device2": "0"
                },
