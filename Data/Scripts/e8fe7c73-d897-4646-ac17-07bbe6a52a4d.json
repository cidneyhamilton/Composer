                        },
                        {
                            "type": "options.text",
                            "id": "4601e163-66f1-40e8-9b2e-114312167433",
                            "text": "Light the Torch",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "fced0454-5044-4a25-8f9b-fe28e2efa842",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I don't know if I want to light this torch. I do like the cozier feel in here.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
