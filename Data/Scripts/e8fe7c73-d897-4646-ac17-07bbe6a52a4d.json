                                    "id": "51ec874b-2612-4afc-8066-c536e3f7dd8d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I could take the skull off of it. That would make it a <i>lot</i> lighter!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "01536553-640d-4ebc-8f7c-820856decde8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I'm joking. I'm not touching the skull.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "1",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "1061a236-9cc1-4fc2-aba8-785c935e0c23",
                            "text": "Steal the Torch",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
