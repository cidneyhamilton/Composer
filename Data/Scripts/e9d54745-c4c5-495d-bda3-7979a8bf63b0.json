                        },
                        {
                            "type": "options.text",
                            "id": "15a405dc-0554-4e7b-b746-d33a80d49e49",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "7c819703-67f9-4081-8b58-aabb968da04a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                    "text": "That's all I wanted to ask you. Good afternoon, sir.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
