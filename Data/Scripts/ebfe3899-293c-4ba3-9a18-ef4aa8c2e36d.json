                        },
                        {
                            "type": "options.text",
                            "id": "1ca9cab0-b599-4b66-bfff-8989e7c42a4a",
                            "text": "Take the Suit of Armor",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "92da70fb-9885-4f1e-85a9-729d495610f1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "He's supposed to be good luck for gamblers. I'm sure it would be bad luck for me if I stole it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
