                                                                    "actorId2": "0b9c114f-a7cb-499a-b151-60c316be07e9",
                                                                    "text": "All I took was this silver letter opener because I needed it to pry open a drawer.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
