                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "93797bae-9205-46ee-8440-2df57a13328c",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "The plaque reads, \"Portrait of Lenkyl Greatstorm in a Yellow Cloak.\" But I think that's more of a dusky sandalwood color.",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
