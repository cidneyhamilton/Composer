                                    "id": "f2d68dd1-48df-450b-bb43-c93a79be24f8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I don't know what kind of bush this is, but judging by its flowers it enjoys being outdoors.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
