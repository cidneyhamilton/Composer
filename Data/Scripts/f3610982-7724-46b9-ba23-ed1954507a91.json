{
    "type": "script",
    "id": "f3610982-7724-46b9-ba23-ed1954507a91",
    "name": "I09: Kyro Resolved",
    "sceneId": "1c60250c-dab6-4aaa-aaeb-b7f4203ccb9d",
    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "5a966f42-3d30-44e6-a788-cca8ce17ed36",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "381e657b-d0ee-4942-ac63-a7da9309ad6f",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "46d94ec0-c5ef-4bc3-b74b-f6bbca08f43d",
                            "text": "Ask About Moira and Heroism",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                "scopeFilterId": null,
                                                "tags": "KyroSaved",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "9b0b31db-d5d0-4d4e-ba0b-9f877a6eee50",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "572fb7a6-c811-40e7-97cf-c27f5ed0da49",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "So is this what it means to be a true hero - to be able to save your friends and others? It feels pretty good.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "01154783-9bce-4448-8cf5-a4f06ae04bea",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "There are many paths to becoming a hero. Saving Joel's life was just one step towards becoming the great hero you can be.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.and",
                                                "left": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "scopeFilterId": null,
                                                    "tags": "KyroSaved",
                                                    "has": false
                                                },
                                                "right": {
                                                    "type": "expressions.inTags",
                                                    "scope": "actor",
                                                    "scopeId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "scopeFilterId": null,
                                                    "tags": "TalkedWraith",
                                                    "has": true
                                                }
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6fb24b23-3331-4c2e-a09a-0d5b41f639c4",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "I tried to save Joel...",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "d9fae48d-513d-4e16-aad4-0fa21a2c64a1",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "After you told me about Joel, it took me a while to get everything I needed to face the Wraith. I met Thomas in the Catacombs and she helped me find Joel, but we were too late. The Wraith had drained the life from him.",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2be1b136-0e9a-430c-8e66-9a6904173f26",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "If only we had had you to lead us directly to Joel, we might have saved him.",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "b982c3f4-ecb0-451f-b8f7-405ee5c88ba2",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "Have you ever had someone die and feel like it was your own fault? Maybe if I had just searched for Joel a little more, I could have saved him.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2af746b3-b2c4-465e-80cd-c66790af4de0",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I know how you are feeling. I've lost comrades and friends before. I've had my share of guilt filled with 'if only I had done something different.' ",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f3721308-765a-4b88-902e-fa7add19109b",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "We can't change the past, Shawn. We can only make the future better.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "2a9f1d95-7a1e-41d7-bbae-c771bc63584a",
                            "text": "Talk About Joel",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                "scopeFilterId": null,
                                                "tags": "KyroSaved",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "3c450bbe-05bc-4485-89bf-882b2f9f0d57",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a5d45742-b157-46f5-a20a-e7dd5c3e4456",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "What do you think of Joel? Will he recover?",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a7a85943-d288-4a64-9e25-5c0dca38f0c4",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Joel should be physically fine soon. He's doing surprisingly well after such an ordeal.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7f5c6a45-2acd-4b04-b297-7cf974dccc99",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "In fact, he is making such good progress, I am allowing Joel to recover in his own room rather than having to stay in the infirmary.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "47576fb0-9b35-4f1f-922a-6ff912f1138d",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "What he needs right now is a true friend to help him get over his feelings of betrayal. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f805af0b-9ea0-4ef2-95b2-baefedb5c639",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "I tried to find Joel, I just didn't get to him in time.",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "99b06eb1-a1f8-4c95-881d-238a952acd8e",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "It is something all heroes need to keep in mind - You can't save everyone. You can only try to save someone and never stop trying.",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "6c082fb0-b880-4444-be91-d372ca28be0a",
                            "text": "Ask About the Wraith",
                            "nodes": [
                                {
                                    "type": "nodes.comment",
                                    "message": "Only show if Moira defeated the wraith.",
                                    "commentor": "Cidney"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "85d49c21-d2b2-47f1-8afc-0170beec292e",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "text": "What about the Wraith? Is it truly destroyed?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "472342ab-6427-4c55-982d-6b65b52d3d61",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Yes. After you and Joel escaped, I managed to defeat it and drive it back to its phylactery. After some searching, I located the phylactery and destroyed it before the Wraith had recovered.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "2d73fd85-5941-4da5-b5a6-6b2db4c7b6cb",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "That Wraith will never harm another person.",
                                    "emotion": "3",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                "scopeFilterId": null,
                                "tags": "MoiraWraith",
                                "has": true
                            },
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "1427bee3-a2f6-40b0-b939-6f7d736b1d85",
                            "text": "Flirt With Moira",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                "scopeFilterId": null,
                                                "tags": "KyroSaved",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "c569ef39-7c6f-4823-8ae6-5e5d93c73678",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "Maybe I could come back here later tonight and you and I could spend a little romantic time together?",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7c9d767b-cb25-4ac6-a6f0-6d22f99e88b8",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Shawn, I'm sorry if I haven't made it clear to you, but I am not interested in having a romance with you.",
                                                    "emotion": "0",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5c111f60-ab05-4613-8c0b-82e774fa197e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "text": "Well, you can't fault me for trying, can you? You are a very attractive woman.",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3c2e63e4-64ab-4a86-b58f-49feb58b212f",
                                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "<sigh>",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f83a97c1-0d8e-48af-a7a8-6c2ca8014b20",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I doubt that Moira would like me to flirt with her right now.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "258e72f4-ea26-41bd-99ae-72378399a149",
                            "text": "Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.onceOnly",
                                    "id": "88aa8736-e447-4c0c-94df-926c9cbb2a2d",
                                    "nodes": [
                                        {
                                            "type": "nodes.changeReputation",
                                            "id": "10903b0a-979e-4bb2-b557-80ded8870aa5",
                                            "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                            "amount": "1",
                                            "change": "1",
                                            "notify": true
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "308ad2c8-ee83-435d-b974-9a96f67b7f69",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "text": "I'll see you later!",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "610bf685-5d99-42b5-8c9c-9c0d08be339c",
                                    "actorId": "6b30a3a9-2324-487d-9db9-03ce174e0e80",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Take care of yourself, Shawn.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.endScript"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
}