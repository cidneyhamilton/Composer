{
    "type": "script",
    "id": "f373fdb4-1774-46a4-9d93-1de62cca170a",
    "name": "Librarian: First Talk",
    "sceneId": "981ec865-3e1b-4f3e-bb0d-11df2bc7d593",
    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "a3996bfb-ee44-4e72-b1c4-f1eaf9d019d4",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "actor",
                                "scopeId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                "scopeFilterId": null,
                                "tags": "IntroductionsAccomplished",
                                "has": false
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "95074921-f744-47d9-a7a1-f870aa4f9e67",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "Hi, I'm Shawn O'Conner. I'm in the Disbarred Bards class.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "9cb13453-35b3-40d1-81d9-d004ecbfbaf4",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Q1.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4a89482c-15b2-41bf-a0eb-6b9d9470c67a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "Um... what?",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "84e26f24-d03c-498e-ab7f-04d6f5ca968f",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The Disbarred Bards section is Q1.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "14d32cfa-4bcc-4c73-916f-19db6acaa56d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "Oh, thanks. I was just trying to introduce myself...",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4f8f05f6-2708-40c3-abf2-9528a1dafe3b",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Very well, you have successfully introduced yourself.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d19fc0f9-0d0f-40cf-bdc7-f0c37b055997",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "So what do I call you?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d65df7a8-9d67-44fb-b3f0-1eeb1c1b3801",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I am the Librarian. That will suffice.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "nodes.showMenu",
                    "id": "431ffdf8-3778-4b4b-b852-df78c2f92a86",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "6b954560-c499-45c1-b4d4-6dece694c479",
                            "text": "Ask About the Librarian",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a63d6097-dbf6-4c32-a926-06b5418672d3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "So, how are you? ",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "105d5821-6d1a-494d-8189-e66b45ac8615",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "How am I what?",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b92671c7-9540-4b5b-b515-df3cd0693a49",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "Um... so how are you feeling? How's your day going? ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "370f6cc9-843c-4a2f-a5e3-c85aa6272e45",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Is there some logical motivation for your inquiries, or are you just wasting my time?",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "4a6ce2e9-fcd0-4a5e-be92-5f9f450949cb",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "741972c3-6b7a-49a6-abaa-7398c04578f5",
                                            "text": "Say Something Charming to Flatter the Librarian",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "cfeed485-85dd-4d70-9161-5310bfc4d059",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "I was just trying to get to know you. You're clearly an important person here in this impressive library and at the school. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "b9581fc8-1ee1-4683-8cb1-3d68c1f64189",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "15",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "1d75ddcc-4b85-4c21-b869-94e0599abea9",
                                                                "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "339fbbf3-29bd-404f-ac49-30b4ea4de928",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Ah, then you are wasting both of our times. Time is far too valuable to fritter away on pointless interactions.",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "4",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "2d565941-e18a-4f5f-8c93-34dfc66ca190",
                                            "text": "Say Something Clever to Please the Librarian",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "30a8fe31-dee7-400e-878c-4e233df801e7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "I was just trying to chat with you. Since I'm new here, it's useful for me to find out who's who and what's what. ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "db1a1db4-ebfa-407a-9faa-590b19633b7b",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "15",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "5b79c5c5-567b-49c9-b6a6-c155d18cac2a",
                                                                "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "ec021640-680f-4788-a584-2b96f858e56c",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I do not 'chat.'",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "ffd17f66-f6ff-4856-9a84-d92e1bd0d046",
                                            "text": "Say Something Snarky to Amuse the Librarian",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "6f5a1c14-c637-46cd-8e78-4e4efe01eaaf",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "I was just trying to suck up to you. Who knows, you might be useful to me.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "2fea4621-b323-4062-8758-b55fde4b0b13",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "15",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "fc69dc18-85ec-49e6-afd4-8c97d2a03e8f",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": null,
                                                    "text": "Indeed.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "507a9e16-6d64-4c1d-a6f9-5ed36d0988c3",
                            "text": "Ask About the Library",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a9904c13-63a3-4c9c-bf58-d2d7cf8a3b56",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "text": "This place is impressive. I've never seen so many books! How do you keep track of them all?",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "42686ec3-06a2-49ec-aedb-c81bfce1e0f0",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": null,
                                    "text": "Books do not leave this library. You are permitted to study or do research in the library until curfew. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "c4256363-7a3a-419e-a717-8cb62cf62955",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The books return to their proper places each night.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "c0b79823-d548-42bf-b425-b3448e9239c4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Are the books all magical?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e5de3707-2926-4f5b-b135-bc452cbb08ce",
                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "There are no magical books accessible here. The bookshelves have been enchanted to keep books properly organized.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "6",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "16c83f7b-0728-423a-a454-4397556b2f1e",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                "scopeFilterId": null,
                                                "tags": "IntroductionsAccomplished",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "44ae7ed9-5a6f-4e19-b568-f8f79fa87550",
                                                    "scope": "actor",
                                                    "scopeId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "IntroductionsAccomplished",
                                                    "tagsToRemove": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0498dcda-9ee2-430f-9b9e-666e235bcbb7",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "text": "Good evening. It was nice talking to you.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3ecbb876-a629-4c8b-ab80-91870cd8012e",
                                                    "actorId": "84d47ff5-c737-42b6-9e21-e8b7413ce007",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "...",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
    ]
