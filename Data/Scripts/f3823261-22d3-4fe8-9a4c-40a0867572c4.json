{
    "type": "script",
    "id": "f3823261-22d3-4fe8-9a4c-40a0867572c4",
    "name": "Darla - Event 4 - Thomas Rogue Morals",
    "sceneId": "25782df1-4461-4651-9bab-1fb4e1b4e366",
    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
    "trigger": {
        "type": "triggers.manual",
        "name": "Manual"
    },
    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "3acb6282-4c4c-4952-ba12-9f135ea3865b",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.changeTags",
                    "id": "8e1c791c-f875-4828-8e01-5df2cc5b265a",
                    "scope": "actor",
                    "scopeId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                    "scopeFilterId": null,
                    "tagsToAdd": "SurprisedMeeting",
                    "tagsToRemove": null
                },
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inTags",
                                "scope": "scene",
                                "scopeId": "25782df1-4461-4651-9bab-1fb4e1b4e366",
                                "scopeFilterId": null,
                                "tags": "ThomasKnows",
                                "has": true
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "583aa06b-4591-454e-89d6-57cc4fbecc09",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Hello, Shawn. Do you come here often?",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8979ecd6-f15e-4ccb-8b48-25bfa8e77d59",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "Only when I'm looking for a little excitement.",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": null,
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                "scopeFilterId": null,
                                                "tags": "SurprisedMeeting",
                                                "has": true
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2b37eedc-ba2f-4dfc-abd5-6869c1b21f22",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Oh, you again!",
                                                    "emotion": "4",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "513c53e1-8cf9-4054-a323-be96a6773ca3",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "text": "Miss me?",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "2b1c2efb-273c-4e6b-a102-bdda33520b1e",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "What are you doing here?",
                                                    "emotion": "4",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f387f5f9-2a31-46f5-b0fd-ce5fbd132925",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "text": "I could ask the same thing of you... ",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "nodes.showMenu",
                    "id": "f6221fce-cad6-461d-985c-cff74a529884",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "d2a98bc2-6c76-4c36-882b-5a8629ebaa5b",
                            "text": "Ask About Thomas",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "e0ade657-e856-40ab-8710-7c90d8fd2964",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "So what is it about the painting in the corner that fascinates you so? It looks rather grim to me.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4ebebc6d-e523-4d8c-b295-d958ad4d945d",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "It's a painting of my father.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeTags",
                                    "id": "a365fe57-6658-4611-88f2-b4978cb557bf",
                                    "scope": "actor",
                                    "scopeId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "RevealedFatherPainting",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "715f0e28-e611-4c7d-950f-7631f0528fe9",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "Oh. He looks very paladiny and noble. I can see the likeness now.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "e0ca8c26-18ba-4fce-92e6-64cd28730d81",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I come here at times to think about what he would say if he were still alive.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "dfb18645-e23d-41a6-bd39-8ee2af2e864e",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "a3fdbeda-8aaf-4874-9d69-eb7392347ed7",
                                            "text": "Say Something Polite",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "32df7f33-e35e-431f-9216-8fc53fcaba26",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "text": "I'm sure he'd be very proud of you.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7b016f4a-1350-44ea-ae9a-8c6996651b7f",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Proud that I'm a Rogue? That I learn how to deceive and steal things? That I failed to become a Paladin like he was? ",
                                                    "emotion": "3",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "246e4eeb-a597-4243-99c0-c53485e79837",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "35",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "4de57e8b-8603-4db4-897f-58e301b6cb0e",
                                                                "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                                "amount": "2",
                                                                "change": "1",
                                                                "notify": true
                                                            },
                                                            {
                                                                "type": "nodes.speak",
                                                                "id": "88dff422-dca0-4cd0-bb80-dcce9f827a4b",
                                                                "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                                "text": "I think that he'd be proud that you always do the best you can, no matter what happens in life. You really are a good person, Thomas.",
                                                                "emotion": "0",
                                                                "emotion2": "4",
                                                                "device": "0",
                                                                "device2": "0"
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "a4cd8143-ba8b-407e-8931-e6f3580b97e6",
                                                                "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            },
                                                            {
                                                                "type": "nodes.speak",
                                                                "id": "52ee400d-0827-4c00-9d0e-72752536f56c",
                                                                "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                                "text": "I don't really know.",
                                                                "emotion": "2",
                                                                "emotion2": "2",
                                                                "device": "0",
                                                                "device2": "0"
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "4",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "53116c6b-0639-4b6b-9567-8ebba8a4f252",
                                            "text": "Say Something Clever",
                                            "nodes": [
                                                {
                                                    "id": "01b0ed94-ae42-41a8-b72a-0e22c085f30c",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "35",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "ec53fa02-80a1-4b78-a814-f578bb9003cc",
                                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "f0a63570-0035-4300-8251-c9dcce996fa2",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "text": "Thomas, it really doesn't matter what your father thinks about what you do. What matters is what YOU think about what you do. Paladins do what they feel is the right thing. So do you.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "080d1c82-9fba-49d1-aeca-592568222289",
                                            "text": "Say Something Snarky",
                                            "nodes": [
                                                {
                                                    "id": "22d48b1a-7bc3-4e38-ae52-e51d95198f8a",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "35",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "bb0ba437-4f7f-4760-87da-d0e8eb5a0124",
                                                                "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "9d7aab2d-46a2-48d4-b050-9866646a4a36",
                                                                "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "3ef938b9-7f84-4dda-b9f6-254298e52fe5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                                    "text": "He'd probably say something like \"What are you doing up so late? It's past your bedtime. Go brush your teeth.\"",
                                                    "emotion": "1",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "4f9565e4-7322-4653-badd-ec398c4768dc",
                            "text": "Talk About Rogue Class",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "0ccc49b8-89bc-48ff-8cc4-c6feb421ffd7",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "Don't you like being in Rogue Class?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "f82ca7b7-67c0-4207-ade5-af4c4c779b41",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I enjoy the class. I've always enjoyed learning new things. I like being here at the castle. It's nice to be able to go where I want and do what I want to do without someone else ordering me around all the time.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "641978fb-ff58-470b-a97d-658030347e2f",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "It just feels wrong for me to be rewarded for being deceitful.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "03821514-1796-4b16-bcdc-41f84a6536d9",
                            "text": "Flirt With Thomas",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "cccb1f09-f3bc-4f92-a854-5efc7536a639",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "Why don't we go up to the garden and look at the stars for a while? I'm sure I can make you feel better.",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fc439664-a459-4691-9bf1-5baf246f40dd",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "No... no, thank you. I should be going to bed soon.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "95f151fa-d479-4b54-b902-f438711edafd",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "Can I go with you?",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "4143b193-3f35-4d58-a9ad-8183fdd8bd5a",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "No!",
                                    "emotion": "4",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d5cdf134-f386-4a82-8491-c768ac29e2d1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "text": "I just meant to walk with you to our dorms.",
                                    "emotion": "1",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "1535c25e-2e84-474f-8cff-2986acddf41b",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "93ca45dd-6bb0-44b8-9748-79c164e38e0b",
                                    "actorId": "bdddd1b1-bbf3-4dd5-bbd6-0ef3115d8b66",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Oh. I think I'd like to just stay here for a bit by myself. Thanks for trying to cheer me up.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "7",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "33ce7bea-6c0f-4a9a-adde-e3b8cdf2d0e2",
                            "text": "Offer a Gift",
                            "nodes": [
                                {
