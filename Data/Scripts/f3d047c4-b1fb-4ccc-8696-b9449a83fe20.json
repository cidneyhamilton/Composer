    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "bd1d9a97-7d9f-4eab-ba3d-4884b7dedb17",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "999f3a69-c3b1-413b-bbea-631aec3bc9ab",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "c42ae61b-3636-4838-9a57-392145e024de",
                            "text": "Look at the Bulletin Board",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "prop",
                                                "scopeId": "2cc6f069-5855-4bf2-a4cb-8dc750c0fc02",
                                                "scopeFilterId": "e8288a23-0b06-4dd7-94d2-9761390d6484",
                                                "tags": "Read01",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "47378351-45ac-4499-97ea-0a9235e7f2a0",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I'd better read all these. They're probably all very important, or they wouldn't be posted here.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "68ee4475-a92d-42a0-ad58-b3ace5bfa677",
                                                    "scope": "prop",
                                                    "scopeId": "2cc6f069-5855-4bf2-a4cb-8dc750c0fc02",
                                                    "scopeFilterId": "e8288a23-0b06-4dd7-94d2-9761390d6484",
                                                    "tagsToAdd": "Read01",
                                                    "tagsToRemove": null
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "802fdd15-63f8-4931-836b-91b8f806ff1e",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "I've glanced at all of these. There's nothing here that's important.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "42beb41d-ecc1-47f4-b731-2ceaa418829d",
                            "text": "Read the First Notice",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "72c5c691-ccc1-41c0-8858-9fb1ee177565",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "Oh, it's the Six Rules again. I'm beginning to think they want us to know these.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "819fa2d3-7a6d-4d46-a969-24522ef8d0c5",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Rule 1: There is no Rogue Class.\" That must be the one I'm not in.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "302ae294-2c5e-46ea-b7c6-5c6cdeb395ac",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Rule 2: You do not speak of Rogue Class.\" Eh, what class is that?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "c323abe6-ad0c-403f-bf59-fd4c1969c475",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Rule 3: You will demonstrate proper respect for authority in the authority's presence.\" Got it - lick boots, but only when they're looking.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7be5f12f-b2a2-415f-a717-ad2f6154f7f3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Rule 4: Thou shalt not steal from the school or classmates (although short-term borrowing without permission is acceptable).\" Hmm, short-term as in the rest of the school term?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "f032ac43-d769-4db3-ad2c-05e549039af4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "\"Rule 5: This is a school for Heroes - act appropriately.\" Okay, gotta act like a Hero, don't need to be one.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fb4c69da-b0c2-4570-ad17-fce905ac04ba",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Rule 6: Above all, don't get caught.\" I've already blown that one; I'd better be more careful next time.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "aa7a1c9d-e8a0-42a4-a00f-a0c6dd9ebf25",
                            "text": "Read the Second Notice",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "1813900f-bc1a-4fcd-bb9d-114b847b39f1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "It says it's the \"Rules for the Throwing Knife Station.\"",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "eed81397-6b3a-4efa-9da5-ed1ac3ac3ab1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Throw knives only at the dagger targets.\" Darn, and here I was thinking that Sosi would make a fine target.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a627228a-e5ff-47d5-bdf6-b71e09cca4d4",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"To properly throw a knife, hold it by the heaviest end.\" Huh, I thought you held it by the pointy end.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "bd8aba40-e0a5-4439-8277-48f95d11eb23",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"Focus upon your future success and not upon your past failure.\" Okay, right. That's easy to say and much harder to do.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a4f849e3-eef0-4986-b28e-dc9300f7546b",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "\"There are four aspects to a perfect throw - a balanced stance, a smooth swing, a clean release, and a clear follow-through.\" Hmmm... no mention of the knife. I guess if you can learn to throw the crummy knives here, you can throw anything. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "b3e5a8d4-dab2-4f17-a6a1-7560ecef5a81",
                            "text": "Read the Third Notice",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "94fbe279-577f-451b-9263-c2d0ef831a84",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "These are various rules for using the equipment, like...",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.nodeCycle",
                                    "id": "bff9189d-cd4f-40a0-846e-4dff9478597d",
                                    "nodes": [
                                        {
                                            "type": "nodes.speak",
                                            "id": "e298d516-8170-4579-95b2-f9f4d7fc100a",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Please do not leave the equipment drenched in your perspiration. A towel is the most massively useful thing a Hero-U trainee can have.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "548c74bb-96fb-4b5d-a90b-ce90c0e38a07",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Management is not responsible for splinters or slivers from the barbell rack.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "522d24dd-f527-43df-8d5d-f24b39da8001",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Please wear proper attire. Sand gets in <i>everything</i>.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "c32580d4-bef1-4b05-8a6e-7df5731d642b",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"The knives are for target practice only. They are not to be used for the cleaning of fingernails.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "c8514faa-203b-431c-845f-46841949cf03",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"Do not drop kettlebells, barbells, and dumbbells. We are tired of replacing the floor.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
                                            "id": "9614fcd8-ebd6-4d62-b6d4-a14d9b498b03",
                                            "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "actorId2": null,
                                            "text": "\"There is no 'p' in 'sand.' Please keep it that way.\"",
                                            "emotion": "0",
                                            "emotion2": "0",
                                            "device": "0",
                                            "device2": "0"
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": 0,
                            "ExitMenu": false
                        }
                    ],
                    "AutoAddDone": true,
                    "Unique": false
                }
            ]
        }
    ]
}