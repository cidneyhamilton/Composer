                        {
                            "type": "options.text",
                            "id": "0a8cb497-90fd-42f1-8aaa-faa22a916ec3",
                            "text": "Ask About Gregor",
                            "nodes": [
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inTags",
                                                "scope": "actor",
                                                "scopeId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                "scopeFilterId": null,
                                                "tags": "TrapTalkGregor",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.changeTags",
                                                    "id": "f8f2c92c-2b33-42cc-8e03-854bda1de76f",
                                                    "scope": "actor",
                                                    "scopeId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "scopeFilterId": null,
                                                    "tagsToAdd": "TrapTalkGregor",
                                                    "tagsToRemove": null
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "335e1bda-ac09-4f37-8b07-9068d3686718",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "So how is Gregor today?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "eeaa7f7c-e99e-4c6b-a579-fa687b4877c8",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Today, Gregor is pleased because Master Shawn has come for a visit.",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeReputation",
                                                    "id": "0475123f-f869-4b01-947b-89b16df1a748",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "amount": "2",
                                                    "change": "1",
                                                    "notify": true
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8e3f40d2-fd39-494e-969e-5b9ff26109cf",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "text": "How are things going?",
                                                    "emotion": "1",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "49bb2aee-5b98-4348-8051-63b0f64d9707",
                                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Gregor is always happy when Master Shawn is here. Master Shawn is such a gentleman.",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "4ff518cb-e260-45d1-bb4d-fd130bdc5be0",
                            "text": "Ask About the Store",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "dc1129ce-4861-4d69-a9f7-e45541466f6f",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "So what do you have new in the store today?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "02ab23e6-a8ae-4285-b4b7-2c4ea7f1c338",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Traps! Gregor has traps just perfect for stopping Drats in their tracks!",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "9991ef74-e384-4e77-8df1-5bbd48d348f3",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Of course, a trap won't stop a Drat for long.",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "07a5a33e-a836-4e76-a1f1-cb5bc4dfa854",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "But a trap will stop a Drat long enough for Master Shawn to end its life, if the Young Master is quick about it.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "94eb2da5-f5aa-447e-a6ef-9cc75a38e0d6",
                            "text": "Ask About Ratties",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "222562bd-a4cd-4dce-9181-5353fcffa9e3",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "So your people, the Ratties... they live underground?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8dc1b275-132a-442a-b6e5-0baae2278e8d",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Ratties like dry, neat secluded places to live. The underground ruins of Old Caligari makes a good place to build homes here. ",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "f60b66ea-c1b0-4a22-abb4-f746a9d06943",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "There's a city underground?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "17720000-fdc5-409c-a483-ac760baa4158",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Yes, Caligari is old, very old. People of all sorts have been living here since time began. There have been fires, invasions, disasters, and monsters destroying Caligari, yet Caligari rebuilds over the ashes of old.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "da780b65-69bd-45fa-9052-927900472c80",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "There are many places under the streets of Caligari that are the inhabited by Ratties and other people and creatures. Ratties call it the Old City.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "cd005ce4-73e2-47bc-af54-4f70220270ac",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Maybe someday, Master Shawn will see the Old City.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "e0bf5375-eb46-469d-b13c-6e0f7f200644",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "amount": "2",
                                    "change": "1",
                                    "notify": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "9",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "21b5fa0f-f82b-4133-a5fe-7da43b6330f4",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "8f751f75-dabe-47ac-aa3c-25684db990f8",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "text": "Be seeing you!",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "fdb71b7d-dbee-4398-a15a-884b83e84b35",
                                    "actorId": "00707f50-1f0d-4c1c-98b3-a2780d28cf0e",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Take care, Young Master!",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
