                {
                    "type": "nodes.moveActor",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "propId": "731c996a-d8ad-488d-a843-20182c601074",
                    "movement": "Running",
                    "wait": true
                },
                {
                    "type": "nodes.turnActor",
                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "propId": null,
                    "wait": true,
                    "scope": "actor",
                    "targetActorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d"
                },
                {
                    "type": "nodes.setCameraFOV",
                    "wait": true,
                    "fov": "4",
                    "time": "0.5"
                },
                {
                    "type": "nodes.speak",
                    "id": "3633b13e-782e-4778-87fc-a9f97f99297b",
                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                    "text": "Don't say a vord to me! Not one vord!",
                    "emotion": "3",
                    "emotion2": "4",
                    "device": "0",
                    "device2": "0"
                },
                {
                    "type": "nodes.showMenu",
                    "id": "39ba7d72-fcff-4be1-9a7f-7439e959fed7",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "60a72ed0-184e-453e-97f7-33a1f0b2f6cf",
                            "text": "Ask About Esme",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "dfa4c6aa-6c20-4367-9d06-264ba9376846",
                                    "scope": "scene",
                                    "scopeId": "cf824b0f-5af6-4a19-bac8-552a2a1bbebf",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "askEsme",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "3f2fd4d8-023b-43c1-8d8a-cab1a27cc91a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "You don't seem very happy right now...",
                                    "emotion": "0",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "1495f39b-80b3-4348-ad90-caa0c987b65a",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": null,
                                    "text": "Sosi did not vant to court me. Sosi vanted just to kiss me! He acted as if I vould do anything he vanted. He is lower than cockroach, that man.",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "1c5bd91c-cf74-4c0c-97ef-c3c6e1657915",
                            "text": "Ask About Sosi",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "3241ab2b-d985-431f-a07a-8543919afbbb",
                                    "scope": "scene",
                                    "scopeId": "cf824b0f-5af6-4a19-bac8-552a2a1bbebf",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "askSosi",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "431dec88-8143-4e84-87a1-fea141796835",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "So Sosi was somewhat less than a gentleman on your date? Who'd have ever guessed that he was a conniving, deceitful scumbag?",
                                    "emotion": "1",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d1434f6f-ff46-4a1d-87f6-1c8c2d9f2a1d",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "<sigh> I hoped that maybe Sosi vas really liking me.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b97c637a-99c8-4ee0-beb4-2be79d968ace",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "The only person Sosi likes is himself. Everyone else is just a tool to be used by him.",
                                    "emotion": "1",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "30e5de7b-2a19-4cbb-813f-01fd7a0a4825",
                            "text": "Say Nothing",
                            "nodes": [
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "0f00dfb6-cdc0-4738-8dfa-4e54cf7477a8",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8c4e838c-b939-4b57-b6e5-9b76c0fa1a19",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Sosi! He is stupid! He is.. is...",
                                    "emotion": "3",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "983bc930-da37-4df0-8f94-9e39137bb140",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "A lying, creepy jerk? A cad and a dastard? A rotter and a two-faced no-goodnik?",
                                    "emotion": "1",
                                    "emotion2": "3",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "222ed771-d46d-4a19-af3e-c1702bf65a1e",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "He is all those and vorse! ",
                                    "emotion": "3",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "ae243128-5516-4295-bfb5-8967b1a2e17d",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "6e2475b2-b969-41f6-bd15-fd7a8e0c2fba",
                                            "text": "\"I told you so\"",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "8e4e2e3c-df64-4004-8fad-ec441f237f51",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "I tried to warn you earlier about Sosi.",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "0ee8fbd6-3578-4155-b89b-6ee903ba16d2",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I thought maybe you vere just a little jealous of him.",
                                                    "emotion": "2",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.showMenu",
                                                    "id": "230816d4-57f3-43d1-ba59-1eae4529ed2c",
                                                    "options": [
                                                        {
                                                            "type": "options.text",
                                                            "id": "98ac89fb-a9d7-4922-afb2-496060883320",
                                                            "text": "Admit to Jealousy",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "cf2b8072-99e0-431f-9a73-866bb1e03459",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "text": "Hey, who wouldn't be jealous of someone who has caught your eye? You are one of the prettiest and nicest people here.",
                                                                    "emotion": "0",
                                                                    "emotion2": "4",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "2",
                                                            "ExitMenu": true
                                                        },
                                                        {
                                                            "type": "options.text",
                                                            "id": "9f463d72-c9e3-43f1-84b5-d40eb5482ad6",
                                                            "text": "Deny any Jealousy",
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "8eaa1f05-ea54-4cd6-bce0-22bbe046ae9f",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                                    "text": "Who could be jealous of Sosi? Okay, so maybe his family has money and power... and he's not bad looking... but he's such a louse. It's hard to look at his smug, smarmy face without wanting to punch him.",
                                                                    "emotion": "0",
                                                                    "emotion2": "1",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ],
                                                            "expression": null,
                                                            "ignoreChildAvailability": false,
                                                            "minutes": "3",
                                                            "ExitMenu": true
                                                        }
                                                    ],
                                                    "AutoAddDone": false,
                                                    "Unique": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "f1721d38-d8b3-45ec-ba84-6aef72407d9a",
                                            "text": "\"I'm sorry to hear that\"",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "1cfbdfd3-c1ca-4db7-9b4e-5d098f2cf76c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "text": "I'm sorry your date didn't go the way you hoped. ",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "db011817-a48a-4bd9-9717-e1be474e61e9",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You tried to varn me. I just didn't vant to hear.",
                                                    "emotion": "2",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "97b7c931-b22b-4dce-b01c-496facb2f9ce",
                                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "You are good friend, Shawn.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
