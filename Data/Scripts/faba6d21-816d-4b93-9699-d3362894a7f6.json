                        {
                            "type": "options.text",
                            "id": "ac044561-ad2e-4697-bba3-0cb2a8b35959",
                            "text": "Say Goodbye",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "26e2aae5-77b3-477e-aed8-6a6620c0e395",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "text": "I'll see you tomorrow. Have a good night.",
                                    "emotion": "0",
                                    "emotion2": "2",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "2801ba9d-1f95-4706-80e3-ce89af106e31",
                                    "actorId": "98ea49aa-86cf-4fa9-9bd5-6aa2fe7c809d",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I am so not happy about Sosi right now. It is not a good night.",
                                    "emotion": "3",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
