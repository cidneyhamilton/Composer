    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "5cc3b75f-35e7-4710-88bc-0a8087580f31",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.branch",
                    "sections": [
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inInventory",
                                "propId": "84ad5776-843d-4706-965c-28285f4b47a4",
                                "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4",
                                "has": false
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "5c490d3f-f189-453e-b1ac-8380f80f7aee",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You will need to be finding some hair from a dead man before I can make you a gris-gris.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.quests",
                                    "id": "85872f60-144a-4d8b-ac6e-acfcf4e22386",
                                    "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                    "propId": "69b6831d-7855-49ce-8b11-a6da90dd7f31",
                                    "target": "0",
                                    "notify": true
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inInventory",
                                "propId": "cec38707-81b1-43a0-a3a7-45ad6b54d4f5",
                                "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4",
                                "has": false
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "c2c50440-e5fe-4aae-a13b-5a33d386bf68",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You will need to bring me some Death's Head mushrooms before I can make you a gris-gris.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.quests",
                                    "id": "8227fca1-d851-4d5f-878e-f561e4de50c7",
                                    "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                    "propId": "7119191e-4116-4ecb-911c-ea119f74dc0c",
                                    "target": "0",
                                    "notify": true
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": {
                                "type": "expressions.inInventory",
                                "propId": "ae8af3c6-bcf2-466c-a577-ea9f06352e55",
                                "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4",
                                "has": false
                            },
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "3f1214dc-82e5-4c1f-9afb-9e23d5a673ca",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You need to bring me the dust of a ghost before I can be making you a 'Speak with the Dead' gris-gris.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b60d7087-5317-4853-8124-7eb1eeaf996c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "How do I get ghost dust?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "8958148a-1335-4131-aa48-ca9e6938b828",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "First, you need to find a Duppy. Duppies, they be made of spirit and the emotions of the person they once were. Duppies be born of magic and thought and the dust of the grave.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "b816df7e-3820-4bc9-86fd-887fb732aaac",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "You will need to catch this Ghost Dust in a special vial.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.branch",
                                    "sections": [
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": {
                                                "type": "expressions.inInventory",
                                                "propId": "2261b420-e2a9-4dc0-8c34-b2473950ebd2",
                                                "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4",
                                                "has": false
                                            },
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e980eafa-e246-45a8-8e5e-7a5eda468d3e",
                                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "Here, take this vial. Do not open it unless you are near a Duppy.",
                                                    "emotion": "0",
                                                    "emotion2": "4",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.changeInventory",
                                                    "id": "1d21bb99-0f26-41cf-95de-e4a5852f39e5",
                                                    "propId": "2261b420-e2a9-4dc0-8c34-b2473950ebd2",
                                                    "sceneId": "a2508b7e-a177-4a96-93bd-4d8ab88dffc4",
                                                    "change": "1",
                                                    "count": 1,
                                                    "notify": true
                                                }
                                            ]
                                        },
                                        {
                                            "type": "nodes.branchSection",
                                            "expression": null,
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "7f24c01a-8673-466b-ade8-b397744373d3",
                                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "text": "I have given you the Ghost Dust Vial. Use it wisely.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "2a1b4c42-2edf-4499-8608-bec0b672ffcd",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Open the Ghost Dust Vial up when you are next to a Duppy. Then hold the vial inside the Duppy's form for the count of daya, biyu, uku - no more.",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "40202b94-e41c-4a1c-ae63-12d041167cff",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Remember, the longer you hold it, the more the Duppy will drain your life. Be very careful not to join the Duppy in the grave, too.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "359b0e3e-b50c-45bc-ac0d-f85eed9aae0c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "Right. Hold the vial in the ghost for the count of dayo, biju, and uni?",
                                    "emotion": "4",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "67c68f1e-86a1-48a4-aa63-d0128816f96e",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Daya, biyu, and uku - one, two, three. Just count slowly to three.",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "5dc524d2-9326-4da7-bef5-4eab752a7b6a",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "text": "Okay, I can do that part. Thanks!",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.quests",
                                    "id": "90fa7ba3-5437-44e0-87cd-30d9ba6438d0",
                                    "sceneId": "e8520824-d970-4c6e-8aec-6c308c8846ab",
                                    "propId": "0e2fc894-781e-4ec7-9ccf-67627d34ae4c",
                                    "target": "0",
                                    "notify": true
                                }
                            ]
                        },
                        {
                            "type": "nodes.branchSection",
                            "expression": null,
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "a64e9c97-b718-4910-9acd-ccd8f447ccd1",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": null,
                                    "text": "I have all the ingredients you needed to allow me to speak with the dead.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "0789cc37-34b5-4abd-a5f8-ceae666282b6",
                                    "actorId": "a0b548d6-b820-451f-9508-8fc7fcf166bd",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Oda! Now I will make a gris-gris for you. Just wait here for a bit.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
