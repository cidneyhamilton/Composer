                                                                            "nodes": [
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "c21e4d00-424b-49b3-8900-4aeeb83179cb",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "Hold on. He's got a bottle... and that's a sack. Like the statue in the courtyard! He's even wearing a straw hat!",
                                                                                    "emotion": "4",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "f32179cd-3879-44e1-985a-f638c9a72583",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "It's another Tanuki!",
                                                                                    "emotion": "4",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
                                                                                {
                                                                                    "type": "nodes.speak",
                                                                                    "id": "d6185b7c-61b7-4bc5-b02f-aa08d279f6ee",
                                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                                    "actorId2": null,
                                                                                    "text": "All right, there must be some greater significance to this animal than I understand.. ",
                                                                                    "emotion": "0",
                                                                                    "emotion2": "0",
                                                                                    "device": "0",
                                                                                    "device2": "0"
                                                                                },
