                                                                "type": "expressions.inTags",
                                                                "scope": "prop",
                                                                "scopeId": "659f1129-6e97-48f7-9ccf-221b96f93f5d",
                                                                "scopeFilterId": "cf824b0f-5af6-4a19-bac8-552a2a1bbebf",
                                                                "tags": "SeenTanuki",
                                                                "has": false
                                                            },
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "8ea428a8-ec1b-43cd-a864-ab2f5f29a414",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "What sort of an animal is this? It looks mostly like a bear, but... not quite...",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "ade291e0-f460-4073-af8d-f64d2e113283",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "He's got a bottle of something... and a bag. And he's in a kind of city I've never seen before. This is so odd. Why is it here? Was he a student? A Founder? ",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                },
                                                                {
                                                                    "type": "nodes.speak",
                                                                    "id": "562d04fe-f7ba-49ee-9e67-b63ba0cd4c0d",
                                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                                    "actorId2": null,
                                                                    "text": "Huh.",
                                                                    "emotion": "0",
                                                                    "emotion2": "0",
                                                                    "device": "0",
                                                                    "device2": "0"
                                                                }
                                                            ]
                                                        },
                                                        {
                                                            "type": "nodes.branchSection",
                                                            "expression": null,
                                                            "nodes": [
                                                                {
                                                                    "type": "nodes.branch",
                                                                    "sections": [
                                                                        {
                                                                            "type": "nodes.branchSection",
                                                                            "expression": {
                                                                                "type": "expressions.inTags",
                                                                                "scope": "prop",
                                                                                "scopeId": "82d275c7-0d17-4adb-b2ae-772e1f41af89",
                                                                                "scopeFilterId": "5aa634b1-8fc5-4b1c-a556-7719d1e665a3",
                                                                                "tags": "SeenOnceNotMet",
                                                                                "has": false
