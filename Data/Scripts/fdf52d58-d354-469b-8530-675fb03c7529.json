                                                    "id": "4378c40a-fc35-423d-99b0-a6e44fbf81aa",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": null,
                                                    "text": "Great. The coin might be in the locked drawer, but I don't have any way to open it. I'll have to hope it's someplace else.",
                                                    "emotion": "0",
                                                    "emotion2": "0",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
