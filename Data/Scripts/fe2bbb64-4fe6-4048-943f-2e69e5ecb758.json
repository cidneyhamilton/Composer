    "entryPoints": [
        {
            "type": "scripts.entryPoint",
            "id": "15133936-85d7-4e3e-926d-6cca58445241",
            "name": "Main",
            "nodes": [
                {
                    "type": "nodes.showMenu",
                    "id": "7d97f364-cd3e-4c42-81d2-bd2d35838dcd",
                    "options": [
                        {
                            "type": "options.text",
                            "id": "292267db-0fc7-4af6-8671-91663911e796",
                            "text": "Ask About Joel",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4ae0892d-e807-4fcb-be51-58869f4d8aa0",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "What are you, Sosi's personal servant or something?",
                                    "emotion": "0",
                                    "emotion2": "4",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "f0b17312-2561-43c2-b437-2306847a812c",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Caesari is my roommate and my friend. ",
                                    "emotion": "2",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d24ff3f9-ea64-4fa7-a5c5-af74841cfbb7",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I would like to be your friend as well, Mr. O'Conner. That is why I thought to give you a little 'suggestion' that you might wish to make friends with Caesari also. He is a most important person.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "76f07d20-995d-48c3-8b72-6a53b8ae7946",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "4",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "3ae67d3c-7de5-4a89-b134-1e78d4299fe8",
                            "text": "Ask About Sosi",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "4d39da05-24d7-4f6b-9aeb-54f59bb6276d",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "What makes Sosi so special?",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "81a8274e-2c7d-403e-9ee7-dda9bff66442",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "The Sosi family is one of great power and influence in the land of Sicilia. \nIt is always wise to have friends in high places.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.showMenu",
                                    "id": "40ed3530-2cd4-4ba0-ade4-ed0758b42434",
                                    "options": [
                                        {
                                            "type": "options.text",
                                            "id": "a52b3cb6-7004-4ea3-880d-f48bcaacf0b0",
                                            "text": "Say Something Polite",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "12f00074-04b7-4d0b-b9f8-be710a0ddd95",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "You seem like a very nice guy, Joel. ",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "a4616097-c69c-4e0e-b5b9-8f63532e231c",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Unfortunately, Sosi is a bit of a all-around jerk.",
                                                    "emotion": "0",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "f06fe0d8-e2fa-4e30-b983-46a8decf4067",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Charm",
                                                    "target": "20",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "c1102700-fe03-4ad9-80e0-ad2d68ccb7f2",
                                                                "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                "amount": "1",
                                                                "change": "1",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "97722299-798b-457a-9b4d-a7203201d5ce",
                                                                "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "58c4ce2a-c8b3-4a08-82ad-958f497fb3f3",
                                            "text": "Say Something Clever",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "929927ac-2c66-47f0-88fe-a4c305370214",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Sosi is smart, cunning, and devious. I can see why you like him.",
                                                    "emotion": "0",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "4208420a-5b3f-4dab-b632-5ebf9646c276",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Unfortunately, Sosi is also smarmy, conceited, and arrogant. I don't find that his charms and his class outweigh the fact that he's a scumbag.",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "8b049bc1-3986-4b75-b6f5-68d866aa9877",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Smarts",
                                                    "target": "20",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "a9f942fa-f213-42a0-a9c1-074b6da70ea4",
                                                                "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "3",
                                            "ExitMenu": true
                                        },
                                        {
                                            "type": "options.text",
                                            "id": "4b0fc500-87eb-4b10-8997-4d84bf5350e3",
                                            "text": "Say Something Snarky",
                                            "nodes": [
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "5a28b779-c119-42b0-90ba-39dc53d24038",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "There's a lot that can be said for Sosi...",
                                                    "emotion": "1",
                                                    "emotion2": "1",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "type": "nodes.speak",
                                                    "id": "e77f4d85-8659-4bcb-ac1d-83e39a88f7b5",
                                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                    "text": "Unfortunately, much of it involves profanity.",
                                                    "emotion": "1",
                                                    "emotion2": "2",
                                                    "device": "0",
                                                    "device2": "0"
                                                },
                                                {
                                                    "id": "e42f56eb-463a-45cf-9e12-42b359461949",
                                                    "type": "nodes.skillBranch",
                                                    "skill": "Moxie",
                                                    "target": "20",
                                                    "success": {
                                                        "type": "nodes.block",
                                                        "nodes": []
                                                    },
                                                    "failure": {
                                                        "type": "nodes.block",
                                                        "nodes": [
                                                            {
                                                                "type": "nodes.changeReputation",
                                                                "id": "49d7738b-efb8-4018-811c-cafcb61c59b0",
                                                                "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                                                "amount": "1",
                                                                "change": "0",
                                                                "notify": true
                                                            }
                                                        ]
                                                    },
                                                    "notify": true
                                                }
                                            ],
                                            "expression": null,
                                            "ignoreChildAvailability": false,
                                            "minutes": "2",
                                            "ExitMenu": true
                                        }
                                    ],
                                    "AutoAddDone": false,
                                    "Unique": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "3",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "e51cd6e4-b40c-4eb7-957a-71c4e06e78ca",
                            "text": "Ask About Games",
                            "nodes": [
                                {
                                    "type": "nodes.changeTags",
                                    "id": "25b94c37-be95-4662-9b38-546613f2b89e",
                                    "scope": "actor",
                                    "scopeId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "scopeFilterId": null,
                                    "tagsToAdd": "Games",
                                    "tagsToRemove": null
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "5931d4ab-c602-4569-a93c-5273d0652a0c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "What games do you like best here?",
                                    "emotion": "0",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "7dfe5091-d753-4f9b-ac3b-a841f429f176",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "I have little interest in playing darts. Billiards is an interesting game, one I have yet to master. ",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "d4140490-1422-4ed5-9d69-1d221468384a",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Gambling is unfortunately discouraged at the school and card games are rather boring without it.",
                                    "emotion": "0",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "22fc722d-d72f-434c-a3f5-2ab9cb63cf13",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Fortunately, we do play Poobah almost weekly here. That game of chance and intrigue is definitely my favorite.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.changeReputation",
                                    "id": "9b88c7c9-4766-4a6b-ae50-f9447839be58",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "amount": "1",
                                    "change": "1",
                                    "notify": true
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": false,
                            "minutes": "5",
                            "ExitMenu": false
                        },
                        {
                            "type": "options.text",
                            "id": "dcc6511c-1110-4efb-9122-a5909addde45",
                            "text": "Say Goodnight",
                            "nodes": [
                                {
                                    "type": "nodes.speak",
                                    "id": "57d924f4-118e-4c99-96bb-b03334397d1c",
                                    "actorId": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "actorId2": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "text": "I'll see you later.",
                                    "emotion": "1",
                                    "emotion2": "1",
                                    "device": "0",
                                    "device2": "0"
                                },
                                {
                                    "type": "nodes.speak",
                                    "id": "a929b068-7237-4e2e-8e6e-52672fdbbee0",
                                    "actorId": "4b05c425-ca25-4abb-9a36-7d793370842f",
                                    "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                    "text": "Indeed you shall, Mr. O'Conner.",
                                    "emotion": "1",
                                    "emotion2": "0",
                                    "device": "0",
                                    "device2": "0"
                                }
                            ],
                            "expression": null,
                            "ignoreChildAvailability": true,
                            "minutes": "2",
                            "ExitMenu": true
                        }
                    ],
                    "AutoAddDone": false,
                    "Unique": false
                }
            ]
        }
