                                            "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "If Herr O'Conner will kindly refrain from snoring in my classroom, then I shall continue this lecture.",
                                            "emotion": "0",
                                            "emotion2": "3",
                                            "device": "0",
                                            "device2": "0"
                                        },
                                        {
                                            "type": "nodes.speak",
