                                            "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "I shall continue my lecture when Herr O'Conner ceases to yawn without the courtesy of covering his gaping mouth. ",
                                            "emotion": "0",
                                            "emotion2": "3",
                                            "device": "0",
                                            "device2": "0"
                                        },
