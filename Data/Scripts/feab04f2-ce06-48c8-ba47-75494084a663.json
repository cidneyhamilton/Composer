                                            "actorId": "b7cbd02d-8e2f-4a8d-99a7-02b132d86e1d",
                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Herr O'Conner - I suggest that you get your sleep at night rather than in my classroom.",
                                            "emotion": "3",
                                            "emotion2": "3",
                                            "device": "0",
                                            "device2": "0"
                                        },
