                                            "actorId2": "30ee2636-15e3-452e-a2db-09cbb9012970",
                                            "text": "Herr O'Conner - I suggest you pay attention to my lecture rather than the inside of your eyelids.",
                                            "emotion": "3",
                                            "emotion2": "3",
                                            "device": "0",
                                            "device2": "0"
